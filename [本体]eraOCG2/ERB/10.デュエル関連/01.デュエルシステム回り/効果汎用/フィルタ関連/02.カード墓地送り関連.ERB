@CARD_DECK_DISPOSE(듀얼리스트,ゾーン,リピート횟수,行先,対面플래그)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC 対面者
#DIMS DYNAMIC 実行者
#DIM DYNAMIC 対面플래그
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC カウンタB
#DIM DYNAMIC リピート횟수
#DIM DYNAMIC 行先
#DIM DYNAMIC 選択カード
#DIM DYNAMIC 実제외セット,1000
#DIM DYNAMIC 行先보존

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%
IF 対面플래그 == 1
	実行者 = %対面者%
ELSE
	実行者 = %듀얼리스트%
ENDIF


IF 行先 >= 1
	行先보존 = 行先
ELSE
	行先보존 = 효과묘지行き
ENDIF

IF ゾーン == "제외"
	VARSET カウンタB
	FOR カウンタ,0,紙束最大値
		SIF GETVAR(@"%実行者%_제외:カウンタ") == -1
			CONTINUE
		実제외セット:カウンタB = カウンタ
		カウンタB ++
	NEXT
	IF カウンタB >= 1
		選択カード = RAND:カウンタB
		選択カード = 実제외セット:選択カード
		CALL CARD_DISPOSE(実行者,"제외",選択カード,묘지行き)
	ENDIF
ELSE
	SIF リピート횟수 == 0
		リピート횟수 = 1
	FOR カウンタ,0,リピート횟수
		CALL CARD_DISPOSE(実行者,ゾーン,0,行先)
	NEXT
ENDIF
RETURN 1


