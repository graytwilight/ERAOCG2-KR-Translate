@IRAI_DECK_212(듀얼리스트)
#DIMS DYNAMIC 듀얼리스트

IF 듀얼리스트 == "상대"
	RETURN 依頼_進行보존:212
ELSEIF 듀얼리스트 == "자신"
	RETURN 依頼_進行보존:1212
ENDIF
RETURN 0


@ANTE_RULE(승리플래그,使用덱)
#DIM DYNAMIC 승리플래그 ;0負け 1勝ち
#DIM DYNAMIC 使用덱
#DIM DYNAMIC 場所
#DIM DYNAMIC 選択カード_ID
#DIM DYNAMIC 캐릭터_ID
#DIM DYNAMIC 候補カード_ID
#DIM DYNAMIC 候補カード_優先度
#DIM DYNAMIC 優先度
#DIM DYNAMIC カウンタ
#DIM DYNAMIC カウンタB
;アンティルールのカード이동処理 정령つきならRESULT = 캐릭터_IDを返す

IF 승리플래그 == 1
	IF 使用덱 >= 1000
		CALLFORM DUELDECK_{使用덱}("상대")
	ELSE
		CALLFORM MYDECK("상대",使用덱)
	ENDIF
	CALL FULLRESET
	효과範囲상대덱 = 1
	효과範囲상대EX덱 = 1
	CALL CARD_CHOOSE_ADV("자신")
	IF RESULT != 9999
		選択カード_ID = RESULT:99 
		CALL CARD_EROTICS(選択カード_ID)
		;毎回정령ついてても特別感ないので確率
		IF RESULT != -1 && RAND:2
			캐릭터_ID = RESULT
			PRINTFORML
			SETCOLOR C_YELLOW
			PRINTFORMW 이 카드에서는 《%NAME:캐릭터_ID%》의 혼을 느낀다…….
			RESETCOLOR
			IF CFLAG:캐릭터_ID:소유함 != 1
				ITEM:選択カード_ID += 1
				CALL CARD_GET_CHARA(캐릭터_ID,1)
				RETURN 캐릭터_ID
			ELSE
			;所持済みなら、別個体のほうが奪った感あるので増殖機能で増やす
				PRINTL
				PRINTFORMW 이 카드의 정령은 이미 가지고 있지만, %CALLNAME:MASTER%의 것과는 별개의 개체인가,
				PRINTFORML 아니면 다른 세계의 인물인가, 어쨌든 %CALLNAME:MASTER%는 모르는 것 같다.
				CALL CARD_GET_SINGLE(選択カード_ID,1)
				CALL CLONING_CHARA_ANTE(캐릭터_ID)
				RETURN RESULT
			ENDIF
		ELSE
			CALL CARD_GET_SINGLE(選択カード_ID,1)
			RETURN 0
		ENDIF
	ENDIF

ELSEIF 승리플래그 == 0
	IF 使用덱 >= 1000
		CALLFORM DUELDECK_{使用덱}("자신")
	ELSE
		CALLFORM MYDECK("자신",使用덱)
	ENDIF
	;ランダムで10枚보고、一番レアっぽいのを選ぶ
	FOR カウンタ, 0, 10
		優先度 = 0
		IF RAND:3 == 0
			FOR カウンタ,0,紙束最大値
				SIF GETVAR(@"자신_EX덱:カウンタ") == -1
					BREAK
			NEXT
			候補カード_ID = GETVAR(@"자신_EX덱:(RAND:カウンタ)")
		ELSE
			FOR カウンタ,0,紙束最大値
				SIF GETVAR(@"자신_덱:カウンタ") == -1
					BREAK
			NEXT
			候補カード_ID = GETVAR(@"자신_덱:(RAND:カウンタ)")
		ENDIF
		TRYCALLFORM CARD_{候補カード_ID}, "성별"	
		SIF MATCH(RESULT, 牝性)
			優先度 += 2
		TRYCALLFORM CARD_{候補カード_ID}, "レア度"	
		IF MATCH(RESULT, ULR)
			優先度 += 2	
		ELSEIF MATCH(RESULT, SPR)
			優先度 += 1
		ENDIF
		IF 優先度 > 候補カード_優先度
			選択カード_ID = 候補カード_ID
			候補カード_優先度 = 優先度
		ENDIF
	NEXT

	SIF 選択カード_ID <= 0
		選択カード_ID = GETVAR(@"자신_덱:0")
	
	IF ITEM:選択カード_ID > 0
		ITEM:選択カード_ID -= 1
		TRYCALLFORM CARDNAME_{選択カード_ID}, "名前"
		SETCOLOR C_RED
		PRINTFORMW 《%RESULTS%》을 잃었다……
		RESETCOLOR
	ENDIF
	CALL CARD_EROTICS(選択カード_ID)
	IF RESULT != -1
	캐릭터_ID = RESULT
		IF CFLAG:캐릭터_ID:소유함 == 1
			CFLAG:캐릭터_ID:소유함 = 0
			ITEM:(NO:캐릭터_ID) -= 1
			PRINTFORMW 잃은 카드들 중에서, 경솔하게 걸었다가 잃은 것에 대한 실망과 경멸의 시선을 받고 있는 것 같다.
			MARK:캐릭터_ID:굴복각인 = 0
			MARK:캐릭터_ID:반발각인 = 3
			TALENT:캐릭터_ID:중고 = 1
			TALENT:캐릭터_ID:애처 = 0
			TALENT:캐릭터_ID:연모 = 0
			TALENT:캐릭터_ID:성별기호 = 3
			TALENT:캐릭터_ID:중고 = 1
			ABL:캐릭터_ID:친밀 = 0
			ABL:캐릭터_ID:순종 = 0
			CFLAG:캐릭터_ID:호감도 = 0
			CFLAG:캐릭터_ID:기정사실 = -1
			CFLAG:캐릭터_ID:시작위치 = 100
			CFLAG:캐릭터_ID:현재위치 = 100
			RETURN 캐릭터_ID
			;一般듀얼리스트は정령実体化できなさそうだし凌辱は一旦なし
		ENDIF
	ENDIF
		RETURN 0
ENDIF

