@CARD_ADD_GY_BASIC(듀얼리스트,元参照先,ゾーン,場所,めくり数)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC 대상者

#DIMS DYNAMIC ゾーン
#DIMS DYNAMIC 参照先
#DIMS DYNAMIC 元参照先

#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カードID
#DIM DYNAMIC めくり数
#DIM DYNAMIC 選択カード,99
#DIM DYNAMIC 選択カード_ID,99
#DIM DYNAMIC 行先보존
#DIM DYNAMIC 条件달성
#DIMS DYNAMIC 対面者
#DIM DYNAMIC 複数カウンタ
#DIM DYNAMIC カウンタ
#DIM DYNAMIC カウンタB
#DIM DYNAMIC 対面カード使用

#DIM DYNAMIC 最大値
#DIM DYNAMIC 消去カード
#DIM DYNAMIC 一時보존,99
#DIM DYNAMIC 選択数
#DIM DYNAMIC 山切り無し
#DIM DYNAMIC 추가플래그
#DIM DYNAMIC 攻撃力合計
#DIM DYNAMIC 守備力合計
#DIM DYNAMIC レベル合計

SIF 효과条件山切り無し == 1
	山切り無し = 1
SIF 효과条件複数 == 100
	RETURN 0

VARSET 選択カード,-1

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

IF GROUPMATCH(元参照先,"묘지","상대묘지") && 효과条件行先플래그 == 0 && GETVAR(@"%듀얼리스트%_제외NG置換") >= 1
	PRINTFORML 카드를 제외할 수 없습니다。
	RETURN 0
ENDIF
IF GROUPMATCH(효과条件行先플래그,제외行き,一時제외行き) && GETVAR(@"%듀얼리스트%_제외NG置換") >= 1
	PRINTFORML 카드를 제외할 수 없습니다。
	RETURN 0
ENDIF

IF 효과条件行先플래그 >= 1
	行先보존 = 효과条件行先플래그
ELSE
	行先보존 = 효과묘지行き
ENDIF
추가플래그 = 효과条件추가플래그




SELECTCASE 元参照先
	CASE "필드","마함","손패","묘지","덱","EX덱","제외"
		대상者 = %듀얼리스트%
		参照先 = %元参照先%
	CASE "상대필드"
		대상者 = %対面者%
		参照先 = 필드
	CASE "상대마함"
		대상者 = %対面者%
		参照先 = 마함
	CASE "상대손패"
		대상者 = %対面者%
		参照先 = 손패
	CASE "상대묘지"
		대상者 = %対面者%
		参照先 = 묘지
	CASE "상대덱"
		대상者 = %対面者%
		参照先 = 덱
	CASE "상대EX덱"
		대상者 = %対面者%
		参照先 = EX덱
	CASE "상대제외"
		대상者 = %対面者%
		参照先 = 제외	
ENDSELECT

IF 参照先 == "덱" && 효과条件行先플래그 == 0
	CALL URARA_CHANCE(듀얼리스트,カードID,"묘지")
	IF RESULT == 1
		CALL SHUFFLE(듀얼리스트)
		RETURN 0
	ENDIF
ENDIF

IF 参照先 == "묘지" && (효과条件行先플래그 == 0 || 효과条件行先플래그 == 제외行き)
	CALL WARASHI_CHANCE(듀얼리스트,カードID,"雑多")
	IF RESULT == 1
		RETURN 0
	ENDIF
ENDIF



IF ゾーン != "어드밴스"
	カードID = 処理中_효과ID
ENDIF
$CHOICE_LOOP
IF 듀얼리스트 == "자신"
	CUSTOMDRAWLINE —
	PRINT 【카드를 선택해 주세요】
	IF オプション_略표시 == 1
		PRINTBUTTON "[간이 표시 중]", 6666
	ELSE
		PRINTBUTTON "[간이 표시]", 6666
	ENDIF
	PRINTFORM　%"[7777] - 필드 / 패 정보 확인",55,RIGHT%

	PRINTL
	CUSTOMDRAWLINE —
	PRINTL 
ELSEIF 듀얼리스트 == "상대"
	CALL ENEMY_RUTIN_CARD_ADD(元参照先,場所,カードID)
ENDIF

$INPUT_LOOP
IF 효과条件複数
	FOR カウンタ,0,複数カウンタ
		효과条件選択済:カウンタ = 選択カード:カウンタ 
	NEXT
ENDIF

CALL CARD_FILTER_BASIC(듀얼리스트,元参照先,ゾーン,場所,めくり数)
条件달성 = RESULT:1

IF 듀얼리스트 == "자신"
	PRINTL
	IF  (INRANGE(효과条件複数,90,99) || 효과条件複数 == 任意の数) && 複数カウンタ >= 1
		PRINTL 　[8888] - 결정
	ENDIF
	IF 条件달성 != 0 && 효과条件任意 != 1
		SETCOLOR C_GRAY
		PRINTFORM 　(9999) - 돌아간다(선택 가능한 카드가 있습니다)
		RESETCOLOR
	ELSE
		PRINTFORM 　%"[9999] - 돌아간다",41,LEFT%
	ENDIF
	PRINTL
	BINPUT
	IF RESULT == 7777
		CALL DIPLAY_BOARD(2)			
		GOTO CHOICE_LOOP
	ELSEIF RESULT == 6666
		オプション_略표시 ^= 1
		GOTO CHOICE_LOOP

	ENDIF
	選択カード:複数カウンタ = RESULT
ELSEIF 듀얼리스트 == "상대"
	IF RESULT == 9999 &&  (INRANGE(효과条件複数,90,99) || 효과条件複数 == 任意の数) && 複数カウンタ >= 1 
		選択カード:複数カウンタ = 8888
	ELSE
		選択カード:複数カウンタ = RESULT
	ENDIF
	SETCOLOR C_W_RED
	IF 選択カード:複数カウンタ == 8888
		SETCOLOR C_W_RED
		PRINTL 이 이상 선택하지 않는 것 같습니다。
		RESETCOLOR
	ENDIF
	PRINTL
	WAIT
ENDIF
IF 選択カード:複数カウンタ == 9999
	PRINTFORMW 돌아갑니다。
	RETURN 0
ELSEIF 選択カード:複数カウンタ == 8888
	選択カード:複数カウンタ = -1
	GOTO INPUT_SKIP
ELSEIF 選択カード:複数カウンタ != 9999
	選択カード_ID = GETVAR(@"%대상者%_%参照先%:{選択カード:複数カウンタ}")
	CALL SELECT_UI_SET(듀얼리스트,選択カード_ID,2)
	IF RESULT == 1
		GOTO CHOICE_LOOP
	ENDIF
ENDIF



IF 효과条件複数 != 0
	IF 효과条件複数 > (複数カウンタ + 1)
		IF 효과条件固名前選択
			CALLFORM CARDNAME_{GETVAR(@"%대상者%_%参照先%:{選択カード:複数カウンタ}")},"名前"
			효과条件固名前:複数カウンタ = %RESULTS%
		ENDIF
		複数カウンタ ++
		選択数 ++
		SIF INRANGE(효과条件複数,90,99) && 複数カウンタ >= (100 - 효과条件複数)
			GOTO INPUT_SKIP
		GOTO INPUT_LOOP
	ELSE
		;大きいほうにリアレンジする
		$INPUT_SKIP
		FOR カウンタ,0,(複数カウンタ + 1)
			最大値 = -1
			FOR カウンタB,0,(複数カウンタ + 1)
				IF 選択カード:カウンタB >= 最大値
					最大値 = 選択カード:カウンタB
					一時보존:カウンタ = 選択カード:カウンタB
					消去カード = カウンタB
				ENDIF
			NEXT
			ARRAYREMOVE 選択カード, 消去カード, 1
		NEXT
		FOR カウンタ,0,(複数カウンタ + 1)
			選択カード:カウンタ = 一時보존:カウンタ
		NEXT
	ENDIF
ENDIF
IF 효과条件複数 != 0
	FOR カウンタ,0,(複数カウンタ + 1)
		SIF 選択カード:カウンタ == -1
			CONTINUE
		選択カード_ID = GETVAR(@"%대상者%_%参照先%:(選択カード:カウンタ)")
		SIF 듀얼리스트 != 대상者
			対面カード使用 = 1
		VARSET RESULT
		TRYCALLFORM CARD_{選択カード_ID}("守備力")
		守備力合計 += RESULT
		TRYCALLFORM CARD_{選択カード_ID}("攻撃力")
		攻撃力合計 += RESULT
		TRYCALLFORM CARD_{選択カード_ID}("レベル")
		レベル合計 += RESULT
		CALL CARD_DISPOSE(대상者,参照先,(選択カード:カウンタ),行先보존,추가플래그)
	NEXT
ELSE
	選択カード_ID = GETVAR(@"%대상者%_%参照先%:選択カード")
	SIF 듀얼리스트 != 대상者
		対面カード使用 = 1
	CALL CARD_DISPOSE(대상者,参照先,選択カード,行先보존,추가플래그)
ENDIF


IF 参照先 == "덱" && 山切り無し != 1
	CALL SHUFFLE(대상者)
ENDIF

VARSET RESULT
RESULT:1 = 選択カード_ID
RESULT:2 = 選択数
RESULT:10 = 対面カード使用

RESULT:98 = 守備力合計
RESULT:99 = 攻撃力合計
RESULT:200 = レベル合計

RETURN 1


;――――――――――――――――――――――――――――――――――――――――――――――――――――――――――――――――――――

@CARD_ADD_GY_ADV(듀얼리스트,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC 対面者
#DIMS DYNAMIC 대상者
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カードID
#DIM DYNAMIC 選択カード,99
#DIM DYNAMIC 選択カード_ID,99
#DIM DYNAMIC 行先보존
#DIM DYNAMIC 上級値
#DIMS DYNAMIC 参照先
#DIM DYNAMIC 条件달성
#DIM DYNAMIC 複数カウンタ
#DIM DYNAMIC カウンタ
#DIM DYNAMIC カウンタB
#DIM DYNAMIC 対面カード使用

#DIM DYNAMIC 最大値
#DIM DYNAMIC 消去カード
#DIM DYNAMIC 一時보존,99
#DIM DYNAMIC 덱サーチ플래그
#DIM DYNAMIC 묘지回収플래그
#DIM DYNAMIC 選択数
#DIM DYNAMIC 추가플래그
#DIM DYNAMIC 攻撃力合計
#DIM DYNAMIC 守備力合計
#DIM DYNAMIC レベル合計

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%
VARSET 選択カード,-1
SIF 효과条件複数 == 100
	RETURN 0

カードID = 処理中_효과ID

IF (효과範囲묘지 == 1 || 효과範囲상대묘지 == 1) && 효과条件行先플래그 == 0 && GETVAR(@"%듀얼리스트%_제외NG置換") >= 1
	PRINTFORML 카드를 제외할 수 없습니다。
	RETURN 0
ENDIF
IF GROUPMATCH(효과条件行先플래그,제외行き) && GETVAR(@"%듀얼리스트%_제외NG置換") >= 1
	PRINTFORML 카드를 제외할 수 없습니다。
	RETURN 0
ENDIF

IF 효과範囲덱 == 1 && 효과条件行先플래그 == 0
	CALL URARA_CHANCE(듀얼리스트,カードID,"묘지")
	IF RESULT == 1
		CALL SHUFFLE(듀얼리스트)
		RETURN 0
	ENDIF
ENDIF

IF (효과範囲묘지 == 1 || 효과範囲상대묘지 == 1) && 효과条件行先플래그 == 0
	CALL WARASHI_CHANCE(듀얼리스트,カードID,"雑多")
	IF RESULT == 1
		RETURN 0
	ENDIF
ENDIF


IF 효과条件行先플래그 >= 1
	行先보존 = 효과条件行先플래그
ELSE
	行先보존 = 효과묘지行き
ENDIF

IF 종류 == 손패起動
	효과条件他所 = 場所
ENDIF
추가플래그 = 효과条件추가플래그




$CHOICE_LOOP
IF 듀얼리스트 == "자신"
	CUSTOMDRAWLINE —
	PRINTL 【카드를 선택해 주세요】
	IF オプション_略표시 == 1
		PRINTBUTTON "[간이 표시 중]", 6666
	ELSE
		PRINTBUTTON "[간이 표시]", 6666
	ENDIF
	PRINTFORM　%"[7777] - 필드 / 패 정보 확인",55,RIGHT%
	PRINTL

	CUSTOMDRAWLINE —
	PRINTL 
ELSEIF 듀얼리스트 == "상대"
	CALL ENEMY_RUTIN_CARD_ADD(参照先,場所,カードID)
ENDIF

$INPUT_LOOP
IF 효과条件複数
	FOR カウンタ,0,複数カウンタ
		효과条件選択済:カウンタ = 選択カード:カウンタ 
	NEXT
ENDIF

CALL CARD_FILTER_BASIC(듀얼리스트,"上級",ゾーン,場所)
条件달성 = RESULT:1
IF 듀얼리스트 == "자신"
	PRINTL
	IF  (INRANGE(효과条件複数,90,99) || 효과条件複数 == 任意の数) && 複数カウンタ >= 1
		PRINTL 　[8888] - 결정
	ENDIF
	IF 条件달성 != 0 && 효과条件任意 != 1
		SETCOLOR C_GRAY
		PRINTFORM 　(9999) - 돌아간다(선택 가능한 카드가 있습니다)
		RESETCOLOR
	ELSE
		PRINTFORM 　%"[9999] - 돌아간다",41,LEFT%
	ENDIF
	PRINTL
	BINPUT
	IF RESULT == 7777
		CALL DIPLAY_BOARD(2)			
		GOTO CHOICE_LOOP
	ELSEIF RESULT == 6666
		オプション_略표시 ^= 1
		GOTO CHOICE_LOOP
	ENDIF
	選択カード:複数カウンタ = RESULT
ELSEIF 듀얼리스트 == "상대"
	IF RESULT == 9999 &&  (INRANGE(효과条件複数,90,99) || 효과条件複数 == 任意の数) && 複数カウンタ >= 1 
		選択カード:複数カウンタ = 8888
	ELSE
		選択カード:複数カウンタ = RESULT
	ENDIF
	IF 選択カード:複数カウンタ == 8888
		SETCOLOR C_W_RED
		PRINTL 이 이상 선택하지 않는 것 같습니다。
		RESETCOLOR
	ENDIF
ENDIF
IF 選択カード:複数カウンタ == 9999
	PRINTFORMW 돌아갑니다。
	RETURN 0

ELSEIF 選択カード:複数カウンタ == 8888
	選択カード:複数カウンタ = -1
	;複数カウンタ -= 1
	GOTO INPUT_SKIP
ELSEIF 選択カード:複数カウンタ != 9999
	CALL CARD_SELECT(듀얼리스트,選択カード:複数カウンタ)
	選択カード_ID = GETVAR(@"%대상者（CS）%_%参照先（CS）%:{選択カード（CS）}")
	CALL SELECT_UI_SET(듀얼리스트,選択カード_ID,2)
	IF RESULT == 1
		GOTO CHOICE_LOOP
	ENDIF
	IF 参照先（CS） == "덱"
		덱サーチ플래그 = 1
	ELSEIF 参照先（CS） == "묘지"
		묘지回収플래그 = 1
	ENDIF
ENDIF


IF 효과条件複数 != 0
	IF 효과条件複数 > (複数カウンタ + 1)
		IF 효과条件固名前選択
			CALL CARD_SELECT(듀얼리스트,選択カード:複数カウンタ)
			CALLFORM CARDNAME_{GETVAR(@"%대상者（CS）%_%参照先（CS）%:{RESULT}")},"名前"
			효과条件固名前:複数カウンタ = %RESULTS%
		ENDIF
		複数カウンタ ++
		選択数 ++
		SIF INRANGE(효과条件複数,90,99) && 複数カウンタ >= (100 - 효과条件複数)
			GOTO INPUT_SKIP
		GOTO INPUT_LOOP
	ELSE
		$INPUT_SKIP
		FOR カウンタ,0, (複数カウンタ + 1)
			最大値 = -1
			FOR カウンタB,0,(複数カウンタ + 1)
				IF 選択カード:カウンタB >= 最大値
					最大値 = 選択カード:カウンタB
					一時보존:カウンタ = 選択カード:カウンタB
					消去カード = カウンタB
				ENDIF
			NEXT
			ARRAYREMOVE 選択カード, 消去カード, 1
		NEXT
		FOR カウンタ,0,(複数カウンタ + 1)
			選択カード:カウンタ = 一時보존:カウンタ
		NEXT
	ENDIF
ENDIF

;PRINTFORML 複数カウンタ= {複数カウンタ}
IF 효과条件複数 != 0
	FOR カウンタ,0,(複数カウンタ + 1)
		;묘지送り処理
		SIF 選択カード:カウンタ == -1
			CONTINUE
		CALL CARD_SELECT(듀얼리스트,(選択カード:カウンタ))
		SIF 듀얼리스트 != 대상者（CS）
			対面カード使用 = 1
		選択カード_ID = GETVAR(@"%대상者（CS）%_%参照先（CS）%:選択カード（CS）")
		VARSET RESULT
		TRYCALLFORM CARD_{選択カード_ID}("攻撃力")
		攻撃力合計 += RESULT
		TRYCALLFORM CARD_{選択カード_ID}("守備力")
		守備力合計 += RESULT
		TRYCALLFORM CARD_{選択カード_ID}("レベル")
		レベル合計 += RESULT
		CALL CARD_DISPOSE(대상者（CS）,参照先（CS）,選択カード（CS）,行先보존,추가플래그)
	NEXT
ELSE
	;묘지送り処理
	CALL CARD_SELECT(듀얼리스트,選択カード)
	選択カード = RESULT
	SIF 듀얼리스트 != 대상者（CS）
		対面カード使用 = 1
	選択カード_ID = GETVAR(@"%대상者（CS）%_%参照先（CS）%:選択カード（CS）")

	CALL CARD_DISPOSE(대상者（CS）,参照先（CS）,選択カード,行先보존,추가플래그)
ENDIF



SIF 덱サーチ플래그
	CALL SHUFFLE(듀얼리스트)

;PRINTFORML リピート数A:{複数カウンタ}
VARSET RESULT

RESULT:1 = 選択カード_ID
RESULT:2 = 選択数
RESULT:10 = 対面カード使用

RESULT:98 = 守備力合計
RESULT:99 = 攻撃力合計
RESULT:200 = レベル合計

RETURN 1


;――――――――――――――――――――――――――――――――――――――――――――――――――――――――――――――――――――

@CARD_DISPOSE(듀얼리스트,ゾーン,場所,行先플래그,추가플래그,身代불가)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC 효과대상者
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 場所
#DIM DYNAMIC 行先플래그
#DIM DYNAMIC 元行先플래그
#DIM DYNAMIC 추가플래그
#DIMS DYNAMIC 対面者
#DIM DYNAMIC 選択カード_ID
#DIMS DYNAMIC 選択カード_名
#DIM DYNAMIC カード추가先
#DIM DYNAMIC 直前발동_ID
#DIM DYNAMIC 身代불가
VARSET RESULT

VARSET 묘지送り종류
;PRINTFORML 行先플래그:{行先플래그}
元行先플래그 = 行先플래그

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

SIF ゾーン == "효과処理"
	RETURN 1
SIF GETVAR(@"%듀얼리스트%_%ゾーン%:場所") == -1
	RETURN 0

IF ゾーン == "덱"
	IF GETVAR(@"%듀얼리스트%_덱:場所") <= 0
		PRINTFORMW %듀얼리스트%의 덱은 ０장입니다。
		RETURN 0
	ENDIF
ELSEIF ゾーン == "EX덱"
	IF GETVAR(@"%듀얼리스트%_EX덱:場所") <= 0
		PRINTFORMW %듀얼리스트%의 EX덱은 ０장입니다。
		RETURN 0
	ENDIF
ELSEIF ゾーン == "필드"
	IF GETVAR(@"%듀얼리스트%_MZ_所有:場所") == 1
		효과대상者 = 자신
	ELSEIF GETVAR(@"%듀얼리스트%_MZ_所有:場所") == 2
		효과대상者 = 상대
	ENDIF
	IF 行先플래그 != 完全묘지行き
		IF GETVAR(@"%듀얼리스트%_MZ_除処:場所") >= 1
			行先플래그 = 제외行き
			IF GETVAR(@"%듀얼리스트%_제외NG置換") >= 1
				行先플래그 = 完全묘지行き
			ENDIF
		ENDIF
	ENDIF
ELSEIF ゾーン == "마함"
	IF GETVAR(@"%듀얼리스트%_魔_所有:場所") == 1
		효과대상者 = 자신
	ELSEIF GETVAR(@"%듀얼리스트%_魔_所有:場所") == 2
		효과대상者 = 상대
	ENDIF
	IF 行先플래그 != 完全묘지行き
		IF GETVAR(@"%듀얼리스트%_魔_除処:場所") >= 1
			行先플래그 = 제외行き
			IF GETVAR(@"%듀얼리스트%_제외NG置換") >= 1
				行先플래그 = 完全묘지行き
			ENDIF
		ENDIF
	ENDIF
ELSEIF ゾーン == "묘지"
	SIF 行先플래그 == 묘지行き
		行先플래그 = 제외行き
ENDIF

IF 行先플래그 == 마함行き && 마함数(듀얼리스트) == 5 && ゾーン != "마함"
	行先플래그 = 0
ENDIF

IF GROUPMATCH(行先플래그,묘지行き,破壊行き,戦闘破壊行き) && GETVAR(@"%듀얼리스트%_제외置換") >= 1
	行先플래그 = 제외行き
ENDIF

IF ゾーン == "손패" || ゾーン == "덱"
	SIF GROUPMATCH(行先플래그,묘지行き,破壊行き) && GETVAR(@"%듀얼리스트%_空제외置換") >= 1
		行先플래그 = 제외行き
ELSEIF ゾーン == "필드" || ゾーン == "마함"
	SIF GROUPMATCH(行先플래그,묘지行き,破壊行き,戦闘破壊行き,絶対破壊行き) && GETVAR(@"%듀얼리스트%_땅제외置換") >= 1
		行先플래그 = 제외行き
	IF ゾーン == "마함"
		SIF GROUPMATCH(行先플래그,묘지行き,破壊行き,戦闘破壊行き,絶対破壊行き) && GETVAR(@"%듀얼리스트%_魔제외置換") >= 1
			行先플래그 = 제외行き
	ENDIF
ENDIF

IF GROUPMATCH(行先플래그,제외行き) && GETVAR(@"%듀얼리스트%_제외NG置換") >= 1
	RETURN 0
ENDIF


選択カード_ID = GETVAR(@"%듀얼리스트%_%ゾーン%:場所")

SIF 選択カード_ID == 使用불가ゾーン
	行先플래그 = 소멸行き
;PRINTFORML 選択カード_ID:{選択カード_ID}
TRYCALLFORM CARDNAME_{選択カード_ID}("名前")
選択カード_名 = %RESULTS%

IF ゾーン == "필드"
	IF GROUPMATCH(元行先플래그,제외行き,덱行き,손패行き,마함行き,破壊行き,戦闘破壊行き,破壊제외行き) && 身代불가 == 0
		SIF 듀얼리스트 == "상대" && GETVAR(@"%듀얼리스트%_MZ_表:場所") == 3
			選択カード_名 = 伏せカード
		CALL CARD_SUBSTEFFECT(듀얼리스트,場所,ゾーン,選択カード_ID,行先플래그)
		IF RESULT == 1
			PRINTFORMW 대역효과가 %選択カード_名%를 지켰습니다。
			RETURN 0
		ENDIF
		IF 元行先플래그 == 破壊行き || 元行先플래그 == 破壊제외行き
			IF GETVAR(@"%듀얼리스트%_MZ_破耐:場所") >= 1
				PRINTFORMW 파괴내성이 %듀얼리스트%의 %選択カード_名%를 지켰습니다。
				RETURN 0
			ELSEIF GETVAR(@"%듀얼리스트%_MZ_一破:場所") >= 1 && GETVAR(@"%듀얼리스트%_MZ_一破済:場所") != 1
				SETVAR @"%듀얼리스트%_MZ_一破済:場所", 1
				PRINTFORMW 일시적 파괴내성이 %듀얼리스트%의 %選択カード_名%를 지켰습니다。
				RETURN 0
			ENDIF

		ELSEIF 元行先플래그 == 戦闘破壊行き
			IF GETVAR(@"%듀얼리스트%_MZ_戦耐:場所") >= 1
				PRINTFORMW 전투파괴내성이 %듀얼리스트%의 %選択カード_名%를 지켰습니다。
				RETURN 0
			ELSEIF GETVAR(@"%듀얼리스트%_MZ_一破:場所") >= 1 && GETVAR(@"%듀얼리스트%_MZ_一破済:場所") != 1
				SETVAR @"%듀얼리스트%_MZ_一破済:場所", 1
				PRINTFORMW 일시적 파괴내성이 %듀얼리스트%의 %選択カード_名%를 지켰습니다。
				RETURN 0
			ENDIF
		ENDIF
	ENDIF

	;――実除去処理――――――――――――――――――――――――――――――――――――――――――――――――
	IF 行先플래그 == 제외行き
		묘지送り종류 = 제외
		TRYCALLFORM CARDCAN_{選択カード_ID}(듀얼리스트,戦破壊→묘지,ゾーン,場所)
		IF RESULT == 1
			CALLFORM CARD_STANBY(選択カード_ID,효과대상者,戦破壊→묘지,"제외")
		ELSE
			CALLFORM CARD_STANBY(選択カード_ID,효과대상者,제외時,"제외")
		ENDIF
		IF 추가플래그 == 0
			CALL CARD_ADD(듀얼리스트, "필드", "제외",場所)
		ELSEIF 추가플래그 == 1
			CALL CARD_ADD(듀얼리스트, "필드", "제외",場所,"帰還")
		ELSEIF 추가플래그 == 2
			CALL CARD_ADD(듀얼리스트, "필드", "제외",場所,"帰墓")
		ELSEIF 추가플래그 == 3
			CALL CARD_ADD(듀얼리스트, "필드", "제외",場所,"帰手")
		ENDIF
	ELSEIF 行先플래그 == 덱行き
		묘지送り종류 = 덱
		TRYCALLFORM CARDCAN_{選択カード_ID}(듀얼리스트,戦破壊→묘지,ゾーン,場所)
		IF RESULT == 1
			CALLFORM CARD_STANBY(選択カード_ID,효과대상者,戦破壊→묘지,"덱")
		ELSE
			CALLFORM CARD_STANBY(選択カード_ID,효과대상者,필드→戦離,"필드")	
		ENDIF
		CALL CARD_ADD(듀얼리스트, "필드", "덱",場所)
	ELSEIF 行先플래그 == 손패行き
		묘지送り종류 = 손패
		TRYCALLFORM CARDCAN_{選択カード_ID}(듀얼리스트,戦破壊→묘지,ゾーン,場所)
		IF RESULT == 1
			CALLFORM CARD_STANBY(選択カード_ID,효과대상者,戦破壊→묘지,"손패")
		ELSE
			CALLFORM CARD_STANBY(選択カード_ID,효과대상者,필드→戦離,"필드")	
		ENDIF
		CALL CARD_ADD(듀얼리스트, "필드", "손패",場所)

	ELSEIF 行先플래그 == 마함行き
		SIF 효과추가カテ != ""
			SETVAR @"%듀얼리스트%_MZ_追カテ:現마함(듀얼리스트)", 효과추가カテ
		IF 추가플래그 == 0
			CALL CARD_ADD(듀얼리스트, "필드", "마함",場所)
		ELSEIF 추가플래그 == 1
			CALL CARD_ADD(듀얼리스트, "필드", "마함",場所,"裏側표시")
		ELSEIF 추가플래그 == 4
			CALL CARD_ADD(듀얼리스트, "필드", "마함",場所,"使用불가")
		ENDIF
	ELSEIF 行先플래그 == EX덱行き
		CALL CARD_ADD(듀얼리스트, "필드", "EX덱",場所)
	ELSEIF 行先플래그 == 破壊行き || 行先플래그 == 絶対破壊行き
		묘지送り종류 = 破壊
		TRYCALLFORM CARDCAN_{選択カード_ID}(듀얼리스트,戦破壊→묘지,ゾーン,場所)
		IF RESULT == 1
			CALLFORM CARD_STANBY(選択カード_ID,효과대상者,戦破壊→묘지,"묘지")
		ELSE
			CALLFORM CARD_STANBY(選択カード_ID,효과대상者,破壊→묘지,"묘지")	
		ENDIF
		CALL CARD_ADD(듀얼리스트, "필드", "묘지",場所)

	ELSEIF 行先플래그 == 破壊제외行き
		묘지送り종류 = 제외
		TRYCALLFORM CARDCAN_{選択カード_ID}(듀얼리스트,戦破壊→묘지,ゾーン,場所)
		IF RESULT == 1
			CALLFORM CARD_STANBY(選択カード_ID,효과대상者,戦破壊→묘지,"제외")
		ELSE
			CALLFORM CARD_STANBY(選択カード_ID,효과대상者,제외時,"필드")
		ENDIF
		CALL CARD_ADD(듀얼리스트, "필드", "제외",場所)
	ELSEIF 行先플래그 == 戦闘破壊行き
		묘지送り종류 = 戦闘破壊
		TRYCALLFORM CARDCAN_{選択カード_ID}(듀얼리스트,戦破壊→묘지,ゾーン,場所)
		IF RESULT == 1				
			CALLFORM CARD_STANBY(選択カード_ID,효과대상者,戦破壊→묘지,"묘지")
		ELSE
			CALLFORM CARD_STANBY(選択カード_ID,효과대상者,戦闘→묘지,"묘지")
		ENDIF	
		CALL CARD_ADD(듀얼리스트, "필드", "묘지",場所)
	ELSEIF 行先플래그 == 完全묘지行き
		CALL CARD_ADD(듀얼리스트, "필드", "묘지",場所,"完全묘지")
	ELSEIF 行先플래그 == 疑似묘지行き
		カード추가先 = 現묘지数(듀얼리스트)
		SETVAR @"%듀얼리스트%_묘지:カード추가先",選択カード_ID
	ELSEIF 行先플래그 == 完全제외行き
		CALL CARD_ADD(듀얼리스트, "필드", "제외",場所)
	ELSEIF 行先플래그 == 소멸行き
		CALL CARD_BANISH(듀얼리스트,"필드",場所)
	ELSE
		묘지送り종류 = リリース
		TRYCALLFORM CARDCAN_{選択カード_ID}(듀얼리스트,戦破壊→묘지,ゾーン,場所)
		IF RESULT == 1	
			CALLFORM CARD_STANBY(選択カード_ID,효과대상者,戦破壊→묘지,"묘지")
		ELSE
			CALLFORM CARD_STANBY(選択カード_ID,효과대상者,필드→묘지,"묘지")	
		ENDIF	
		CALL CARD_ADD(듀얼리스트, "필드", "묘지",場所)
	ENDIF
	CALL KOJO_MESSAGE_SEND("CARD",選択カード_ID,3,行先플래그)
ELSEIF ゾーン == "마함"
	IF GROUPMATCH(元行先플래그,제외行き,덱行き,손패行き,마함行き,破壊行き,戦闘破壊行き)
		SIF 듀얼리스트 == "상대" && GETVAR(@"%듀얼리스트%_魔_表:場所") >= 2
			選択カード_名 = 伏せカード
		CALL CARD_SUBSTEFFECT(듀얼리스트,場所,ゾーン,選択カード_ID,行先플래그)
		IF RESULT == 1
			PRINTFORMW 대역효과가 %選択カード_名%를 지켰습니다。
			RETURN 0
		ENDIF
		IF 元行先플래그 == 破壊行き
			IF GETVAR(@"%듀얼리스트%_魔_破耐:場所") >= 1
				PRINTFORMW 파괴내성이 %듀얼리스트%의 %選択カード_名%를 지켰습니다。
				RETURN 0
			ELSEIF GETVAR(@"%듀얼리스트%_魔_一破:場所") >= 1 && GETVAR(@"%듀얼리스트%_魔_一破済:場所") != 1 
				SETVAR @"%듀얼리스트%_魔_一破済:場所", 1
				PRINTFORMW 일시적 파괴내성이 %듀얼리스트%의 %選択カード_名%를 지켰습니다。
				RETURN 0
			ENDIF
		ENDIF
	ENDIF
	IF 行先플래그 == 제외行き
		묘지送り종류 = 제외
		TRYCALLFORM CARDCAN_{選択カード_ID}(듀얼리스트,戦破壊→묘지,ゾーン,場所)
		IF RESULT == 1
			CALLFORM CARD_STANBY(選択カード_ID,효과대상者,戦破壊→묘지,"제외")
		ELSE
			CALLFORM CARD_STANBY(選択カード_ID,효과대상者,제외時,"제외")
		ENDIF
		IF 추가플래그 == 0
			CALL CARD_ADD(듀얼리스트, "마함", "제외",場所)
		ELSEIF 추가플래그 == 1
			CALL CARD_ADD(듀얼리스트, "마함", "제외",場所,"帰還")
		ELSEIF 추가플래그 == 2
			CALL CARD_ADD(듀얼리스트, "마함", "제외",場所,"帰墓")
		ENDIF
	ELSEIF 行先플래그 == 덱行き
		묘지送り종류 = 덱
		TRYCALLFORM CARDCAN_{選択カード_ID}(듀얼리스트,戦破壊→묘지,ゾーン,場所)
		IF RESULT == 1
			CALLFORM CARD_STANBY(選択カード_ID,효과대상者,戦破壊→묘지,"덱")
		ELSE
			CALLFORM CARD_STANBY(選択カード_ID,효과대상者,필드→戦離,"마함")	
		ENDIF
		CALL CARD_ADD(듀얼리스트, "마함", "덱",場所)
	ELSEIF 行先플래그 == 손패行き
		묘지送り종류 = 손패
		TRYCALLFORM CARDCAN_{選択カード_ID}(듀얼리스트,戦破壊→묘지,ゾーン,場所)
		IF RESULT == 1
			CALLFORM CARD_STANBY(選択カード_ID,효과대상者,戦破壊→묘지,"손패")
		ELSE
			CALLFORM CARD_STANBY(選択カード_ID,효과대상者,필드→戦離,"마함")	
		ENDIF
		CALL CARD_ADD(듀얼리스트, "마함", "손패",場所)
	ELSEIF 行先플래그 == 마함行き
		IF 추가플래그 == 4
			SETVAR @"%듀얼리스트%_魔_表:場所", 3
			SETVAR @"%듀얼리스트%_魔_無効:場所", 2
		ENDIF
		PRINTL 
	ELSEIF 行先플래그 == EX덱行き
		CALL CARD_ADD(듀얼리스트, "마함", "EX덱",場所)
	ELSEIF 行先플래그 == 破壊行き || 行先플래그 == 絶対破壊行き
		묘지送り종류 = 破壊
		TRYCALLFORM CARDCAN_{選択カード_ID}(듀얼리스트,戦破壊→묘지,ゾーン,場所)
		IF RESULT == 1
			CALLFORM CARD_STANBY(選択カード_ID,효과대상者,戦破壊→묘지,"묘지")
		ELSE
			CALLFORM CARD_STANBY(選択カード_ID,효과대상者,破壊→묘지,"묘지")	
		ENDIF
		CALL CARD_ADD(듀얼리스트, "마함", "묘지",場所)
	ELSEIF 行先플래그 == 完全묘지行き
		CALL CARD_ADD(듀얼리스트, "마함", "묘지",場所)
	ELSEIF 行先플래그 == 完全제외行き
		CALL CARD_ADD(듀얼리스트, "마함", "제외",場所)
	ELSEIF 行先플래그 == 疑似묘지行き
		カード추가先 = 現묘지数(듀얼리스트)
		SETVAR @"%듀얼리스트%_묘지:カード추가先",選択カード_ID
	ELSEIF 行先플래그 == 소멸行き
		CALL CARD_BANISH(듀얼리스트,"마함",場所)
	ELSE
		묘지送り종류 = リリース
		TRYCALLFORM CARDCAN_{選択カード_ID}(듀얼리스트,戦破壊→묘지,ゾーン,場所)
		IF RESULT == 1	
			CALLFORM CARD_STANBY(選択カード_ID,효과대상者,戦破壊→묘지,"묘지")
		ELSE
			CALLFORM CARD_STANBY(選択カード_ID,효과대상者,필드→묘지,"묘지")	
		ENDIF
		CALL CARD_ADD(듀얼리스트, "마함", "묘지",場所)
	ENDIF
