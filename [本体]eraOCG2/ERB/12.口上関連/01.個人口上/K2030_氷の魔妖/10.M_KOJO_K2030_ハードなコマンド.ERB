;流れるタイミングは買った後
;==================================================
@M_KOJO_MESSAGE_COM_K2030_153
CALL TRAIN_MESSAGE
CALL M_KOJO_MESSAGE_COM_K2030_153_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K2030_153_1
;-------------------------------------------------
;記入チェック（=0, 非표시、1, 표시）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小분기
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		PRINTFORMW 「はい、콘돔。忘れるなんてどうかしたの？」
		PRINTFORMW 「……は？　恥ずかしがってるところがみたかった？　なんで恥ずかしがる必要があるのよ」
		RETURN 1
	ENDIF
	;基本セット
	PRINTFORMW %CALLNAME:TARGET%から買ってきた콘돔をひょいと投げ渡された。……前買ったやつと違うやつだ！
	PRINTFORMW 「どうせ何度も使うんだし、色々試してみましょうよ。……生と同じ感触だといいわね」
ENDIF
RETURN 1

;==================================================
;154,リミッター解除
;가슴사이즈は元々の大きさ（リミ解使用前）を参照
;==================================================
@M_KOJO_MESSAGE_COM_K2030_154
CALL TRAIN_MESSAGE
CALL M_KOJO_MESSAGE_COM_K2030_154_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K2030_154_1
;-------------------------------------------------
;記入チェック（=0, 非표시、1, 표시）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小분기
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		PRINTFORMW 「……はっ？」
		PRINTFORMW 「えっ？　えっ？　……はっ？　なんで、わたしのむねが……？」
		RETURN 1
	ENDIF
	;基本セット
	SELECTCASE TALENT:TARGET:가슴사이즈 
		;ひんぬー
		CASE -2,-1,0
			PRINTFORMW 「ひゃぁ！？　え、いきなり大きく！？」
			PRINTFORMW 「いや待って！？　じゃあなんでわざわざ小さくしたの！？　わたしのおっぱい！」
		;普通と爆乳
		CASE 1,2
			PRINTFORMW 「きゃあっ！？　ま、またわたしの胸が！？」
			PRINTFORMW 「もう！　どうやってるのよそれぇ……！」
		;泣いていいよ
		CASE -3
			PRINTFORMW 
		;ウシ
		CASE 3
			PRINTFORMW 「きゃあっ！？　ま、また大きく！？」
			PRINTFORMW 「あんたねぇ、こういうこと繰り返してたから普段からも大きくなっちゃったんじゃない！？　ンッ❤」
	ENDSELECT
ENDIF
RETURN 1
