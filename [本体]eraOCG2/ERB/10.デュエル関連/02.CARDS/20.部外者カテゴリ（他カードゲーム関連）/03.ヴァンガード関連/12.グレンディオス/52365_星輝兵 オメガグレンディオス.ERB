@CARDNAME_52365,参照先
;ココで지정カードの名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
		RESULTS = 스타베이더 “Ω” 그렌디오스
	CASE "略称"
		RESULTS = Ω그렌디오스
	CASE "カテゴリ"
		RESULTS:0 = 그렌디오스
		RESULTS:1 = 스타베이더
		RESULTS:2 = 링크 조커
		RESULTS:4 = 뱅가드
ENDSELECT

@CARD_52365(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "외부자"
		RETURN 1
	CASE "종류"
		RETURN 엑시즈
	CASE "속성"
		RETURN 어둠속성
	CASE "종족"
		RETURN 기계족
	CASE "レベル"
		RETURN 10
	CASE "攻撃力"
		RETURN 3000
	CASE "守備力"
		RETURN 3000
	CASE "성별"
		RETURN 無性
	CASE "추가日"
		RETURN 250709
ENDSELECT
RETURN 0

