;最終使用カード――――――――――――――――――――――――――――――――――――――――――――――――――――――――――――――――――――
@CARD_FINAL(듀얼리스트,ゾーン,場所,カードID)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC 対面者
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC カードID
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC リスタート플래그
#DIM DYNAMIC リザルト

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%
CALL RESET_FIELD_CONDITION(ターンプレイヤー,6,1)

;소환情報のリセット
CALL RESET_SUMACT

;前回의 효과後状態の解消
CALL CARD_ACT_RESET
;보존から효과後状態의 이동
CALL CARD_ACT_MOVE
;보존효과後状態の解消
CALL CARD_SAV_RESET
VARSET リスタート플래그

;フリーチェーン開始
CALL CARD_LINKEFFECT(듀얼리스트)
リザルト = RESULT
CALL CARD_LOADUP
CALLFORM ENSLAVE_RELEASE
IF リザルト == 1
	RESTART
ENDIF
RETURN 0

;誘発효과――――――――――――――――――――――――――――――――――――――――――――――――――――――――――――――――――――
@CARD_LINKEFFECT(듀얼리스트,ゾーン,場所)
#DIMS DYNAMIC 実行者
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC 対面者
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 場所
#DIM DYNAMIC カードID
#DIM DYNAMIC 選択カード
#DIM DYNAMIC カウンタ
#DIM DYNAMIC カウンタB
#DIM DYNAMIC カウンタC
#DIMS DYNAMIC 参照先
#DIM DYNAMIC 上級値
#DIM DYNAMIC サーチ範囲
#DIM DYNAMIC フリチェ確認
#DIM DYNAMIC 효과在り
#DIM DYNAMIC 발동在り
#DIM DYNAMIC 待機_選択済,100
#DIM DYNAMIC ループ횟수
#DIM DYNAMIC 選択済, 100
#DIM DYNAMIC 자신_選択済,  100
#DIM DYNAMIC 상대_選択済,  100

#DIM DYNAMIC 효과종류
#DIM DYNAMIC 条件달성
#DIM DYNAMIC 자신_효과無し
#DIM DYNAMIC 상대_효과無し
#DIM DYNAMIC チェーン불가
#DIM DYNAMIC フリチェOFF状態
#DIM DYNAMIC 選択カード_ID

VARSET 選択済, -1
VARSET 자신_選択済, -1
VARSET 상대_選択済, -1

VARSET 誘発選択済,-1
VARSET 자신_誘発選択済,-1
VARSET 상대_誘発選択済,-1

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%
実行者 = %듀얼리스트%

;過去の待機データのリセット
CALL CARD_STANBY_RESET
;_保データから、待機효과へ이동
CALL CARD_STANBY_MOVE
;_保データのリセット
CALL CARD_STANBY_SAV_RESET
;効後_データのリセット（ここからデータが入れられて行く）
CALL CARD_LINK_RESET


;強制효과の보존：
$FORCED_LOOP
ループ횟수 += 1
SELECTCASE ループ횟수
;효과後誘発――――――――――――――――――――――――――――――――――――――――――――――
	CASE 1
		参照先 = 필드
		효과종류 = 효과後強制誘発
		上級値 = 0
		サーチ範囲 = 5
	CASE 2
		参照先 = 마함
		효과종류 = 효과後強制誘発
		上級値 = 10
		サーチ範囲 = 5
	
	CASE 3
		参照先 = 손패
		효과종류 = 효과後強制誘発
		上級値 = 100
		サーチ範囲 = 紙束最大値
	CASE 4
		参照先 = 묘지
		효과종류 = 효과後強制誘発
		上級値 = 200
		サーチ範囲 = 紙束最大値
ENDSELECT

FOR カウンタ,0,サーチ範囲
	VARSET RESULT
	TRYCALLFORM CARDCAN_{GETVAR(@"%実行者%_%参照先%:カウンタ")}(実行者,효과종류,参照先,カウンタ)
	SIF RESULT != 1
		CONTINUE
	CALL CARD_SELECT(実行者,カウンタ + 上級値)
	効後_효과ID:効後_カウンタ = GETVAR(@"%実行者%_%参照先（CS）%:{選択カード（CS）}")
	SIF 参照先 == "필드"
		効後_個別ID:効後_カウンタ = GETVAR(@"%実行者%_MZ_個ID:{選択カード（CS）}");Atra추가
	効後_実行者:効後_カウンタ = %実行者%
	効後_ゾーン:効後_カウンタ = %参照先（CS）%
	効後_효과種:効後_カウンタ = 효과後強制誘発
	効後_設場所:効後_カウンタ = 選択カード（CS）
	効後_カウンタ ++
NEXT
FOR カウンタ,0,サーチ範囲
	VARSET RESULT
	TRYCALLFORM CARDCAN_{GETVAR(@"%対面者%_%参照先%:カウンタ")}(対面者,효과종류,参照先,カウンタ)
	SIF RESULT != 1
		CONTINUE
	CALL CARD_SELECT(対面者,カウンタ + 上級値)
	効後_효과ID:効後_カウンタ = GETVAR(@"%対面者%_%参照先（CS）%:{選択カード（CS）}")
	SIF 参照先 == "필드"
		効後_個別ID:効後_カウンタ = GETVAR(@"%対面者%_MZ_個ID:{選択カード（CS）}");Atra추가
	効後_実行者:効後_カウンタ = %対面者%
	効後_ゾーン:効後_カウンタ = %参照先（CS）%
	効後_효과種:効後_カウンタ = 효과後強制誘発
	効後_設場所:効後_カウンタ = 選択カード（CS）
	効後_カウンタ ++
NEXT

SIF ループ횟수 < 4
	GOTO FORCED_LOOP


IF 특수듀얼종류 == ライディングＤ
	VARSET RESULT
	TRYCALLFORM CARDCAN_{GETVAR(@"%実行者%_엔진")}(実行者,효과後強制誘発,"",0)
	IF RESULT == 1
		CALL CARD_SELECT(実行者,カウンタ + 上級値)
		効後_효과ID:効後_カウンタ = GETVAR(@"%実行者%_엔진")
		効後_実行者:効後_カウンタ = %実行者%
		効後_효과種:効後_カウンタ = 효과後強制誘発
		効後_カウンタ ++
	ENDIF

	VARSET RESULT
	TRYCALLFORM CARDCAN_{GETVAR(@"%対面者%_엔진")}(対面者,효과後強制誘発,"",0)
	IF RESULT == 1
		CALL CARD_SELECT(対面者,カウンタ + 上級値)
		効後_효과ID:効後_カウンタ = GETVAR(@"%対面者%_엔진")
		効後_実行者:効後_カウンタ = %対面者%
		効後_효과種:効後_カウンタ = 효과後強制誘発
		効後_カウンタ ++
	ENDIF
ENDIF


FOR カウンタ,0,20
	VARSET RESULT
	TRYCALLFORM CARDCAN_{GETVAR(@"%実行者%_効後紋章:カウンタ")}(実行者,効後紋章起動,"",カウンタ)
	SIF RESULT != 1
		CONTINUE
	効後_효과ID:効後_カウンタ = GETVAR(@"%実行者%_効後紋章:カウンタ")
	効後_実行者:効後_カウンタ = %実行者%
	効後_효과種:効後_カウンタ = 効後紋章起動
	効後_カウンタ ++
NEXT

FOR カウンタ,0,20
	VARSET RESULT
	TRYCALLFORM CARDCAN_{GETVAR(@"%対面者%_効後紋章:カウンタ")}(対面者,効後紋章起動,"",カウンタ)
	SIF RESULT != 1
		CONTINUE
	効後_효과ID:効後_カウンタ = GETVAR(@"%対面者%_効後紋章:カウンタ")
	効後_実行者:効後_カウンタ = %対面者%
	効後_효과種:効後_カウンタ = 効後紋章起動
	効後_カウンタ ++
NEXT


;발동한 能動的な起動효과를 いったんストック（필드起動 TO EX起動）
;플레이어는 無差別でタイムスタンプ準
FOR カウンタ, (待機_カウンタ - 1), -1, -1
	VARSET RESULT
	TRYCALLFORM CARDFORCE_{待機_효과ID:カウンタ}(待機_듀얼리스트:カウンタ,待機_효과種:カウンタ,待機_ゾーン:カウンタ,待機_場所:カウンタ)
	SIF !INRANGE(待機_효과種:カウンタ, 필드起動, EX덱起動) || RESULT == 1
		CONTINUE
	VARSET RESULT
	TRYCALLFORM CARDCAN_{待機_효과ID:カウンタ}(待機_듀얼리스트:カウンタ,待機_효과種:カウンタ,待機_ゾーン:カウンタ,待機_場所:カウンタ)
	SIF RESULT != 1
		CONTINUE
	選択済:効後_カウンタ = (待機_場所2:カウンタ) + 2000
	SETVAR @"%待機_듀얼리스트:カウンタ%_選択済:効後_カウンタ", (待機_場所2:カウンタ) + 2000
	効後_효과ID:効後_カウンタ = 待機_효과ID:カウンタ
	効後_個別ID:効後_カウンタ = 待機_個別ID:カウンタ;Atra추가
	効後_実行者:効後_カウンタ = %待機_듀얼리스트:カウンタ%
	効後_ゾーン:効後_カウンタ = %待機_ゾーン:カウンタ%
	効後_효과種:効後_カウンタ = 待機_효과種:カウンタ
	効後_設場所:効後_カウンタ = 待機_場所:カウンタ
	SETVAR @"%実行者%_誘発選択済:効後_カウンタ", 待機_효과ID:カウンタ
	効後_STANDB:効後_カウンタ = 1
	効後_カウンタ ++
NEXT

$CHAIN_LOOP
;플레이어는 無差別でタイムスタンプ順
;待機효과발동確認画面――――――――――――――――――――――――――――――――――――――――――――――――――――――――――
;발동한 カードのフリーや효과後 이외의 、효과가 ここにストックされる
IF 実行者 == "자신"
	CUSTOMDRAWLINE —
	PRINTFORML 【대기 효과 발동 확인】（사용할 효과를 선택해 주세요）
	CUSTOMDRAWLINE —
	;발동予定確認コラム
	FOR カウンタ,0,効後_カウンタ
		IF 効後_実行者:カウンタ == "자신"
			SETCOLOR C_W_BLUE 
		ELSEIF 効後_実行者:カウンタ == "상대"
			SETCOLOR C_W_RED
		ENDIF
		SIF カウンタ == 0
			PRINTL 선택된 카드：
		TRYCALLFORM CARDNAME_{効後_효과ID:カウンタ}, "名前"
		PRINTFORML 체인({カウンタ + 1,4})대기 %RESULTS%
		RESETCOLOR
	NEXT
	CUSTOMDRAWLINE —
ENDIF

;チェーンの最後のプレイヤーが현재의 実行者じゃないかつ、
;チェーンの最後의 효과가 チェーン불가能であればば、待機処理をすべて無視し、
;実行者権を대전상대에게 渡して효과後誘発/フリーチェーンの処理へ。
IF 効後_カウンタ >= 1 && 効後_実行者:(効後_カウンタ - 1) != 実行者
	CALLFORM CHAIN_CAN(効後_実行者:(効後_カウンタ - 1),待機_효과種:(効後_カウンタ - 1),効後_ゾーン:(効後_カウンタ - 1),効後_設場所:(効後_カウンタ - 1),効後_효과ID:(効後_カウンタ - 1))
	IF RESULT != 0
		PRINTFORMW %実行者%는 이 카드에 효과에 체인을 할 수 없습니다！！
		IF 実行者 == 듀얼리스트
			実行者 = %対面者%
		ELSEIF 実行者 == 対面者
			実行者 = %듀얼리스트%
		ENDIF
		GOTO CHAIN_SKIP
	ENDIF
ENDIF

;待機발동選択――――――――――――――――――――――――――――――――――――――――――――――――――――――――――
IF 実行者 == "자신"
	VARSET カウンタB
	FOR カウンタ,0,待機_カウンタ
		VARSET RESULT
		SIF INRANGE(待機_효과種:カウンタ, 필드起動, EX덱起動)
			CONTINUE
		SIF 待機_듀얼리스트:カウンタ != "자신"
			CONTINUE
		IF 待機_효과種:カウンタ != 戦破壊→묘지
			TRYCALLFORM CARDCAN_{待機_효과ID:カウンタ}((待機_듀얼리스트:カウンタ),(待機_효과種:カウンタ),(待機_ゾーン:カウンタ),(待機_場所:カウンタ))
			SIF RESULT != 1
				CONTINUE
		ENDIF
		SIF 待機_選択済:カウンタ == 1
			CONTINUE
		カウンタB ++
		SETCOLOR C_W_BLUE 
		CALLFORM CARD_KANPRINT(待機_ゾーン:カウンタ,カウンタ,待機_효과ID:カウンタ,100 + 待機_효과種:カウンタ,1)
		RESETCOLOR
	NEXT

	IF カウンタB == 0
		CUSTOMDRAWLINE —
	ELSEIF カウンタB >= 1
		CUSTOMDRAWLINE —
		PRINTL
		PRINTFORM 　[9999] - 발동 확인
		PRINTFORM　%"[7777] - 필드 / 패 정보 확인",54,RIGHT%
		PRINTL
		BINPUT
		IF RESULT == 7777
			CALL DIPLAY_BOARD(2)
			
			GOTO CHAIN_LOOP
		ELSEIF RESULT != 9999
			選択カード = RESULT
			選択カード_ID = 待機_효과ID:選択カード
			CALL SELECT_UI_SET(実行者,選択カード_ID,8,選択カード)
			SIF RESULT == 1
				GOTO CHAIN_LOOP

			待機_選択済:選択カード = 1
			選択済:効後_カウンタ = (待機_場所2:選択カード) + 2000
			SETVAR @"%実行者%_選択済:効後_カウンタ", (待機_場所2:選択カード) + 2000
			効後_효과ID:効後_カウンタ = 待機_효과ID:選択カード
			効後_個別ID:効後_カウンタ = 待機_個別ID:選択カード;Atra추가
			効後_実行者:効後_カウンタ = %待機_듀얼리스트:選択カード%
			効後_ゾーン:効後_カウンタ = %待機_ゾーン:選択カード%
			効後_효과種:効後_カウンタ = 待機_효과種:選択カード
			効後_設場所:効後_カウンタ = 待機_場所:選択カード
			SETVAR @"%実行者%_誘発選択済:効後_カウンタ", 待機_효과ID:選択カード
			効後_STANDB:効後_カウンタ = 1
			効後_カウンタ ++
			GOTO CHAIN_LOOP
		ENDIF
	ENDIF
ELSEIF 実行者 == "상대"
	FOR カウンタ, (待機_カウンタ - 1), -1, -1
		SIF INRANGE(待機_효과種:カウンタ, 필드起動, EX덱起動)
			CONTINUE
		SIF 待機_듀얼리스트:カウンタ != "상대"
			CONTINUE
		IF 待機_효과種:カウンタ != 戦破壊→묘지
;			PRINTFORML 「{カウンタ}」待機_효과ID:{待機_효과ID:カウンタ}/待機_효과種:{待機_효과種:カウンタ}/待機_ゾーン:%待機_ゾーン:カウンタ%
			TRYCALLFORM CARDCAN_{待機_효과ID:カウンタ}(待機_듀얼리스트:カウンタ,待機_효과種:カウンタ,待機_ゾーン:カウンタ,待機_場所:カウンタ)
			SIF RESULT != 1
				CONTINUE
		ENDIF
		選択カード_ID = 待機_효과ID:カウンタ
		CALL SELECT_UI_SET(実行者,選択カード_ID,8,カウンタ)
		選択済:効後_カウンタ = (待機_場所2:カウンタ) + 2000
		SETVAR @"%待機_듀얼리스트:カウンタ%_選択済:効後_カウンタ", (待機_場所2:カウンタ) + 2000
		効後_효과ID:効後_カウンタ = 待機_효과ID:カウンタ
		効後_個別ID:効後_カウンタ = 待機_個別ID:カウンタ;Atra추가
		効後_実行者:効後_カウンタ = %待機_듀얼리스트:カウンタ%
		効後_ゾーン:効後_カウンタ = %待機_ゾーン:カウンタ%
		効後_효과種:効後_カウンタ = 待機_효과種:カウンタ
		効後_設場所:効後_カウンタ = 待機_場所:カウンタ
		SETVAR @"%実行者%_誘発選択済:効後_カウンタ", 待機_효과ID:カウンタ
		効後_STANDB:効後_カウンタ = 1
		効後_カウンタ ++
	NEXT
ENDIF
SETVAR @"%実行者%_효과無し", 1
IF 자신_효과無し == 0 || 상대_효과無し == 0
	IF 実行者 == 듀얼리스트
		実行者 = %対面者%
	ELSEIF 実行者 == 対面者
		実行者 = %듀얼리스트%
	ENDIF
	GOTO CHAIN_LOOP
ENDIF

;待機발동選択ここまで――――――――――――――――――――――――――――――――――――――――――――――――――――――――――
$CHAIN_SKIP
;효과無し플래그のリセット
VARSET 자신_효과無し
VARSET 상대_효과無し

フリチェOFF状態 = オプション_フリチェ

$CHOICE_LOOP
VARSET 選択カード, 999
VARSET 효과在り
VARSET ループ횟수
VARSET チェーン불가
IF 実行者 == "자신"
	CUSTOMDRAWLINE —
	PRINTFORML 【유발 효과 확인】
	CUSTOMDRAWLINE —
	IF 効後_カウンタ >= 1 && 実行者 == "자신"
		FOR カウンタ,0,効後_カウンタ
			IF 効後_実行者:カウンタ == "자신"
				SETCOLOR C_W_BLUE 
			ELSEIF 効後_実行者:カウンタ == "상대"
				SETCOLOR C_W_RED
			ENDIF
			SIF カウンタ == 0
				PRINTL 선택된 카드：
			IF 効後_STANDB:カウンタ == 1
				TRYCALLFORM CARDNAME_{効後_효과ID:カウンタ}, "名前"
				PRINTFORML 체인({カウンタ + 1,4})대기 『{効後_設場所:カウンタ,3}』 %RESULTS%
				RESETCOLOR
				;レイ追記20250124
				SIF 効後_実行者:カウンタ == "상대"
					TRYCALLFORM CARD_UI(効後_효과ID:カウンタ) 
			ELSE
				IF 選択済:カウンタ >= 3000
					CALL CARD_SELECT(効後_実行者:カウンタ,選択済:カウンタ - 3000)
				ELSEIF 選択済:カウンタ >= 2000
					CALL CARD_SELECT(効後_実行者:カウンタ,選択済:カウンタ - 2000)
				ELSE
