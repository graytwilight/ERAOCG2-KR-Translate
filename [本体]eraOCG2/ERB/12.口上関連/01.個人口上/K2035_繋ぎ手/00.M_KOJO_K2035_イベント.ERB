;夜寝るとき（おやすみ구상）
;==================================================
@M_KOJO_EVENT_K2035_3(ARG,ARG:1)
;구상主はTARGET
;--------------------------------------------------
;記入チェック（=0, 非표시、1, 표시）
LOCAL = 1
;-------------

IF LOCAL
	;-------------------------------------------------
	;小분기
	;-------------------------------------------------
	;自室に帰って寝る（～は【現在地】から【캐릭터の自室】に戻りました）
	;記入チェック（=0, 非표시、1, 표시）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 1
		IF CFLAG:우후후
			;우후후ダウン後に何も言わせない場合はここに何も書かない
			PRINTFORML 
		ELSE
			PRINTFORMW "이제 햇님도 졌으니, 이만 가볼게요?"
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;수면中（～は眠っている）
	;記入チェック（=0, 非표시、1, 표시）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 3
	    SELECTCASE RAND:3
			CASE 0
				PRINTFORMW "으음…… 말하는 손 언니……"
			CASE 1
				PRINTFORMW "깨닫는 손…… 언니이…… 스읍"
			CASE 2
				PRINTFORML "……지키는 손 언니, 박쥐는 좀……"
				PRINTFORMW %CALLNAME:TARGET%는 뭔가 가위에 눌리고 있다……
		ENDSELECT
		RETURN 1
	ENDIF

	;-------------------------------------------------
	;쇠약して寝る（체력500以下,우후후時は우후후強制終了）
	;（캐릭터の自室以外：～を【現在地】から【캐릭터の自室】に運びました）
	;（캐릭터の自室　　：～は疲れたと言って、横になりました）
	;この後ARG == 1 が流れる
	;記入チェック（=0, 非표시、1, 표시）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 5
		IF CFLAG:우후후
			;우후후中にダウン
			PRINTFORMW "정말♡……너무, 강해……요♡"
		ELSE
			PRINTFORMW "으음…… 폐를 끼쳐서 죄송해요……"
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;MASTERがダウン（연모のみ,そのまま一日が終了）
	;記入チェック（=0, 非표시、1, 표시）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 6
		IF CFLAG:우후후
			;우후후中にダウン
			PRINTFORMW "으음…… 괜찮아요❤ 제가 이-만큼 자장가를 불러줄게요……❤"
		ELSE
			PRINTFORMW "정말, 다 큰 어른이니까, 제대로 밤에는 주무세요"
			PRINTFORMW %CALLNAME:TARGET%는 투덜거리며 어떻게든 당신을 침대로 옮겼다……
		ENDIF
		RETURN 1
	ENDIF
ENDIF
RETURN 1

