@CARD_SELL
#DIM DYNAMIC 캐릭터_ID
#DIM DYNAMIC 캐릭터_NO
#DIM DYNAMIC 買取価格
#DIM DYNAMIC カウンタ

PRINTFORML 
PRINTFORML 
PRINTFORML 「이쪽의 카드라면 매입 가능합니다」
CALL MONSTER_LIST(9)
IF RESULT >= 0
	캐릭터_ID = RESULT
	캐릭터_NO = NO:캐릭터_ID
	CALL PRINT_STATE(캐릭터_ID)
	PRINTFORMW 「《%NAME:캐릭터_ID%》인가요？」
	PRINTFORMW 「그럼 사정에 들어가겠으니 잠시만 기다려주세요ー」
	PRINTFORML 
	PRINTFORML 
	CALL CARD_SELL_VALUE(캐릭터_ID,0)
	買取価格 = RESULT
	PRINTFORML 
	PRINTFORML 
	PRINTFORMW 「이상으로 {買取価格}엔에 매입하겠습니다만、어떻게 하시겠습니까ー？」
	PRINTFORML 
	PRINTFORML 
	PRINTL [0] - 판다
	PRINTL [1] - 그만둔다
	BINPUT
    IF RESULT == 0
	    PRINTFORML 
		SETCOLOR C_RED
	    PRINTFORMW 《주의！》 판매한 카드와의 관계가 붕괴하고、판매된 카드가 심한 일을 당할 수도 있지만 정말로 판매하시겠습니까？
        RESETCOLOR
	   	PRINTFORML 
        PRINTL [0] - 그래도 판다
        PRINTL [1] - 역시 그만둔다
        BINPUT
	    IF RESULT == 0
        	PRINTFORML 
   		    PRINTFORMW 「감사합니다ー」
     		PRINTFORML 
        	SETCOLOR C_RED
    	    PRINTFORMW 《%NAME:캐릭터_ID%》를 팔았습니다。
		    RESETCOLOR
        	CFLAG:캐릭터_ID:소유함 = 0
	    	SIF ITEM:(NO:캐릭터_ID) >= 1
	    		ITEM:(NO:캐릭터_ID) -= 1
		    PRINTFORML 
    		PRINTFORMW 소지금 +{買取価格}엔
			CALL MONEY_GET(買取価格)
			SIF 買取価格 >= 100000
				実績_進行보존:52 = 1
	    	PRINTFORML 
		    PRINTFORMW 판매에 출품된 카드 중에서、쇼크를 받아 노려보는듯한 기분이 듭니다。
    		MARK:캐릭터_ID:굴복각인 = 0
    		MARK:캐릭터_ID:반발각인 = 3
    		TALENT:캐릭터_ID:애처 = 0
	    	TALENT:캐릭터_ID:연모 = 0
			TALENT:캐릭터_ID:성별기호 = 3
			TALENT:캐릭터_ID:중고 = 1
    		ABL:캐릭터_ID:친밀 = 0
    		ABL:캐릭터_ID:순종 = 0
        	CFLAG:캐릭터_ID:호감도 = 0
			CFLAG:캐릭터_ID:기정사실 = -1
			CFLAG:캐릭터_ID:시작위치 = 100
			CFLAG:캐릭터_ID:현재위치 = 100
			CALL EVILTY_POINT(10)
    	    ;ここから趣味が分かれる、売られたカードのその後の処理
	    	IF TALENT:캐릭터_ID:성별 == 1
            	PRINTFORML 판매한 %NAME:캐릭터_ID%은 그 후로 험한 꼴을……？
            	PRINTFORML 
            	PRINTL [0] - 당한다
            	PRINTL [1] - 당하지 않는다
                BINPUT
                IF RESULT == 0
        		    CALL CARD_LOST_AFTER(캐릭터_ID)
                ENDIF
        	ENDIF
			;CALL LIST_SHORI(17,캐릭터_ID)
			PRINTFORML 판매한 %NAME:캐릭터_ID%와 영원히 헤어지시겠습니까？
			PRINTFORML 
			PRINTL [0] - 영원히 헤어진다（캐릭터 삭제）
			PRINTL [1] - 또 만날거라 믿는다（캐릭터 보존）
			BINPUT
			IF RESULT == 0
				PRINTFORML 
				PRINTFORMW 판매한 %NAME:캐릭터_ID%은 그 후 여러 듀얼리스트와 샵을 전전하다、어둠 속으로 사라진 모양이다。
				PRINTFORML 
				CALL DELETE_CHARA(캐릭터_ID)
				IF 캐릭터_NO >= 1000 && 캐릭터_NO < 9100
					ADDCHARA 캐릭터_NO
					캐릭터_ID = CHARANUM - 1
					CALL SET_BODYSIZE(캐릭터_ID,0,1)
					; CALL SHOP_BUST_RESET(캐릭터_ID)
					; CALL SHOP_BUTT_RESET(캐릭터_ID)
					CALL CLOTHES_SETTING(캐릭터_ID)
					CFLAG:캐릭터_ID:시작위치 = 100
					CFLAG:캐릭터_ID:현재위치 = 100
					CFLAG:캐릭터_ID:현재위치 = CFLAG:캐릭터_ID:시작위치
					CFLAG:캐릭터_ID:키스상실상대 = -1
					CFLAG:캐릭터_ID:처녀버린상대 = -1
					CFLAG:캐릭터_ID:동정버린상대 = -1
					SIF TALENT:캐릭터_ID:듀얼리스트 >= 1
						CFLAG:캐릭터_ID:오프라인레이트 = 4000
					IF EXISTCSV(NO:캐릭터_ID)
						NAME:캐릭터_ID = %CSVNAME(NO:캐릭터_ID)%
						CALLNAME:캐릭터_ID = %CSVCALLNAME(NO:캐릭터_ID)%
						FOR カウンタ, 0, VARSIZE("MAXBASE")
							MAXBASE:캐릭터_ID:カウンタ = CSVBASE(NO:캐릭터_ID,カウンタ)
						NEXT
						FOR カウンタ, 0, VARSIZE("BASE")
							BASE:캐릭터_ID:カウンタ = CSVBASE(NO:캐릭터_ID,カウンタ)
						NEXT
						FOR カウンタ, 0, VARSIZE("TALENT")
							TALENT:캐릭터_ID:カウンタ = CSVTALENT(NO:캐릭터_ID,カウンタ)

						NEXT
						FOR カウンタ, 0, VARSIZE("ABL")
							ABL:캐릭터_ID:カウンタ = CSVABL(NO:캐릭터_ID,カウンタ)
						NEXT
						FOR カウンタ, 0, VARSIZE("EXP")
							EXP:캐릭터_ID:カウンタ = CSVEXP(NO:캐릭터_ID,カウンタ)
						NEXT
						CFLAG:캐릭터_ID:지금복장 = CSVCFLAG(NO:캐릭터_ID,7)

						CSTR:캐릭터_ID:등장작품 = %CSVCSTR(NO:캐릭터_ID,10)%

						CALL CLOTHES_SETTING(캐릭터_ID)
						CALL SET_BODYSIZE(캐릭터_ID,0,2)
					ENDIF
				ENDIF
			ELSEIF RESULT == 1
				FOR カウンタ,0,10
					SIF お気に入り:カウンタ == 캐릭터_ID
						お気に入り:カウンタ = 0
				NEXT
				FOR カウンタ,0,10
					SIF 소환이력:カウンタ == 캐릭터_ID
						소환이력:カウンタ = 0
				NEXT
				PRINTFORML 
				PRINTFORMW 판매한 %NAME:캐릭터_ID%은 그 후 여러 듀얼리스트와 샵을 전전한 것 같다。
				PRINTFORML 
			ENDIF
        ELSEIF RESULT == 1
	        PRINTFORML 
	        PRINTFORMW 「알겠습니다ー 또 와주세요ー」
        ENDIF
    ELSEIF RESULT == 1
	    PRINTFORML 
	    PRINTFORMW 「알겠습니다ー 또 와주세요ー」
    ENDIF
ENDIF


