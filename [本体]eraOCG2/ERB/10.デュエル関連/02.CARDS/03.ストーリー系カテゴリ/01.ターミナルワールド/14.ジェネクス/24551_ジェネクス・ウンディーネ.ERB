#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
		RESULTS = 제넥스 운디네
	CASE "略称"
		RESULTS = Gn운디네
	CASE "カテゴリ"
		RESULTS:0 = 제넥스
ENDSELECT


@CARD_24551(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
		RETURN 효과モン
	CASE "속성"
		RETURN 물속성
	CASE "종족"
		RETURN 원소정령족
	CASE "レベル"
		RETURN 3
	CASE "攻撃力"
		RETURN 1200
	CASE "守備力"
		RETURN 600
	CASE "추가日"
		RETURN 250922
ENDSELECT

RETURN 0

@CARD_EXPLANATION_24551(종류)
#DIM DYNAMIC 종류

PRINTL 이 카드명의 ②의 효과는 １턴에 １번밖에 사용할 수 없다。
CALLFORM CARD_ABLE(0,소환時)
PRINTL ①：이 카드가 소환됐을 때、덱에서 물속성 몬스터 １장을 묘지로 보내고 발동할 수 있다。
PRINTL 덱에서 "제넥스 컨트롤러" １장을 패에 넣는다。
CALLFORM CARD_ABLE(2,묘지起動)
PRINTL ②：묘지의 이 카드를 제외하고 발동할 수 있다。
PRINTL 패에서 "제넥스 컨트롤러" １장을 특수 소환한다。

@CARDCAN_24551(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,24551)
SIF RESULT == 1
	RETURN 0

IF 종류 == 소환時
	CALL FULLRESET
	효과範囲덱 = 1
	효과条件속성 = 물속성
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0

	CALL FULLRESET
	효과範囲덱 = 1
	효과条件名前 = 제넥스 컨트롤러
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0

	RETURN 1
ELSEIF 종류 == 묘지起動
	SIF 발동済_2(듀얼리스트,24551)
		RETURN 0
	SIF メインフェイズ != 1
		RETURN 0
	SIF ターンプレイヤー != 듀얼리스트
		RETURN 0
	SIF GETVAR(@"%듀얼리스트%_제외NG置換") >= 1
		RETURN 0
	CALL FULLRESET
	효과範囲손패 = 1
	효과条件名前 = 제넥스 컨트롤러
	효과条件소환可 = 1
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ENDIF

@CARDEFFECT_24551(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIMS DYNAMIC 対面者
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 選択カード

IF 종류 == 소환時
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,24551)
	IF RESULT == 0
		CALL FULLRESET
		효과範囲덱 = 1
		효과条件속성 = 물속성
		CALL CARD_ADD_GY_ADV(듀얼리스트,ゾーン,場所)
		IF RESULT == 1
			CALL FULLRESET
			효과範囲덱 = 1
			효과条件名前 = 제넥스 컨트롤러
			CALL CARD_ADD_ADV(듀얼리스트,ゾーン,場所)
		ENDIF
	ENDIF
ELSEIF 종류 == 묘지起動
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,24551,2)
	IF RESULT == 0
		CALL CARD_DISPOSE(듀얼리스트,ゾーン,場所)

		CALL FULLRESET
		효과範囲손패 = 1
		효과条件名前 = 제넥스 컨트롤러
		CALL CARD_SP_SUMMON_ADV(듀얼리스트,ゾーン,場所)
	ENDIF
ENDIF

