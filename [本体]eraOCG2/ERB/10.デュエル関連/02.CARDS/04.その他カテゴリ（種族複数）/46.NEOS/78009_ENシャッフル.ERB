@CARDNAME_78009,参照先
;ココで지정カードの名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
		RESULTS = EN 셔플
	CASE "略称"
		RESULTS = EN 셔플
	CASE "カテゴリ"
		RESULTS:0 = 네오스
		RESULTS:1 = E•HERO
ENDSELECT


@CARD_78009(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
		RETURN 通常마법
	CASE "レア度"
		RETURN CMN
	CASE "추가日"
		RETURN 250921
ENDSELECT

RETURN 0

@CARD_EXPLANATION_78009(종류)
#DIM DYNAMIC 종류

PRINTL 이 카드명의 ①②의 효과는 각각 １턴에 １번밖에 사용할 수 없다。
CALL CARD_ABLE(1,마함起動 ,손패起動)
PRINTL ①：자신 필드의、"E•HERO" 몬스터 또는 "N" 몬스터 １장을 원래 주인의 덱으로 되돌리고、
PRINTL 그 몬스터와는 카드명이 다른、"E•HERO" 몬스터 또는 "N"  몬스터 １장을 덱에서 특수 소환한다。
CALL CARD_ABLE(2,묘지起動)
PRINTL ②：묘지의 이 카드를 제외하고 발동할 수 있다。
PRINTL "E•HERO" 몬스터와 "N" 몬스터를 １장씩、또는 "엘리멘틀 히어로 네오스" １장을 자신의 묘지에서 덱으로 되돌린다。
PRINTL 그 후、자신은 덱에서 １장 드로우한다。



@CARDCAN_78009(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ


CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,78009)
SIF RESULT == 1
	RETURN 0

IF 종류 == 마함起動 || 종류 == 손패起動
	SIF 발동済_1(듀얼리스트,78009)
		RETURN 0
	SIF メインフェイズ != 1
		RETURN 0
	SIF ターンプレイヤー != 듀얼리스트
		RETURN 0
	CALL FULLRESET
	효과範囲필드 = 1
	효과条件カテゴリ:0 = 네오 스페이시언
	효과条件カテゴリ:1 = E•HERO
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	CALL FULLRESET
	효과範囲덱 = 1
	효과条件カテゴリ:0 = 네오 스페이시언
	효과条件カテゴリ:1 = E•HERO
	효과条件소환可 = 1
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ELSEIF 종류 == 묘지起動
	SIF 발동済_2(듀얼리스트,78009)
		RETURN 0
	SIF メインフェイズ != 1
		RETURN 0
	SIF ターンプレイヤー != 듀얼리스트
		RETURN 0
	SIF GETVAR(@"%듀얼리스트%_제외NG置換") >= 1
		RETURN 0
	CALL CARD_DRAW_CHECK
	CALL FULLRESET
	효과範囲묘지 = 1
	효과条件名前 = 엘리멘틀 히어로 네오스
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT == 1
		RETURN 1
	CALL FULLRESET
	효과範囲묘지 = 1
	효과条件カテゴリ = E•HERO
	효과条件종류 = モン全般
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	IF RESULT == 1
		CALL FULLRESET
		효과範囲묘지 = 1
		효과条件カテゴリ = 네오 스페이시언
		효과条件종류 = モン全般
		CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
		SIF RESULT != 1
		RETURN 0
	ENDIF
	RETURN 1
ENDIF



@CARDEFFECT_78009(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 選択カード
#DIM DYNAMIC 選択カード_ID

IF 종류 == 마함起動 || 종류 == 손패起動
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,78009,1)
	IF RESULT == 0
		CALL CARD_DISPOSE(듀얼리스트,ゾーン,場所)
		CALL FULLRESET
		효과条件カテゴリ:0 = E•HERO
		효과条件カテゴリ:1 = 네오 스페이시언
		효과条件行先플래그 = 덱行き
		CALL CARD_ADD_GY_BASIC(듀얼리스트,"필드",ゾーン,場所)
		IF RESULT == 1
			選択カード_ID = RESULT:1
			CALL FULLRESET
			효과範囲덱 = 1
			효과条件カテゴリ:0 = E•HERO
			효과条件カテゴリ:1 = 네오 스페이시언
			효과条件固ID = 選択カード_ID
			CALL CARD_SP_SUMMON_ADV(듀얼리스트,ゾーン,場所)
		ENDIF
	ENDIF
ELSEIF 종류 == 묘지起動
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,78009,2)
	IF RESULT == 0
		CALL FULLRESET
		효과条件カテゴリ = E•HERO
		효과条件行先플래그 = 덱行き
		효과条件종류 = モン全般
		CALL CARD_ADD_GY_BASIC(듀얼리스트,"묘지",ゾーン,場所)
		IF RESULT == 1
			選択カード_ID = RESULT:1
			IF 選択カード_ID != 38000
				CALL FULLRESET
				효과条件カテゴリ = 네오 스페이시언
				효과条件行先플래그 = 덱行き
				효과条件종류 = モン全般
				CALL CARD_ADD_GY_BASIC(듀얼리스트,"묘지",ゾーン,場所)
				SIF RESULT != 1
					RETURN 0
			ENDIF
			CALL CARDDRAW_BASIC(듀얼리스트,1)
		ENDIF
	ENDIF
ENDIF
