;TFLAG:SELECTCOM분기(1=通常승리 -1=通常패배 -2=引き分け
;10=通常듀얼前 11=パック賭け듀얼前 12=デート賭け듀얼前 13=パンツ賭け듀얼前 14=お持ち帰り듀얼前 15=お前モンスターじゃないか？ 16=スケベ要求듀얼前 20=闇のゲーム
;31=パック賭け승리 32=デート賭け승리 33=パンツ賭け승리 34=お持ち帰り成功 35=カード化승리（牝以外） 36=スケベ要求事後 40=罰ゲーム！
;135=カード化見逃す 235=穏便カード化 335=凌辱後カード化 435=凌辱後見逃す
;50=賭け듀얼패배怒られた 51=パック賭け패배 52=デート패배デート 55=カード化패배 56=スケベ要求패배 156=スケベ要求弱味削除 256=スケベ要求패배패배버릇付きおねだり)
;==================================================
@M_KOJO_MESSAGE_COM_K1953_480
CALL M_KOJO_MESSAGE_COM_K1953_480_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K1953_480_1
;-------------------------------------------------
;記入チェック（=0, 非표시、1, 표시）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;これが０の時はデュエルしない時なのでおわり
	SIF TFLAG:SELECTCOM분기 == 0
		RETURN 1

	;デュエル前
	IF INRANGE(TFLAG:SELECTCOM분기,10,21)
		SELECTCASE TFLAG:SELECTCOM분기
			;通常듀얼
			CASE 10
				PRINTFORMW 「마음대로 두들겨 패줄 테니까、넌 카드만 들고 있으면 돼」
			
			;ここから賭け듀얼
			;パック賭け
			CASE 11
				PRINTFORMW 「제대로 준비해 두라구요？　・・・별로 갖고 싶진 않지만」
			;デート賭け
			CASE 12
				PRINTFORMW 「어쩔 수 없네・・・자、준비해」
			;パンツ賭け
			CASE 13
				PRINTFORMW 「패、팬티 같은 걸 줄 수 있을 리 없잖아！？　절대 안 질 거야！」
			;お持ち帰り
			CASE 14
				IF TALENT:TARGET:연모 || TALENT:TARGET:굴복됨 || TALENT:TARGET:음란
					PRINTFORMW 「그냥 평범하게 초대하면 되잖아・・・」
				ELSE
					PRINTFORMW 「・・・조만간 찔릴지도 몰라」
				ENDIF
			;モンスターか問い詰める（人間は불가）
			CASE 15
				PRINTFORMW 「왜 들켰는지 모르겠네・・・일단、입막음은 해둘께！」
			;スケベ強要
			CASE 16
				;満更でもない
				IF TALENT:TARGET:패배버릇
					PRINTFORMW 「어、음・・・좋아요❤　이번에는・・・지지 않을 거야・・・❤」
				;恥ずかしい写真は消してくれよなー
				ELSEIF CFLAG:TARGET:약점장악 == 1
					IF EXP:TARGET:패배조교경험 >= 1
						PRINTFORMW 「내가 이기면 그 사진、절대로 지우세요！」
					ELSE
						PRINTFORMW 「쳇・・・내가 이기면、그거 버려요！」
					ENDIF
				;金金金！듀얼리스트として恥ずかしくないのか！
				ELSE
					PRINTFORMW 「이 돈만 있으면、이것도 저것도・・・자、준비해！」
				ENDIF
			;闇のゲーム
			CASE 20
				PRINTFORMW 「싫은 느낌・・・이기지 않으면 안된다는 것이지・・・！」
		ENDSELECT

	;デュエル後
	ELSE
		SELECTCASE TFLAG:SELECTCOM분기
			;通常승리
			CASE 1
				PRINTFORML 「뭐、이 정도로군。
				PRINTFORMW 　다음에도 상대해 줄 테니까、그때까지 실력을 갈고닦아 두어라」
			;通常패배
			CASE -1
				PRINTFORMW 「잘、잘하네・・・다음에는 지지 않을 거야！」
			;通常引き分け
			CASE -2
				PRINTFORMW 「무승부네・・・다음에는 이길 거야」

			;ここから賭け승리
			;パック賭け
			CASE 31
				PRINTFORMW 「하아・・・어때？　좋은 카드는 나왔어？」
			;デート賭け
			CASE 32
				PRINTFORMW 「자、어디든 데려가세요。　%CALLNAME:MASTER%이 에스코트 해주실 거죠？」
			;パンツ賭け
			CASE 33
				PRINTFORM 「으으・・・초조해・・・
				SIF ABL:TARGET:노출증 >= 3
					PRINT ❤
				PRINTFORMW 」
			;お持ち帰り
			CASE 34
				IF TALENT:TARGET:연모 || TALENT:TARGET:굴복됨 || TALENT:TARGET:음란
					PRINTFORMW 「억지라니까・・・❤」
				ELSE
					PRINTFORMW 「흥・・・이번만이야・・・」
				ENDIF
			;下二桁３５は"モンスターか問い詰める"の분기
			;カード化（牝以外、選択肢無し）
			CASE 35
				PRINTFORMW 
			;カード化を見逃す（元気でな！）
			CASE 135
				PRINTFORMW 「아、고마워・・・！」
			;穏便に話してカード化（怖くないよ～痛くないよ～）
			CASE 235
				PRINTFORMW 「뭐・・・그렇게까지 말한다면・・・」
			;身体を対価に見逃す（見逃すとは言ってない）
			CASE 335
				PRINTFORMW 「아・・・❤　왜、어째서・・・」
			;身体を対価に見逃す（見逃す）（패배버릇付いたり弱み握られたりする）
			CASE 435
				PRINTFORMW 「흐윽・・・우우윽・・・！」
			;身体要求事後
			CASE 36
				;満更でもない
				IF TALENT:TARGET:패배버릇
					PRINTFORMW 「하아・・・에헤헤・・・❤」
				ELSE
					;三回負けたら色違い패배버릇ゲット！
					SELECTCASE EXP:TARGET:패배조교경험
						CASE 1
							PRINTFORMW 「흐윽・・・우우윽・・・！」
						CASE 2
							PRINTFORMW 「하아・・・하아・・・❤　다음에는、지지 않을 거야・・・❤」
					ENDSELECT
				ENDIF
			;闇のゲーム
			CASE 40
				;思いつかないし無くていいや

			;ここから賭け패배
			;怒られた
			CASE 50
				PRINTFORMW 「이걸 교훈 삼아、내기 규얼 같은 건 그만두지 않을래요？」
			;パック賭け（パックを手渡す）
