@CARDNAME_39634,参照先
;ココで지정カードの名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
		RESULTS = 낙인의 노예짐승 키트
	CASE "略称"
		RESULTS = 노예짐승키트
	CASE "カテゴリ"
		RESULTS:0 = 낙인
		RESULTS:1 = 비스테드
		RESULTS:2 = 트라이브리게이드
		RESULTS:3 = E.R.A
ENDSELECT


@CARD_39634(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
		RETURN 효과モン
	CASE "속성"
		RETURN 빛속성
	CASE "종족"
		RETURN 야수족
	CASE "レベル"
		RETURN 2
	CASE "攻撃力"
		RETURN 0
	CASE "守備力"
		RETURN 0
	CASE "튜너"
		RETURN 1
	CASE "성별"
		RETURN 牝性
	CASE "別途ID"
		RETURN 1891
	CASE "連動ID"
		RETURN 1891
	CASE "추가日"
		RETURN 250920
ENDSELECT

RETURN 0

@CARD_EXPLANATION_39634(종류)
#DIM DYNAMIC 종류

CALL TEXT_DECORATION("ROGUE")
PRINTL 이 카드의 카드명은 룰 상、"트라이브리게이드 키트"로도 취급한다。
PRINTL 이 카드는 "데스피아"의 성노예이며、"비스테드의 모체"이기도 하다。
PRINTL 이 카드명의 ①②③의 효과는 각각 １턴에 １번밖에 사용할 수 없다。
CALLFORM CARD_ABLE(1,손패起動,필드起動)
PRINTL ①：패 / 몬스터 존의 이 카드를 릴리스 하고 발동할 수 있다。
PRINTL 자신의 덱 / 묘지에서 레벨２ 이상의 "비스테드" １장을 특수 소환한다。
CALLFORM CARD_ABLE(2,마함起動)
PRINTL ②：자신 / 상대 턴에 이 카드가 마법＆함정 존에 "씨앗주머니"로서 존재하는 경우에 발동할 수 있다。
PRINTL 자신의 패 / 덱에서 레벨４ 이상의 "비스테드" 몬스터 １장을 특수 소환한다。
PRINTL 그 후、이 카드에서 씨앗 카운터를 １개 제거한다。
PRINTL 씨앗 카운터가 없는 경우、대신에 이 카드를 파괴한다。
CALLFORM CARD_ABLE(3,필드→묘지 ,손패→묘지 ,덱→묘지 ,破壊→묘지 ,戦闘→묘지)
PRINTL ③：이 카드가 묘지로 보내진 경우에 발동할 수 있다。
PRINTL 이 카드와 자신 묘지의 "트라이브리게이드 도화 페리지트" １장을 "씨앗주머니"로서 자신의 마법＆함정 존에 놓는다。
PRINTL 그 후、필드의 수컷 몬스터 １장당、이 카드와 "트라이브리게이드 도화 페리지트"에 씨앗 카운터를 １개 놓는다。
CALL TEXT_DECORATION("ROGUE")




@CARDCAN_39634(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIMS DYNAMIC 対面者
#DIM DYNAMIC 装備先
#DIMS DYNAMIC 装備者
#DIM DYNAMIC 条件달성
#DIM DYNAMIC カウンタ

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

CALL 装備판정(듀얼리스트,場所,ゾーン)
装備者 = %RESULTS%
装備先 = RESULT

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,39634)
SIF RESULT == 1
	RETURN 0

IF 종류 == 손패起動 || 종류 == 필드起動
	SIF 발동済_1(듀얼리스트,39634)
		RETURN 0
	SIF メインフェイズ != 1
		RETURN 0
	SIF ターンプレイヤー != 듀얼리스트
		RETURN 0
	CALL FULLRESET
	효과範囲덱 = 1
	효과範囲묘지 = 1
	효과条件カテゴリ = 비스테드
	CALLFORM 효과条件レベル_ADV(2,"以上")
	효과条件소환可 = 1
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ELSEIF 종류 == 마함起動 || 종류 == フリー誘発
	SIF 발동済_2(듀얼리스트,39634)
		RETURN 0
	SIF !GROUPMATCH(ゾーン,"마함")
		RETURN 0
	SIF GETVARS(@"%듀얼리스트%_魔_追カテ:場所") != "種袋"
		RETURN 0
	CALL FULLRESET
	효과範囲손패 = 1
	효과範囲덱 = 1
	효과条件カテゴリ = 비스테드
	CALLFORM 효과条件レベル_ADV(6,"以上")
	효과条件소환可 = 1
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ELSEIF 종류 == 필드→묘지 || 종류 == 손패→묘지 || 종류 == 덱→묘지 || 종류 == 破壊→묘지 || 종류 == 戦闘→묘지
	SIF 발동済_3(듀얼리스트,39634)
		RETURN 0
	SIF 마함数(듀얼리스트) >= 5
		RETURN 0
	RETURN 1
; ELSEIF 종류 == リアクション효과
; 	SIF ゾーン != "마함"
; 		RETURN 0
;	SIF GETVARS(@"%듀얼리스트%_魔_追カテ:場所") != "種袋"
;		RETURN 0
; 	RETURN 1
ENDIF

@CARDEFFECT_39634(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 選択カード
#DIM DYNAMIC 選択カード_ID
#DIMS DYNAMIC 対面者
#DIM DYNAMIC 조교대상
#DIM DYNAMIC 装備先
#DIM DYNAMIC 설치先
#DIMS DYNAMIC 装備者

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

CALL 装備판정(듀얼리스트,場所,ゾーン)
装備者 = %RESULTS%
装備先 = RESULT

IF 종류 == 손패起動 || 종류 == 필드起動
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,39634,1)
	IF RESULT == 0
		CALL FULLRESET
		효과範囲덱 = 1
		효과範囲묘지 = 1
		효과条件カテゴリ = 비스테드
		CALLFORM 효과条件レベル_ADV(2,"以上")
		CALL CARD_SP_SUMMON_ADV(듀얼리스트,ゾーン,場所)
	ENDIF
ELSEIF 종류 == 마함起動 || 종류 == フリー誘発
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,39634,2)
	IF RESULT == 0
		CALL FULLRESET
		효과範囲손패 = 1
		효과範囲덱 = 1
		효과条件カテゴリ = 비스테드
		CALLFORM 효과条件レベル_ADV(4,"以上")
		CALL CARD_SP_SUMMON_ADV(듀얼리스트,ゾーン,場所)
		IF RESULT == 1
			PRINTFORML
			SETCOLOR C_PINK
			PRINTFORML >>——MESSAGE:——————————————————————————————————————————————————————————————————————————————————————————————————————————————————————————————————————————>>>
			RESETCOLOR
			PRINTFORML
			PRINTFORML 키트를 모체로、비스테드 몬스터가 태어난다！
			RESETCOLOR
			PRINTFORML 
			CALL EROTIC_MOVE("疑似出産",NO_TO_CHARANUM(1891),,100)

			IF GETVAR(@"%듀얼리스트%_魔_個Ｃ:場所") == 0
				PRINTFORML
				SETCOLOR C_RED
				PRINTFORML >>——MESSAGE:——————————————————————————————————————————————————————————————————————————————————————————————————————————————————————————————————————————>>>
				RESETCOLOR
				PRINTFORML
				PRINTFORML 한계까지 몬스터를 낳은 키트는 힘이 다했다…
				RESETCOLOR
				PRINTFORML 
				CALL CARD_DISPOSE(듀얼리스트,ゾーン,場所)
			ELSE
				CALL SET_FIELD_INFO(듀얼리스트,"마함",場所,"個Ｃ",-1)
			ENDIF
		ENDIF
	ENDIF
ELSEIF 종류 == 필드→묘지 || 종류 == 손패→묘지 || 종류 == 덱→묘지 || 종류 == 破壊→묘지 || 종류 == 戦闘→묘지
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,39634,3)
	IF RESULT == 0
		설치先 = 現마함(듀얼리스트)
		CALL FULLRESET
		효과範囲묘지 = 1
		효과条件名前 = 트라이브리게이드 키트
		효과추가カテ = 種袋
		CALL CARD_SET_ADV(듀얼리스트,ゾーン,場所,"表側표시")
		IF RESULT == 1
			CALL FULLRESET
			효과範囲필드 = 1
			효과範囲상대필드 = 1
			효과条件성별 = 雄性
			효과条件CNT = 1
			CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
			カウンタ = RESULT
			IF カウンタ
				SETCOLOR C_PINK
				PRINTFORML >>——MESSAGE:——————————————————————————————————————————————————————————————————————————————————————————————————————————————————————————————————————————>>>
				PRINTFORML
				PRINTFORML 낙인의 권유에 따라、키트는 필드의 수컷에게서 씨앗을 집어넣는다。
				PRINTFORML
				RESETCOLOR
				FOR 装備先,0,カウンタ
					CALL EROTIC_MOVE("前穴凌辱",NO_TO_CHARANUM(1891),,100)
					CALL FULLRESET
					CALL SET_FIELD_INFO(듀얼리스트,"마함",설치先,"個Ｃ",1)
				NEXT
			ENDIF
			설치先 = 現마함(듀얼리스트)
			CALL FULLRESET
			효과条件名前 = 트라이브리게이드 도화 페리지트
			CALL CARD_CHOOSE_BASIC(듀얼리스트,"묘지",ゾーン,場所)
			IF RESULT != 9999
				選択カード = RESULT
				選択カード_ID = RESULT:99
				SETCOLOR C_PINK
				PRINTFORML >>——MESSAGE:——————————————————————————————————————————————————————————————————————————————————————————————————————————————————————————————————————————>>>
				PRINTFORML
				PRINTFORML 낙인의 권유에 따라、키트는 언니인 페리지트를 자신과 같은 길로 끌어냈다。
				PRINTFORMW
				IF 選択カード_ID == 1890
					CALL CARD_RECONSTRUCT(듀얼리스트,"묘지",選択カード,39633)
					PRINTFORML
				ENDIF
				RESETCOLOR
				CALL FULLRESET
				효과추가カテ = 種袋
				CALL CARD_DISPOSE(듀얼리스트,"묘지",選択カード,마함行き)
				CALL FULLRESET
				효과範囲필드 = 1
				효과範囲상대필드 = 1
				효과条件성별 = 雄性
				효과条件CNT = 1
				CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
				カウンタ = RESULT
				IF カウンタ
					SETCOLOR C_PINK
					PRINTFORML >>——MESSAGE:——————————————————————————————————————————————————————————————————————————————————————————————————————————————————————————————————————————>>>
					PRINTFORML
					PRINTFORML 낙인의 권유에 따라、페리지트는 필드의 수컷에게서 씨앗을 집어넣는다。
					PRINTFORML
					RESETCOLOR
					FOR 装備先,0,カウンタ
						CALL EROTIC_MOVE("前穴凌辱",NO_TO_CHARANUM(1890),,100)
						CALL FULLRESET
						CALL SET_FIELD_INFO(듀얼리스트,"마함",설치先,"個Ｃ",1)
					NEXT
				ENDIF
			ENDIF
		ENDIF
	ENDIF
ENDIF
