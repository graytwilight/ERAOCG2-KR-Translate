@CARD_EXPLANATION_52365(종류)
#DIM DYNAMIC 종류

CALL CARD_ABLE(1,EX덱起動)
PRINTL 이 카드명의 카드는 １턴에 １번、자신 필드에 "스타베이더"ＥＸ몬스터가 존재하지 않는 경우에、
PRINTL "스타베이더"몬스터 １장 위에 겹쳐서만 Ｘ소환할 수 있다。
PRINTL 이 카드명의 ②③④의 효과는 각각 １턴에 １번밖에 사용할 수 없다。
CALL CARD_ABLE(0,지속효과)
PRINTL ①：자신 필드의 "Я"몬스터는 효과 무효를 얻고、공격력이 ２８００이 된다。
PRINTL 이 카드가 필드에 앞면 표시로 존재하는 한、자신은 "그렌디오스"몬스터 밖에 ＥＸ덱에서 특수 소환할 수 없으며,
PRINTL 1턴에 1번, 일반 소환에 더해 패의 "Я"몬스터 1장을 릴리스 없이 소환할 수 있다。
CALL CARD_ABLE(3,효과後誘発)
PRINTL ③：자신 필드에 "Я"몬스터가 소환 / 특수 소환된 경우에、상대 필드의 앞면 표시의 몬스터 １장을 대상으로 하고 발동할 수 있다。
PRINTL 그 몬스터를 주박한다。
SETCOLOR C_L_GRAY
PRINTL （주박상태인 몬스터는 뒷면 수비 표시가 되며、공격 회피를 얻는다。
PRINTL 　다만、상대 필드에 주박상태인 몬스터 밖에 존재하지 않는 경우、자신 필드의 몬스터가 상대 플레이어에게 주는 전투 데미지는 절반이 된다）
RESETCOLOR
CALL CARD_ABLE(4,ターン開始時)
PRINTL ④：상대 스탠바이 페이즈에 발동할 수 있다。
PRINTL 패 / 필드의 "Я"몬스터 １장을 묘지로 보내고、이 턴동안、이하의 효과를 적용한다。
CALL CARD_ABLE(0,短紋章起動)
PRINTL ●상대는 주박상태인 몬스터를 소재로 할 수 없으며、표시 형식을 변경할 수 없다。

@CARDCAN_52365(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所

#DIM DYNAMIC カウンタ
#DIM DYNAMIC 選択カードID
#DIM DYNAMIC 存在カードID,20
#DIM DYNAMIC カウンタB
#DIM DYNAMIC 条件달성

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,52365)
SIF RESULT == 1
	RETURN 0

IF 종류 == EX덱起動
	SIF 발동済_1(듀얼리스트,52365)
		RETURN 0
	SIF メインフェイズ != 1
		RETURN 0
	SIF ターンプレイヤー != 듀얼리스트
		RETURN 0
	CALL CARD_EX_CHECK(듀얼리스트,52365)
	SIF RESULT == 1
		RETURN 0
	CALL FULLRESET
	효과範囲필드 = 1
	효과条件종류 = EXモン全般
	효과条件カテゴリ = 스타베이더
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT == 1
		RETURN 0
	CALL FULLRESET
	효과範囲필드 = 1
	효과条件カテゴリ = 스타베이더
	효과条件重ねてＸ = 1
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ELSEIF 종류 == 필드起動
	SIF ゾーン != "필드"
		RETURN 0
	SIF 발동済_2(듀얼리스트,52365)
		RETURN 0
	SIF メインフェイズ != 1
		RETURN 0
	SIF ターンプレイヤー != 듀얼리스트
		RETURN 0
	IF GETVARS(@"%最終소환決者%_MZ_改名:最終소환場所") == ""
		TRYCALLFORM CARDNAME_{最終소환ID}("カテゴリ")
		SIF MATCH(RESULTS,"Я")
			条件달성++
	ENDIF
	SIF GETVARS(@"%最終소환決者%_MZ_追カテ:最終소환場所") == "Я"
		条件달성++
	SIF GETVARS(@"%最終소환決者%_MZ_追カテ2:最終소환場所") == "Я"
		条件달성++
	SIF 条件달성 < 1
		RETURN 0
	CALL FULLRESET
	효과範囲상대필드 = 1
	효과条件표시 = 表側표시
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ELSEIF 종류 == ターン開始時
	SIF 발동済_4(듀얼리스트,52365)
		RETURN 0
	SIF ゾーン != "필드"
		RETURN 0
	SIF ターンプレイヤー == 듀얼리스트
		RETURN 0
	CALL FULLRESET
	효과範囲손패 = 1
	효과範囲필드 = 1
	효과条件カテゴリ = Я
	효과条件종류 = モン全般
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ELSEIF 종류 == 지속효과 || 종류 == 攻守変動
	SIF ゾーン != "필드"
		RETURN 0
	RETURN 1
ELSEIF 종류 == 攻撃時強制誘発
	SIF ゾーン != "필드"
		RETURN 0
	CALL FULLRESET
	효과範囲상대필드 = 1
	효과条件표시 = 裏側표시
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	CALL FULLRESET
	효과範囲상대필드 = 1
	효과条件표시 = 表側표시
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT == 1
		RETURN 0
	RETURN 1
ELSEIF 종류 == 短紋章起動
	RETURN 1　
ENDIF

@CARDEFFECT_52365(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIMS DYNAMIC 対面者
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ

#DIM DYNAMIC 選択カード
#DIMS DYNAMIC 選択カードゾーン
#DIM DYNAMIC 選択カードID
#DIM DYNAMIC 소환先
#DIM DYNAMIC 存在カードID,20
#DIM DYNAMIC カウンタB
#DIM DYNAMIC 条件カード数
#DIM DYNAMIC 攻撃力보정
#DIM DYNAMIC 守備力보정
#DIM DYNAMIC 条件달성

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%


IF 종류 == EX덱起動
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,52365,1)
	IF RESULT == 0
		CALL RESETCONDITION_MAT
		必要素材数 = 1
		素材条件カテゴリ = 스타베이더
		重ねてＸ = 1
		TRYCALLFORM XYZSUMMON(듀얼리스트,場所)
	ENDIF
ELSEIF 종류 == 소환誘発
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,52365,3)
	IF RESULT == 0
		CALL FULLRESET
		효과範囲상대필드 = 1
		효과条件표시 = 表側표시
		CALL CARD_REMOVAL_ADV(듀얼리스트,"単体除去",場所,"표시변경")
	ENDIF
ELSEIF 종류 == ターン開始時
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,52365,4)
	IF RESULT == 0
		CALL FULLRESET
		효과範囲손패 = 1
		효과範囲필드 = 1
		효과条件カテゴリ = Я
		효과条件종류 = モン全般
		CALL CARD_ADD_GY_ADV(듀얼리스트,ゾーン,場所)
		IF RESULT == 1
			CALL CARD_RESERV(듀얼리스트,52365,短紋章予約)
		ENDIF
	ENDIF
ELSEIF 종류 == 지속효과
	CALL CARD_EX_RESTRICTION(듀얼리스트,"그렌디오스")

	CALL FULLRESET
	효과条件カテゴリ = Я
	CALL STATCHANGE_EFFECT(듀얼리스트,"自身",カウンタ,"無効",3)

	CALL FULLRESET
	효과条件표시 = 裏側표시
	CALL STATCHANGE_EFFECT(듀얼리스트,"상대필드",場所,"攻避",3)

	CALL FULLRESET
	효과範囲상대필드 = 1
	효과条件표시 = 表側표시
	효과条件CNT = 1
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	IF RESULT == 0
		CALL FULLRESET
		CALL STATCHANGE_EFFECT(듀얼리스트,"필드",場所,"直攻")
	ENDIF

	CALL FULLRESET
	효과条件カテゴリ = Я
	CALL STATCHANGE_HAND(듀얼리스트,"全体",場所,"召可")
	CALL STATCHANGE_HAND(듀얼리스트,"全体",場所,"一星",99)

ELSEIF 종류 == 攻守変動
	FOR カウンタ, 0, 5
		VARSET 条件달성
		選択カードID = GETVAR(@"%듀얼리스트%_필드:カウンタ")
		SIF 選択カードID == -1
			CONTINUE
		IF GETVARS(@"%듀얼리스트%_MZ_改名:カウンタ") == ""
			TRYCALLFORM CARDNAME_{選択カードID}("カテゴリ")
			SIF MATCH(RESULTS,"Я")
				条件달성++
		ENDIF
		SIF GROUPMATCH(GETVARS(@"%듀얼리스트%_MZ_追カテ:カウンタ"),"Я")
			条件달성++
		SIF GROUPMATCH(GETVARS(@"%듀얼리스트%_MZ_追カテ2:カウンタ"),"Я")
			条件달성++
		IF GETVAR(@"%듀얼리스트%_MZ_変化元ID:カウンタ") > -1
			TRYCALLFORM CARDNAME_{GETVAR(@"%듀얼리스트%_MZ_変化元ID:カウンタ")}("カテゴリ")
			SIF MATCH(RESULTS,"Я")
				条件달성++
		ENDIF
		SIF 条件달성 < 1
			CONTINUE
        攻撃力보정 = 2800 - GETVAR(@"%듀얼리스트%_MZ_攻:カウンタ")
        守備力보정 = 2800 - GETVAR(@"%듀얼리스트%_MZ_守:カウンタ")
		CALL STATCHANGE_BASIC(듀얼리스트,"自身",カウンタ,攻撃力보정,守備力보정)
	NEXT
ELSEIF 종류 == 攻撃時強制誘発
	CALL SET_PLAYER_INFO(듀얼리스트,"ダメージ半化",3,1)
ELSEIF 종류 == 短紋章起動
	CALL FULLRESET
	효과条件표시 = 裏側守備표시
	CALL STATCHANGE_EFFECT(듀얼리스트,"상대필드",場所,"不材",3)
	CALL STATCHANGE_EFFECT(듀얼리스트,"상대필드",場所,"表固",3)
ENDIF
