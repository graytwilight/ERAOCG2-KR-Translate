@CARDNAME_71635,参照先
;ココで지정カードの名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
		RESULTS = “Ω” 월드엔드
	CASE "略称"
		RESULTS = Ω월드엔드
	CASE "カテゴリ"
		RESULTS:0 = 그렌디오스
		RESULTS:1 = 스타베이더
		RESULTS:2 = 링크 조커
		RESULTS:4 = 뱅가드
ENDSELECT

@CARD_71635(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "외부자"
		RETURN 1
	CASE "종류"
		RETURN フィールド
	CASE "추가日"
		RETURN 250709
ENDSELECT
RETURN 0

