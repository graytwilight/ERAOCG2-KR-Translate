@CARDNAME_24550,参照先
;ココで指定カードの名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
		RESULTS = 제넥스 컨트롤러
	CASE "略称"
		RESULTS = Gn컨트롤러
	CASE "カテゴリ"
		RESULTS:0 = 제넥스
		RESULTS:1 = 제넥스 컨트롤러
ENDSELECT


@CARD_24550(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
		RETURN 通常モン
	CASE "속성"
		RETURN 어둠속성
	CASE "종족"
		RETURN 기계족
	CASE "レベル"
		RETURN 3
	CASE "攻撃力"
		RETURN 1400
	CASE "守備力"
		RETURN 1200
	CASE "튜너"
		RETURN 1
	CASE "추가日"
		RETURN 250922
ENDSELECT

RETURN 0

