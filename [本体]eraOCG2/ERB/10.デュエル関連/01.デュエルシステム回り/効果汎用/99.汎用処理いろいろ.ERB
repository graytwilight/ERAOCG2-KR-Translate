			RETURN 1
	ENDIF

	IF !GROUPMATCH(종류, 攻守変動,지속효과,常在無効)
		SIF GETVAR(@"%듀얼리스트%_魔_発無:場所") >= 1
			RETURN 1
	ENDIF


ENDIF

IF ゾーン == "손패"
	IF GETVAR(@"%듀얼리스트%_魔香효과") >= 1
		TRYCALLFORM CARD_{カードID}("종류")
