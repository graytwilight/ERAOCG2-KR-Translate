@CARDCAN_124865(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,124865)
SIF RESULT == 1
	RETURN 0
IF 종류 == 필드起動 || 종류 == フリー誘発
	SIF ゾーン != "필드"
		RETURN 0
	SIF 발동済_1(듀얼리스트,124865) && 발동済_2(듀얼리스트,124865)
		RETURN 0
	CALL FULLRESET
	효과範囲필드 = 1
	효과条件他所 = 1
	효과条件표시 = 表側표시
	효과条件カテゴリ = 카게로우
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	CALL FULLRESET
	효과範囲상대필드 = 1
	효과範囲상대마함 = 1
	효과条件除去種 = 묘지
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1

ELSEIF 종류 == 攻撃後誘発
	SIF ゾーン != "필드"
		RETURN 0
	SIF 발동済_1(듀얼리스트,124865)
		RETURN 0
	SIF バトルフェイズ != 1
		RETURN 0
	SIF ターンプレイヤー != 듀얼리스트
		RETURN 0
	SIF 戦闘場所(듀얼리스트) != 場所
		RETURN 0
	RETURN 1
ENDIF

@CARDEFFECT_124865(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC 対面者
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC 条件달성
#DIM DYNAMIC 選択カード,5

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

IF 종류 == 필드起動 || 종류 == フリー誘発
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,124865,15)
	IF RESULT == 0
		CALL FULLRESET
		효과範囲필드 = 1
		효과条件他所 = 1
		효과条件표시 = 表側표시
		효과条件カテゴリ = 카게로우
		CALL CARD_CHOOSE_ADV(듀얼리스트,ゾーン,場所)
		選択カード = RESULT
		IF 選択カード != 9999
			CALL SET_FIELD_INFO(듀얼리스트,"필드",選択カード,"表",3,1)
			CALL FULLRESET
			효과範囲상대필드 = 1
			효과条件任意 = 1
			CALL CARD_REMOVAL_ADV(듀얼리스트,"単体除去",場所,"묘지")
			CALL FULLRESET
			효과範囲상대마함 = 1
			효과条件任意 = 1
			CALL CARD_REMOVAL_ADV(듀얼리스트,"単体除去",場所,"묘지")
		ENDIF
	ENDIF
ELSEIF 종류 == 攻撃後誘発
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,124865)
	IF RESULT == 0
		CALL FULLRESET
		CALL RESRICT_FIELD(듀얼리스트,ゾーン,場所)
	ENDIF
ENDIF


