@CARD_FILTER_BASIC(듀얼리스트,元参照先,ゾーン,場所,めくり数,チェック플래그,カードID)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC 対面者
#DIMS DYNAMIC 実行者
#DIM DYNAMIC めくり数

#DIMS DYNAMIC 元参照先
#DIMS DYNAMIC 参照先
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 場所
#DIM DYNAMIC 上級플래그
#DIM DYNAMIC 上級値
#DIM DYNAMIC 参照先セットアップ, 30

#DIM DYNAMIC カウンタ
#DIM DYNAMIC カウンタB
#DIM DYNAMIC カウンタC
#DIM DYNAMIC カウンタD
#DIM DYNAMIC チェック플래그
#DIM DYNAMIC カードID
#DIM DYNAMIC サーチ範囲
#DIM DYNAMIC 選択カード
#DIM DYNAMIC 条件달성

#DIM DYNAMIC 条件合致数
#DIM DYNAMIC 条件合致レベル数
#DIM DYNAMIC 条件合致攻撃力数
#DIM DYNAMIC 条件合致守備力数
#DIM DYNAMIC 条件合致魔力Ｃ数
#DIM DYNAMIC 条件合致Ｘ素材数
#DIM DYNAMIC 条件合致個別Ｃ数
#DIM DYNAMIC 条件合致悪性Ｃ数
#DIM DYNAMIC 条件合致薬毒Ｃ数

#DIM DYNAMIC 現在最大攻撃力
#DIM DYNAMIC 現在最小攻撃力
#DIMS DYNAMIC MZ魔판정
#DIM DYNAMIC スーパー条件달성
#DIM DYNAMIC 대상カードID
#DIM DYNAMIC 裏側플래그
#DIM DYNAMIC 攻撃力설정

#DIM DYNAMIC 성별판정

;PRINTFORML 듀얼리스트 = %듀얼리스트%
CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

VARSET 選択カード, -1
VARSET 現在最大攻撃力, -1
VARSET 現在最小攻撃力, 999999


IF 元参照先 == "上級"
	SIF 효과範囲필드 == 1
		参照先セットアップ:0 = 1
	SIF 효과範囲마함 == 1
		参照先セットアップ:1 = 1
	SIF 효과範囲손패 == 1
		参照先セットアップ:2 = 1
	SIF 효과範囲묘지 == 1
		参照先セットアップ:3 = 1
	SIF 효과範囲덱 == 1
		参照先セットアップ:4 = 1
	SIF 효과範囲EX덱 == 1
		参照先セットアップ:5 = 1
	SIF 효과範囲제외 == 1
		参照先セットアップ:6 = 1

	SIF 효과範囲상대필드 == 1
		参照先セットアップ:10 = 1
	SIF 효과範囲상대마함 == 1
		参照先セットアップ:11 = 1
	SIF 효과範囲상대손패 == 1
		参照先セットアップ:12 = 1
	SIF 효과範囲상대묘지 == 1
		参照先セットアップ:13 = 1
	SIF 효과範囲상대덱 == 1
		参照先セットアップ:14 = 1
	SIF 효과範囲상대EX덱 == 1
		参照先セットアップ:15 = 1
	SIF 효과範囲상대제외 == 1
		参照先セットアップ:16 = 1
	上級플래그 = 1
ENDIF


$INPUTLOOP
IF 上級플래그
	IF 参照先セットアップ:カウンタB == 0
		カウンタB ++
		IF カウンタB <= 19
			GOTO INPUTLOOP
		ELSE
			GOTO INPUTSKIP
		ENDIF
	ENDIF
	IF カウンタB == 0
		元参照先 = 필드
	ELSEIF カウンタB == 1
		元参照先 = 마함
	ELSEIF カウンタB == 2
		元参照先 = 손패
	ELSEIF カウンタB == 3
		元参照先 = 묘지
	ELSEIF カウンタB == 4
		元参照先 = 덱
	ELSEIF カウンタB == 5
		元参照先 = EX덱
	ELSEIF カウンタB == 6
		元参照先 = 제외

	ELSEIF カウンタB == 10
		元参照先 = 상대필드
	ELSEIF カウンタB == 11
		元参照先 = 상대마함
	ELSEIF カウンタB == 12
		元参照先 = 상대손패
	ELSEIF カウンタB == 13
		元参照先 = 상대묘지
	ELSEIF カウンタB == 14
		元参照先 = 상대덱
	ELSEIF カウンタB == 15
		元参照先 = 상대EX덱
	ELSEIF カウンタB == 16
		元参照先 = 상대제외
	ENDIF
ENDIF

IF 元参照先 == "필드"
	サーチ範囲 = 5
	実行者 = %듀얼리스트%
	参照先 = 필드
ELSEIF 元参照先 == "마함"
	サーチ範囲 = 5
	実行者 = %듀얼리스트%
	参照先 = 마함
	上級値 = 10
ELSEIF 元参照先 == "손패"
	サーチ範囲 = 紙束最大値
	実行者 = %듀얼리스트%
	参照先 = 손패
	上級値 = 100
ELSEIF 元参照先 == "묘지"
	サーチ範囲 = 紙束最大値
	実行者 = %듀얼리스트%
	参照先 = 묘지
	上級値 = 200
ELSEIF 元参照先 == "덱"
	IF めくり数 != 0
		サーチ範囲 = めくり数
	ELSE
		サーチ範囲 = 紙束最大値
	ENDIF
	実行者 = %듀얼리스트%
	参照先 = 덱
	上級値 = 300
ELSEIF 元参照先 == "EX덱"
	IF めくり数 != 0
		サーチ範囲 = めくり数
	ELSE
		サーチ範囲 = 紙束最大値
	ENDIF
	実行者 = %듀얼리스트%
	参照先 = EX덱
	上級値 = 400
ELSEIF 元参照先 == "제외"
	サーチ範囲 = 紙束最大値
	実行者 = %듀얼리스트%
	参照先 = 제외
	上級値 = 500

ELSEIF 元参照先 == "상대필드"
	サーチ範囲 = 5
	実行者 = %対面者%
	参照先 = 필드
	上級値 = 2000
ELSEIF 元参照先 == "상대마함"
	サーチ範囲 = 5
	実行者 = %対面者%
	参照先 = 마함
	上級値 = 2010
ELSEIF 元参照先 == "상대손패"
	サーチ範囲 = 紙束最大値
	実行者 = %対面者%
	参照先 = 손패
	上級値 = 2100
ELSEIF 元参照先 == "상대묘지"
	サーチ範囲 = 紙束最大値
	実行者 = %対面者%
	参照先 = 묘지
	上級値 = 2200
ELSEIF 元参照先 == "상대덱"
	IF めくり数 != 0
		サーチ範囲 = めくり数
	ELSE
		サーチ範囲 = 紙束最大値
	ENDIF
	実行者 = %対面者%
	参照先 = 덱
	上級値 = 2300
ELSEIF 元参照先 == "상대EX덱"

	IF めくり数 != 0
		サーチ範囲 = めくり数
	ELSE
		サーチ範囲 = 紙束最大値
	ENDIF
	実行者 = %対面者%
	参照先 = EX덱
	上級値 = 2400
ELSEIF 元参照先 == "상대제외"
	サーチ範囲 = 紙束最大値
	実行者 = %対面者%
	参照先 = 제외
	上級値 = 2500
ELSE
	サーチ範囲 = 紙束最大値
ENDIF

SIF 上級플래그 == 0
	上級値 = 0

FOR カウンタ,0,サーチ範囲
	SIF GETVAR(@"%実行者%_%参照先%:カウンタ") <= -1
		CONTINUE
		대상カードID = GETVAR(@"%実行者%_%参照先%:カウンタ")

	IF 효과条件固定 > -1
		SIF 효과条件固定 != カウンタ
			CONTINUE
	ENDIF
	IF 효과条件複数 > 0
		SIF MATCH(효과条件選択済, (カウンタ + 上級値))
			CONTINUE
	ENDIF
	IF 효과条件他所 > 0
		IF 元参照先 == ゾーン
			SIF カウンタ == 場所
				CONTINUE
		ENDIF
	ENDIF
	IF 듀얼리스트 != "編集"
		IF GETVAR(@"%듀얼리스트%_アザミナ呪") >= 1 && ゾーン != "어드밴스"
			IF 元参照先 == "묘지" || 元参照先 == "제외" || 元参照先 == "필드"
				CONTINUE
			ENDIF
		ENDIF
	ENDIF
	VARSET 条件달성
	CALL CARD_S_FILTER_SET(実行者,参照先,カウンタ,듀얼리스트,チェック플래그,カードID)
	SIF RESULT == 1
		条件달성 = 1
	CALL CARD_FILTER_SET(実行者,参照先,カウンタ,듀얼리스트,チェック플래그,カードID)
	SIF RESULT == 1
		条件달성 = 1
	SIF 효과条件상대モン && 元参照先 == "상대필드"
		条件달성 = 1
	SIF 条件달성 != 1
		CONTINUE

	IF ゾーン == "어드밴스"
		SIF GETVAR(@"%実行者%_MZ_不材:カウンタ") >= 1
			CONTINUE
	ENDIF
	IF 효과条件固名前起動 == 1
		TRYCALLFORM CARDNAME_{대상カードID}, "名前"
		효과条件固名前:カウンタD = %RESULTS%
		カウンタD ++
	ENDIF
	IF 듀얼리스트 == "자신"
		SIF チェック플래그 == 1
			SKIPDISP チェック플래그
		;상대カードの場合（上級値＋2000）
		IF (参照先 == "필드" || 参照先 == "마함") && 上級値 >= 2000
			IF 参照先 == "필드"		
				CALL CARD_SETCOLOR(実行者,参照先,カウンタ)
				;上級値を使い、裏側플래그まで確認している。
				TRYCALLFORM CARD_KANPRINT("상대필드",(カウンタ + 上級値),대상カードID)
			ELSEIF 参照先 == "마함"		
				CALL CARD_SETCOLOR(実行者,参照先,カウンタ)
				;上級値を使い、裏側플래그まで確認している。
				TRYCALLFORM CARD_KANPRINT("상대마함",(カウンタ + 上級値),대상カードID)
			ENDIF
			RESETCOLOR
		ELSE
			CALL CARD_SETCOLOR(実行者,参照先,カウンタ)
			;上級値を使っている。
			TRYCALLFORM CARD_KANPRINT(元参照先,(カウンタ + 上級値),대상カードID)
			RESETCOLOR
		ENDIF

	ELSEIF 듀얼리스트 == "상대"
		;ゾーン특수：上級소환の릴리스、モンスター묘지送りのためのやつ
		IF ゾーン == "어드밴스"
			IF 参照先 == "필드"
				IF GETVAR(@"%実行者%_MZ_攻:カウンタ") < 現在最小攻撃力
					現在最小攻撃力 = RESULT
					選択カード = カウンタ + 上級値
				ENDIF
			ELSE
				VARSET RESULT
				TRYCALLFORM CARD_{대상カードID}, "攻撃力"
				IF RESULT < 現在最小攻撃力
					現在最小攻撃力 = RESULT
					選択カード = カウンタ + 上級値
				ENDIF
			ENDIF
		ELSE
			IF 実行者 != 듀얼리스트
				IF 参照先 == "필드"
					IF GETVAR(@"%実行者%_MZ_攻:カウンタ") + 99999 > 現在最大攻撃力
						;Anonym修正
						現在最大攻撃力 = GETVAR(@"%実行者%_MZ_攻:カウンタ") + 99999
						選択カード = カウンタ + 上級値
					ENDIF
				ELSE
					VARSET RESULT
					TRYCALLFORM CARD_{대상カードID}, "攻撃力"
					IF  RESULT + 99999 > 現在最大攻撃力
						;Anonym修正
						現在最大攻撃力 = RESULT + 99999
						選択カード = カウンタ + 上級値
					ENDIF
				ENDIF
			ELSE
				IF 参照先 == "필드"
					IF GETVAR(@"%実行者%_MZ_攻:カウンタ") > 現在最大攻撃力
						選択カード = カウンタ + 上級値
					ENDIF
				ELSE
					VARSET RESULT
					TRYCALLFORM CARD_{대상カードID}, "攻撃力"
					攻撃力설정 = RESULT
					CALL ENEMY_PRIORITY_FILTER(대상カードID,攻撃力설정)	
					;PRINTFORML リザルト：{RESULT}/대상カードID:{대상カードID}
					IF RESULT > 現在最大攻撃力
						現在最大攻撃力 = RESULT
						選択カード = カウンタ + 上級値
					ENDIF
				ENDIF
			ENDIF
		ENDIF
	ENDIF

	;合計数確認系
	条件合致数 ++
	IF 参照先 == "필드"
		条件合致レベル数 += GETVAR(@"%実行者%_MZ_星:カウンタ")
		条件合致攻撃力数 += GETVAR(@"%実行者%_MZ_攻:カウンタ")
		条件合致守備力数 += GETVAR(@"%実行者%_MZ_守:カウンタ")

		条件合致Ｘ素材数 += GETVAR(@"%実行者%_MZ_Ｘ:カウンタ") + 1
		条件合致魔力Ｃ数 += GETVAR(@"%実行者%_MZ_魔Ｃ:カウンタ")
		条件合致個別Ｃ数 += GETVAR(@"%実行者%_MZ_個Ｃ:カウンタ")
		条件合致悪性Ｃ数 += GETVAR(@"%実行者%_MZ_悪Ｃ:カウンタ")
		条件合致薬毒Ｃ数 += GETVAR(@"%実行者%_MZ_薬Ｃ:カウンタ")
	ELSE
		TRYCALLFORM CARD_{대상カードID}, "レベル"
		条件合致レベル数 += RESULT
		TRYCALLFORM CARD_{대상カードID}, "攻撃力"
		条件合致攻撃力数 += RESULT
		TRYCALLFORM CARD_{대상カードID}, "守備力"
		条件合致守備力数 += RESULT

		IF 参照先 == "마함"
			条件合致魔力Ｃ数 += GETVAR(@"%実行者%_魔_魔Ｃ:カウンタ")
			条件合致個別Ｃ数 += GETVAR(@"%実行者%_魔_個Ｃ:カウンタ")
			条件合致悪性Ｃ数 += GETVAR(@"%実行者%_魔_悪Ｃ:カウンタ")
			条件合致薬毒Ｃ数 += GETVAR(@"%実行者%_魔_薬Ｃ:カウンタ")
		ENDIF
	ENDIF
	SIF めくり数 == 99
		BREAK
NEXT


IF 上級플래그 && カウンタB <= 19
	カウンタB ++
	GOTO INPUTLOOP
ENDIF
$INPUTSKIP
VARSET RESULT
RESULT:1 = 条件合致数
RESULT:2 = 条件合致レベル数
RESULT:3 = 条件合致攻撃力数
RESULT:4 = 条件合致守備力数

RESULT:7 = 条件合致薬毒Ｃ数
RESULT:8 = 条件合致魔力Ｃ数
RESULT:9 = 条件合致個別Ｃ数
RESULT:10 = 条件合致悪性Ｃ数
RESULT:11 = 条件合致Ｘ素材数
SKIPDISP 0

IF 듀얼리스트 == "자신"
	SIF チェック플래그 != 1
		CUSTOMDRAWLINE —
	RETURN 0
ELSEIF 듀얼리스트 == "상대"
	IF 選択カード != -1
		RETURN 選択カード
	ELSE
		RETURN 9999
	ENDIF
ENDIF



;――――――――――――――――――――――――――――――――――――――――――――――――――――――――――――――――――――――――――――――――――――――――――――――――――――――――――――

@CARD_FILTER_SET(実行者,参照先,カウンタ,듀얼리스트,チェック플래그,カードID)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC 対面者
#DIMS DYNAMIC 実行者
#DIM DYNAMIC めくり数
#DIMS DYNAMIC 元参照先
#DIMS DYNAMIC 参照先
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC カウンタB
#DIM DYNAMIC 설정カードID
#DIM DYNAMIC カードID
#DIMS DYNAMIC カード追カテ,10
#DIMS DYNAMIC カード改名

#DIMS DYNAMIC MZ魔판정
#DIM DYNAMIC チェック플래그
#DIM DYNAMIC 条件달성
#DIM DYNAMIC 성별판정
#DIM DYNAMIC 캐릭터番号
;ザポルグ追記20250102
#DIM DYNAMIC 元レベル
#DIM DYNAMIC 現レベル

#DIM DYNAMIC 攻撃力설정
#DIM DYNAMIC 守備力설정
#DIM DYNAMIC 検出속성,99
#DIM DYNAMIC 検出종족,99

#DIM DYNAMIC 戻り値


;チェック플래그
;1： CARD_CHECK_BASIC用
;2： 특수소환可否の確認
;3： 除去판정用
;4： CARD_START_2用確認
;5： ERO효과用
;6： CARD_START用確認


SIF 参照先 == ""
	RETURN 0

IF チェック플래그 == 5
	戻り値 = -1
ELSE
	戻り値 = 0
ENDIF
SIF 듀얼리스트 == "" ;Anonym추가
	듀얼리스트 = %実行者%
;
IF チェック플래그 == 6
	参照先 = 묘지
	カウンタ = 0
	설정カードID = カードID
ELSEIF チェック플래그 == 4
	설정カードID = カードID
ELSE
	SIF GETVAR(@"%実行者%_%参照先%:カウンタ") == -1
		RETURN 戻り値
	SIF GETVAR(@"%実行者%_%参照先%:カウンタ") == 使用불가ゾーン
		RETURN 戻り値
	설정カードID = GETVAR(@"%実行者%_%参照先%:カウンタ")
ENDIF

IF 효과条件ID > -1
	VARSET 条件달성
	TRYCALLFORM CARD_{설정カードID}, "別途ID"
	IF RESULT >= 1
		SIF MATCH(효과条件ID, RESULT)
			条件달성 ++
	ELSE
		SIF MATCH(효과条件ID, 설정カードID)
			条件달성 ++
	ENDIF
	IF 参照先 == "필드"
		IF GETVAR(@"%実行者%_MZ_変化元ID:カウンタ") > -1
			SIF MATCH(효과条件ID, GETVAR(@"%実行者%_MZ_変化元ID:カウンタ"))
				条件달성 ++
		ENDIF
		;改名されてると名前すべて失い、今の名前のみ
		IF GETVARS(@"%実行者%_MZ_改名:カウンタ") != ""
			VARSET 条件달성
			カード改名 = %GETVARS(@"%実行者%_MZ_改名:カウンタ")%
			TRYCALLFORM CARDNAME_{효과条件ID}, "名前"
			SIF MATCH(RESULTS, カード改名)
				条件달성 ++
		ENDIF
	ELSEIF 参照先 == "마함"
		;改名されてると名前すべて失い、今の名前のみ
		IF GETVARS(@"%実行者%_魔_改名:カウンタ") != ""
			VARSET 条件달성
			カード改名 = %GETVARS(@"%実行者%_魔_改名:カウンタ")%
