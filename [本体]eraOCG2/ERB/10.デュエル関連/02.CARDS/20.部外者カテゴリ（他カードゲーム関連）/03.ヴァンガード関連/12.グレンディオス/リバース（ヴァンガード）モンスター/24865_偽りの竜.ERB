#DIM DYNAMIC 종류

PRINTL 상처를 입고 쓰러진 영웅들은, 허무의 에너지의 영향을 받아 존재가 바뀌어 허무의 첨병으로 변모해갔다……Я하게 된 것이다.

@CARDCAN_24865(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,24865)
SIF RESULT == 1
	RETURN 0

IF 종류 == 손패起動
	SIF 발동済_1(듀얼리스트,24865)
		RETURN 0
	SIF メインフェイズ != 1
		RETURN 0
	SIF ターンプレイヤー != 듀얼리스트
		RETURN 0
	CALL FULLRESET
	효과範囲필드 = 1
	효과条件カテゴリ:0 = 돈트리스
	효과条件カテゴリ:1 = 오버로드
	효과条件固カテゴリ = Я
	효과条件종류 = モン全般
	효과条件重ねてＸ = 1
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ENDIF


@CARDEFFECT_24865(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIMS DYNAMIC 対面者
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 選択カード
#DIM DYNAMIC 選択カード_ID

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%


IF 종류 == 손패起動
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,24865,1)
	IF RESULT == 0
		CALL CARD_DISPOSE(듀얼리스트,ゾーン,場所,소멸行き)
		CALL FULLRESET
		효과範囲필드 = 1
		효과条件カテゴリ:0 = 돈트리스
		효과条件カテゴリ:1 = 오버로드
		효과条件固カテゴリ = Я
		효과条件종류 = モン全般
		효과条件重ねてＸ = 1
		CALL CARD_ADD_GY_ADV(듀얼리스트,ゾーン,場所)
		IF RESULT == 1
			選択カード_ID = RESULT:1
			
			TRYCALLFORM CARDNAME_{選択カード_ID}("カテゴリ")
			IF MATCH(RESULTS,"돈트리스")
				CALL FULLRESET
				CALL CARD_GENERATE(듀얼리스트,124865)
			ELSEIF MATCH(RESULTS,"오버로드")
				CALL FULLRESET
				CALL CARD_GENERATE(듀얼리스트,224865)
			ELSE
				RETURN 0
			ENDIF
		ENDIF
	ENDIF
ENDIF

