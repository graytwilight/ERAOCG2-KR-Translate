ENDIF



@SET_EXIST_CARDS(듀얼리스트,参照先지정)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 参照先지정
#DIM DYNAMIC カウンタ
#DIM DYNAMIC カウンタB
#DIM DYNAMIC カウンタC
#DIM DYNAMIC カード설정
#DIM DYNAMIC サーチ範囲
#DIM DYNAMIC 参照先セットアップ, 30
#DIMS DYNAMIC 参照先
#DIM DYNAMIC 条件달성

IF 参照先지정 == 필드지정
	;필드+마함
	参照先セットアップ:0 = 1
	参照先セットアップ:1 = 1
ELSEIF 参照先지정 == 손패지정
	;손패
	参照先セットアップ:2 = 1
ELSEIF 参照先지정 == 묘지지정
	;묘지
	参照先セットアップ:3 = 1
ELSEIF 参照先지정 == 필드묘지지정
	;필드+마함＋묘지
	参照先セットアップ:0 = 1
	参照先セットアップ:1 = 1
	参照先セットアップ:3 = 1
ELSEIF 参照先지정 == 필드손패지정
	;필드+마함＋손패
	参照先セットアップ:0 = 1
	参照先セットアップ:1 = 1
	参照先セットアップ:2 = 1
ENDIF

효과条件固名前:0 = 仮置き

$INPUTLOOP
IF 参照先セットアップ:カウンタB == 0
	カウンタB ++
	IF カウンタB <= 10
		GOTO INPUTLOOP
	ELSE
		RETURN 1
	ENDIF
ENDIF
IF カウンタB == 0
	参照先 = 필드
	サーチ範囲 = 5
ELSEIF カウンタB == 1
	参照先 = 마함
	サーチ範囲 = 5
ELSEIF カウンタB == 2
	参照先 = 손패
	サーチ範囲 = 紙束最大値
ELSEIF カウンタB == 3
	参照先 = 묘지
	サーチ範囲 = 紙束最大値
ENDIF
FOR カウンタ,0,サーチ範囲
	VARSET 条件달성
	CALL CARD_S_FILTER_SET(듀얼리스트,参照先,カウンタ,듀얼리스트)
	SIF RESULT == 1
		条件달성 = 1
	CALL CARD_FILTER_SET(듀얼리스트,参照先,カウンタ,듀얼리스트)
	SIF RESULT == 1
		条件달성 = 1
	SIF 条件달성 != 1
		CONTINUE
	カウンタC ++
	VARSET RESULTS
	TRYCALLFORM CARDNAME_{GETVAR(@"%듀얼리스트%_%参照先%:カウンタ")},"名前"
	효과条件固名前:カウンタC = %RESULTS%
NEXT

IF カウンタB <= 10
	カウンタB ++
	GOTO INPUTLOOP
ENDIF

RETURN 1