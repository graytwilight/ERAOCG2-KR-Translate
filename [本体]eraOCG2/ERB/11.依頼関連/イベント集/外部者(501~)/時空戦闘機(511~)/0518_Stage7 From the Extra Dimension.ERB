@CARDCAN_288004(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC 対面者
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,288004)
SIF RESULT == 1
	RETURN 0


IF 종류 == EX덱起動
	SIF 발동済_D1(듀얼리스트,288004)
		RETURN 0
	CALL CARD_SS_CHECK(듀얼리스트,288004,"EX덱")
	SIF RESULT == 1
		RETURN 0
	CALL FULLRESET
	효과範囲덱 = 1
	효과条件종류 = モン全般
	효과条件CNT = 1
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT < 10
		RETURN 0
	RETURN 1

ELSEIF 종류 == 소환時 || 종류 == 특수소환時 || 종류 == EX소환時
	SIF ゾーン != "필드"
		RETURN 0
	CALL SET_FIELD_INFO(듀얼리스트,"필드",場所,"個Ｃ",10)

ELSEIF 종류 == 攻撃後強制誘発
	SIF ゾーン != "필드"
		RETURN 0
	SIF GETVAR(@"%듀얼리스트%_戦闘カード") != 場所
		RETURN 0
	SIF GETVAR(@"%対面者%_戦闘カード") == 5
		RETURN 0
	RETURN 1

ELSEIF 종류 == 발동誘発
	SIF ゾーン != "필드"
		RETURN 0
	SIF 효과발동済(듀얼리스트,場所,ゾーン)
		RETURN 0
	IF ターンプレイヤー == 듀얼리스트
		CALL FULLRESET
		효과条件종류 = 함정全般
		CALL CARD_CHECK_START_DATA(듀얼리스트,1)
		SIF RESULT != 1
			RETURN 0
	ELSEIF ターンプレイヤー != 듀얼리스트
		CALL FULLRESET
		효과条件종류 = 마법全般
		CALL CARD_CHECK_START_DATA(듀얼리스트,1)
		SIF RESULT != 1
			RETURN 0
	ENDIF
	RETURN 1

ELSEIF 종류 == ターン終了時強制
	SIF ゾーン != "필드"
		RETURN 0
	RETURN 1

ELSEIF 종류 == 지속효과
	SIF ゾーン != "필드"
		RETURN 0
	RETURN 1
ENDIF


@CARDEFFECT_288004(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC ライフ変数値

IF 종류 == EX덱起動
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,288004,20)
	IF RESULT == 0
		REPEAT 99
			CALL FULLRESET
			효과範囲덱 = 1
			효과条件종류 = モン全般
			CALL CARD_ADD_GY_ADV(듀얼리스트,ゾーン,場所)

			CALL FULLRESET
			효과範囲묘지 = 1
			효과条件종류 = モン全般
			효과条件CNT = 1
			CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
			SIF RESULT >= 10
				BREAK
			CALL FULLRESET
			효과範囲덱 = 1
			효과条件종류 = モン全般
			CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
			SIF RESULT == 0
				RETURN 0
		REND
		CALL FULLRESET
		CALL CARD_EX_SUMMON(듀얼리스트,"EX덱",場所)
	ENDIF

ELSEIF 종류 == 攻撃後強制誘発
	IF GETVAR(@"%듀얼리스트%_MZ_個Ｃ:場所") < 1
		CALL FLASH(1)
		INPUTMOUSEKEY 100
		CALL FLASH(2)
		INPUTMOUSEKEY 200
		SETCOLOR C_YELLOW
		PRINTW 《열화 거대요새 빅 코어ＭｋーＥ.Ｒ.Ａ》　굉침！
		RESETCOLOR
		CALL CARD_RECONSTRUCT(듀얼리스트,"필드",場所,152360)
		CALL CARD_DISPOSE(듀얼리스트,ゾーン,場所,完全묘지行き)
		ライフ変数値 = ライフ_상대 * 9 / 10
		CALL LIFE_CHANGE(듀얼리스트,"減少","コスト",ライフ変数値)
	ELSE
		CALL SET_FIELD_INFO(듀얼리스트,"필드",場所,"個Ｃ",-1)
		PRINTFORMW 빅 코어의 실드가 1개 벗겨졌다! (나머지 {GETVAR(@"%듀얼리스트%_MZ_個Ｃ:場所")}장)
	ENDIF

ELSEIF 종류 == 발동誘発
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,288004,10)
	IF RESULT == 0
		CALL FULLRESET
		CALL CARD_REMOVAL_BASIC(듀얼리스트,"발동無効",場所,"제외")
		RETURN 1
	ENDIF
	RETURN 0

ELSEIF 종류 == ターン終了時強制
	CALL SET_FIELD_INFO(듀얼리스트,"필드",場所,"永攻",1000)
	SETCOLOR C_YELLOW
	PRINTW 빅・코어의 눈동자의 반짝임이 더욱 강해졌다！
	RESETCOLOR

ELSEIF 종류 == 지속효과
	CALL STATCHANGE_EFFECT(듀얼리스트,"自身",場所,"戦耐")
	CALL STATCHANGE_EFFECT(듀얼리스트,"自身",場所,"モ耐")
ENDIF
