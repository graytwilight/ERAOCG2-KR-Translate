;==================================================
@M_KOJO_EVENT_K2035_1(ARG,ARG:1)
;구상主はTARGET
;--------------------------------------------------
;記入チェック（=0, 非표시、1, 표시）
LOCAL = 1
;-------------
IF LOCAL
	;空行
	PRINTFORML
	;-------------------------------------------------
	;一日初めての挨拶분기（MAIN/お外どちらも）
	;記入チェック（=0, 非표시、1, 표시）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && GROUPMATCH(ARG,1,2,6) && CFLAG:TARGET:인사했음 == 0
	PRINTFORML "아, %CALLNAME:MASTER% 씨!"
	PRINTFORMW "『오늘도 하루 좋은 날이 되길』……에헤헤 기도해둘게요!"
		PRINTFORMW
		RETURN 1
	ENDIF

	;-------------------------------------------------
	;小분기
	;-------------------------------------------------
	;MASTERが入ってきた
	;記入チェック（=0, 非표시、1, 표시）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 1
	PRINTFORMW "아, %CALLNAME:MASTER% 씨, 방해하고 있어요!"
		PRINTFORMW
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;TARGETが入ってきた
	;eraOCG2_V0.40_BT_240518時点では流れないため注意
	;記入チェック（=0, 非표시、1, 표시）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 2
	PRINTFORML "아, %CALLNAME:MASTER% 씨!"
	PRINTFORMW %CALLNAME:TARGET%는 볼을 부풀리며 그것만 말하고 등을 돌려 달려갔다
		PRINTFORMW
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;TARGETが風呂に入ってきて一緒に入る
	;eraOCG2_V0.40_BT_240518時点では流れないため注意
	;記入チェック（=0, 非표시、1, 표시）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 3
	PRINTFORML "엣, %CALLNAME:MASTER% 씨!?"
	PRINTFORMW "저, 저기…… 이, 이쪽 실례해도 괜찮을까요!?"
		PRINTFORMW
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;TARGETが風呂に入ってきて出て行った
	;eraOCG2_V0.40_BT_240518時点では流れないため注意
	;記入チェック（=0, 非표시、1, 표시）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 4
	PRINTFORML "아, 들어가 있었군요. 죄송합니다!!"
	PRINTFORMW "으으, 죄송합니다. 노크했어야 했는데……"
		PRINTFORMW
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;TARGETが風呂に入ってきて追い出された
	;未確認
	;記入チェック（=0, 非표시、1, 표시）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 5
		PRINTFORML "엣, %CALLNAME:MASTER% 씨!? 저, 목욕 중이에요!?"
		PRINTFORMW "……뭘 멍하니 있는 거예요!? 나가주세요!!"
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;外出先で遭遇した
	;eraOCG2_V0.40_BT_240522時点では流れないため注意
	;記入チェック（=0, 非표시、1, 표시）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 6
		PRINTFORML "아, %CALLNAME:MASTER% 씨? 우연이네요."
		PRINTFORMW "에헤헤, 『신』이 인연을 맺어준 건가요?"
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;他캐릭터と데이트중に遭遇した
	;eraOCG2_V0.40_BT_240522時点では流れないため注意
	;ARG:1にデート상대
	;記入チェック（=0, 非표시、1, 표시）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 7
		IF TALENT:TARGET:연모
				PRINTFORML 「아、%CALLNAME:MASTER%씨…… 데이트 중、이셨나요」
				PRINTFORMW 「죄송합니다、방해해서……。%CALLNAME:MASTER%씨 같은 분이 인기가 없을 리、없으니」
			ELSE
			PRINTFORML 「아、%CALLNAME:MASTER%さ…… 혹시 방해가 됐나요！？」
			PRINTFORMW 「죄、죄송합니다！　절대로 방해하려는 건 아니었어요……！」
			ENDIF
		RETURN 1
	ENDIF
ENDIF
RETURN 1

