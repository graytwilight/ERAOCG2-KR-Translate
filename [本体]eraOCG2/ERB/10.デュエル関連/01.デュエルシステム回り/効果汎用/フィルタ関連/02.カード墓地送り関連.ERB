@CARD_ADD_DECK_SPC(듀얼리스트,ゾーン,場所,行先플래그,不正隠し)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC 효과대상者
#DIMS DYNAMIC ゾーン
#DIMS DYNAMIC 이동カード名
#DIM DYNAMIC 場所
#DIM DYNAMIC 行先플래그
#DIM DYNAMIC 추가플래그
#DIMS DYNAMIC 対面者
#DIM DYNAMIC 選択カード_ID
#DIM DYNAMIC カード추가先
#DIM DYNAMIC 不正隠し

SIF GETVAR(@"%듀얼리스트%_%ゾーン%:場所") == -1
	RETURN 0

選択カード_ID = GETVAR(@"%듀얼리스트%_%ゾーン%:場所")

CALLFORM CARDNAME_{選択カード_ID}, "名前"
이동カード名 = %RESULTS%
SELECTCASE 듀얼리스트
	CASE "자신"
		SELECTCASE ゾーン
			CASE "손패"
				자신_손패:場所 = -1
			CASE "필드"
				자신_필드:場所 = -1
			CASE "마함"
				자신_마함:場所 = -1
			CASE "덱"
				ARRAYREMOVE 자신_덱, 場所, 1
				자신_덱:(VARSIZE("자신_덱")-1) = -1
			CASE "EX덱"
				ARRAYREMOVE 자신_EX덱, 場所, 1
				자신_EX덱:(VARSIZE("자신_EX덱")-1) = -1
			CASE "묘지"
				자신_묘지:場所 = -1
			CASE "제외"
				자신_제외:場所 = -1
		ENDSELECT
	CASE "상대"
		SELECTCASE ゾーン
			CASE "손패"
				상대_손패:場所 = -1
			CASE "필드"
				상대_필드:場所 = -1
			CASE "마함"
				상대_마함:場所 = -1
			CASE "덱"
				ARRAYREMOVE 상대_덱, 場所, 1
				상대_덱:(VARSIZE("상대_덱")-1) = -1
			CASE "EX덱"
				ARRAYREMOVE 상대_EX덱, 場所, 1
				상대_EX덱:(VARSIZE("상대_EX덱")-1) = -1
			CASE "묘지"
				상대_묘지:場所 = -1
			CASE "제외"
				상대_제외:場所 = -1
		ENDSELECT
ENDSELECT

IF 듀얼리스트 == "자신"
	ARRAYSHIFT 자신_덱, 1 , -1 
ELSEIF 듀얼리스트 == "상대"
	ARRAYSHIFT 상대_덱, 1 , -1
ENDIF

カード추가先 = 0
SETVAR @"%듀얼리스트%_덱:カード추가先", 選択カード_ID
SIF 不正隠し != 1
	PRINTFORMW %듀얼리스트%는 %ゾーン%에서 『{場所}』%이동カード名%를 덱의 맨 위에 넣었습니다。

CALL CARD_FIN_ID_SET(듀얼리스트,選択カード_ID,"효과山送")
CALL CARD_FIN_BIT_SET(듀얼리스트,ゾーン)
RETURN 1


