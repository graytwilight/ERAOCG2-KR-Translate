			ENDSELECT
		ELSE
			SETVAR @"%듀얼리스트%_%효과種%", 효과修正
		ENDIF
	ELSEIF 反転플래그 == 1
		SETVAR @"%対面者%_%효과種%", 효과修正
	ELSEIF 反転플래그 == 2
		SETVAR @"%듀얼리스트%_%효과種%", 효과修正
		SETVAR @"%対面者%_%효과種%", 효과修正
	ENDIF

	SIF 효과修正 != 3 && システム플래그 == 0
		PRINTFORMW %대상名%에 『%효과種%』가 적용되었습니다。

