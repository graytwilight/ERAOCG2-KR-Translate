@CARDNAME_72456,参照先
;ココで指定カードの名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
		RESULTS = 제넥스 디벨로퍼
	CASE "略称"
		RESULTS = Gn디벨로퍼
	CASE "カテゴリ"
		RESULTS:0 = 제넥스
		RESULTS:1 = 게임 오리지널
ENDSELECT


@CARD_72456(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
		RETURN 지속마법
	CASE "추가日"
		RETURN 250922
ENDSELECT

RETURN 0

@CARD_EXPLANATION_72456(종류)
#DIM DYNAMIC 종류

PRINTL 이 카드는 １턴에 １번밖에 발동할 수 없으며、이 카드명의 ①③의 효과는 １턴에 １번밖에 사용할 수 없다。
CALLFORM CARD_ABLE(1,마함起動 ,손패起動)
PRINTL ①：이 카드의 발동시의 효과처리로서、
PRINTL 덱에서 원소정령족인 "제넥스" 몬스터 １장을 패에 넣을 수 있다。
CALLFORM CARD_ABLE(0,攻守変動)
PRINTL ②：필드 위의 기계족 몬스터의 공격력은、자신 필드의 몬스터의 속성 종류×３００ 오른다。
CALLFORM CARD_ABLE(3,마함起動)
PRINTL ③：자신 필드에 땅 / 화염 / 물 / 바람속성 S몬스터가 존재하는 경우에 발동할 수 있다。
PRINTL 이 카드를 묘지로 보내고、EX덱에서 빛속성 또는 어둠속성인 기계족 S몬스터 １장을 특수 소환한다。



@CARDCAN_72456(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,72456)
SIF RESULT == 1
	RETURN 0
IF 종류 == 마함起動 && GETVAR(@"%듀얼리스트%_魔_表:場所") == 1
	SIF 발동済_3(듀얼리스트,75556)
		RETURN 0
	SIF メインフェイズ != 1
		RETURN 0
	SIF ターンプレイヤー != 듀얼리스트
		RETURN 0
	CALL FULLRESET
	효과範囲필드 = 1
	효과条件종류 = 싱크로
	효과条件속성 = 땅속성
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	CALL FULLRESET
	효과範囲필드 = 1
	효과条件종류 = 싱크로
	효과条件속성 = 화염속성
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	CALL FULLRESET
	효과範囲필드 = 1
	효과条件종류 = 싱크로
	효과条件속성 = 물속성
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	CALL FULLRESET
	효과範囲필드 = 1
	효과条件종류 = 싱크로
	효과条件속성 = 바람속성
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0


	CALL FULLRESET
	효과範囲EX덱 = 1
	효과条件종류 = 싱크로
	효과条件종족 = 기계족
	효과条件特不無視 = 1
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ELSEIF 종류 == 마함起動 || 종류 == 손패起動
	SIF 발동済_1(듀얼리스트,75556)
		RETURN 0
	SIF メインフェイズ != 1
		RETURN 0
	SIF ターンプレイヤー != 듀얼리스트
		RETURN 0
	IF 종류 == 마함起動
		SIF GETVAR(@"%듀얼리스트%_魔_表:場所") == 1
			RETURN 0
	ENDIF
	CALL CARD_DRAW_CHECK
	RETURN 1
ELSEIF 종류 == 지속효과	|| 종류 == 攻守変動
	SIF ゾーン != "마함"
		RETURN 0
	SIF GETVAR(@"%듀얼리스트%_魔_表:場所") >= 2	
		RETURN 0
	RETURN 1
ENDIF

@CARDEFFECT_72456(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM 소환カード

IF 종류 == 마함起動 && GETVAR(@"%듀얼리스트%_魔_表:場所") == 1
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,75556,3)
	IF RESULT == 0
		CALL CARD_DISPOSE(듀얼리스트,ゾーン,場所)
		CALL FULLRESET
		효과範囲EX덱 = 1
		효과条件종류 = 싱크로
		효과条件종족 = 기계족
		효과条件特不無視 = 1
		CALL CARD_SP_SUMMON_ADV(듀얼리스트,ゾーン,場所)
		PRINTFORML 『RETURN 1;』
	ENDIF
ELSEIF 종류 == 攻守変動
	;攻撃UP
	CALL FULLRESET
	CALL FIELD_INFO_COUNT(듀얼리스트,"属")
	CALL STATCHANGE_BASIC(듀얼리스트,"필드",場所,(200 * RESULT))
ELSEIF 종류 == 마함起動 || 종류 == 손패起動
	;カード발동確認
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,72456,1)
	IF RESULT == 0
		CALL CARD_DISPOSE(듀얼리스트,ゾーン,場所,마함行き)
		CALL FULLRESET
		효과範囲덱 = 1
		효과条件カテゴリ = 제넥스
		효과条件종족 = 원소정령족
		CALL CARD_ADD_ADV(듀얼리스트,ゾーン,場所)
	ENDIF
ENDIF