@M_KOJO_EVENT_K2103_1(ARG,ARG:1)
;구상主はTARGET
;--------------------------------------------------
;記入チェック（=0, 非표시、1, 표시）
LOCAL = 1
;-------------
IF LOCAL
	;-------------------------------------------------
	;小분기
	;-------------------------------------------------
	;MASTERが入ってきた
	;記入チェック（=0, 非표시、1, 표시）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 1
		IF CFLAG:TARGET:1423
			PRINTFORMW 「아아、주인님❤ 기분이 좋으시군요… 후후❤」
			PRINTFORMW %CALLNAME:TARGET%는 어딘가 황홀한 미소를 지으며 인사했다…
		ENDIF
	RETURN 1
		PRINTFORMW 「어머、%CALLNAME:MASTER%님。」
		PRINTFORMW %CALLNAME:TARGET%는 우아하게 인사했다。
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;TARGETが入ってきた
	;記入チェック（=0, 非표시、1, 표시）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 2
		IF CFLAG:TARGET:1423
			PRINTFORMW 「주인님、실례합니다…❤」
		ENDIF
	RETURN 
		PRINTFORMW 「실례합니다、%CALLNAME:MASTER%님。」
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;TARGETが風呂に入ってきて一緒に入る
	;記入チェック（=0, 非표시、1, 표시）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 3
		IF CFLAG:TARGET:1423
			PRINTFORMW 「주인님❤ 봉사하러 왔습니다❤ 후후❤」
		ENDIF
	RETURN 
		PRINTFORMW 「%CALLNAME:MASTER%님、불편하지 않으시다면 등을 씻겨 드릴까요？」
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;TARGETが風呂に入ってきて出て行った
	;記入チェック（=0, 非표시、1, 표시）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 4
		IF CFLAG:TARGET:1423
			PRINTFORMW 「어머 주인님❤ 괜찮아요❤ 이제 나올거라、몸도、꼼꼼히 봐주셔도…❤」
		ENDIF
	RETURN 
		PRINTFORMW 「！？실、실례했습니다！」
		PRINTFORMW %CALLNAME:TARGET%는 얼굴을 새빨갛게 하며 허둥지둥 나가버렸다…
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;TARGETが風呂に入ってきて追い出された
	;記入チェック（=0, 非표시、1, 표시）
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 5
		PRINTFORMW 
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;外出先で遭遇した
	;記入チェック（=0, 非표시、1, 표시）
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 6
		PRINTFORMW
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;他캐릭터と데이트중に遭遇した
	;ARG:1にデート상대
	;記入チェック（=0, 非표시、1, 표시）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 7
		PRINTFORMW  「어머、%CALLNAME:MASTER%님… 과、%CALLNAME:(ARG:1)%씨」
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;MAIN_MAP上で一日最初の挨拶
	;注、このあと通常の遭遇구상、ARG==1 OR ARG==2に接続します
	;記入チェック（=0, 非표시、1, 표시）
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 8
		PRINTFORMW  
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;外出先で一日最初の挨拶
	;ARG:1にデート상대、ARG:1が0の場合は非デート
	;注、このあと通常の外出先で遭遇した구상、ARG==6 OR ARG==7に接続します
	;記入チェック（=0, 非표시、1, 표시）
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 9
		;ARG:1と데이트중
		IF ARG:1
		;一人で外出
		ELSE
		ENDIF
		PRINTFORMW  
		RETURN 1
	ENDIF
ENDIF
RETURN 1

