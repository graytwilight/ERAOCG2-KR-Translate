@CARDNAME_11890,参照先
;ココで지정カードの名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
		RESULTS = 트라이브리게이드 암즈 “Mouser”
	CASE "略称"
		RESULTS = 철수“Mouser”
	CASE "カテゴリ"
		RESULTS:0 = 트라이브리게이드
ENDSELECT


@CARD_11890(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
		RETURN 링크
	CASE "속성"
		RETURN 땅속성
	CASE "종족"
		RETURN 야수족
	CASE "レベル"
		RETURN 2
	CASE "攻撃力"
		RETURN 1800
	CASE "守備力"
		RETURN 1800
	CASE "성별"
		RETURN 牝性
	CASE "連動ID"
		RETURN 1890
	CASE "추가日"
		RETURN 250920
ENDSELECT
RETURN 0

@CARD_EXPLANATION_11890(종류)
