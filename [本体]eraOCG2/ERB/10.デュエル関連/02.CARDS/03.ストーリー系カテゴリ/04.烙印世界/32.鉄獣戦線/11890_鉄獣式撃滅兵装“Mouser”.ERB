@CARDNAME_11890,参照先
;ココで지정カードの名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
		RESULTS = 트라이브리게이드 암즈 “Mouser”
	CASE "略称"
		RESULTS = 철수“Mouser”
	CASE "カテゴリ"
		RESULTS:0 = 트라이브리게이드
ENDSELECT


@CARD_11890(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
		RETURN 링크
	CASE "속성"
		RETURN 땅속성
	CASE "종족"
		RETURN 야수족
	CASE "レベル"
		RETURN 2
	CASE "攻撃力"
		RETURN 1800
	CASE "守備力"
		RETURN 1800
	CASE "성별"
		RETURN 牝性
	CASE "連動ID"
		RETURN 1890
	CASE "추가日"
		RETURN 250920
ENDSELECT
RETURN 0

@CARD_EXPLANATION_11890(종류)
#DIM DYNAMIC 종류


PRINTL 야수족・비행야수족モンスター２体
PRINTL このカード名の①②の효과はそれぞれ１ターンに１度しか使用できない。
CALLFORM CARD_ABLE(1,EX소환時)
PRINTL ①：このカードがL소환した場合に발동できる。
PRINTL 덱・EX덱から「트라이브리게이드 암즈 “Mouser”」以外の「트라이브리게이드」カード２枚を묘지へ送る。（同名カードは１枚まで）
PRINTL このターン、자신は야수족・獣전사족・비행야수족モンスターしかEX덱から특수소환できない。
CALLFORM CARD_ABLE(2,필드→묘지 ,손패→묘지 ,덱→묘지 ,破壊→묘지 ,戦闘→묘지)
PRINTL ②：このカードが묘지へ送られた場合に발동できる。
PRINTL 상대フィールドの表側표시モンスター１体を裏側守備표시にする。



@CARDSUMMON_11890(듀얼리스트,소환カード,EX参照)
#DIMS DYNAMIC 듀얼리스트
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 소환カード
#DIM DYNAMIC EX参照
;素材条件

CALL RESETCONDITION_MAT
必要링크数 = 2
素材条件종족:0 = 야수족
素材条件종족:1 = 비행야수족

IF EX参照 == 1
	CALL LINCAN(듀얼리스트,소환カード,11890) 
	RETURN RESULT
ENDIF


@CARDCAN_11890(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 必要カード

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,11890)
SIF RESULT == 1
	RETURN 0

IF 종류 == EX소환時
	SIF 발동済_1(듀얼리스트,11890)
		RETURN 0
	CALL FULLRESET
	효과範囲덱 = 1
	효과範囲EX덱 = 1
	효과条件종족:0 = 야수족
	효과条件종족:1 = 비행야수족
	효과条件固名前 = 트라이브리게이드 암즈 “Mouser”
	효과条件CNT = 1
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT < 2
		RETURN 0
	RETURN 1

ELSEIF 종류 == 필드→묘지 || 종류 == 손패→묘지 || 종류 == 덱→묘지 || 종류 == 破壊→묘지 || 종류 == 戦闘→묘지
	SIF 발동済_2(듀얼리스트,11890)
		RETURN 0
	CALL CARD_DRAW_CHECK
	RETURN 1
ENDIF

@CARDEFFECT_11890(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIMS DYNAMIC 対面者
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 攻撃力修正
#DIM DYNAMIC 選択カード

IF 종류 == EX소환時
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,11890,1)
	IF RESULT == 0
		CALL FULLRESET
		효과範囲덱 = 1
		효과範囲EX덱 = 1
		효과条件종족:0 = 야수족
		효과条件종족:1 = 비행야수족
		효과条件固名前 = 트라이브리게이드 암즈 “Mouser”
		효과条件複数 = 2
		효과条件固名前選択 = 1
		CALL CARD_ADD_GY_ADV(듀얼리스트,ゾーン,場所)
	ENDIF
ELSEIF 종류 == 필드→묘지 || 종류 == 손패→묘지 || 종류 == 덱→묘지 || 종류 == 破壊→묘지 || 종류 == 戦闘→묘지
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,11890,2)
	IF RESULT == 0
		CALL FULLRESET
		효과範囲상대필드 = 1
		효과条件표시 = 表側표시
		CALL CARD_REMOVAL_ADV(듀얼리스트,"単体除去",場所,"표시변경")
	ENDIF
ENDIF




