VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
		RESULTS = 드래고닉 오버로드
	CASE "略称"
		RESULTS = 오버로
	CASE "カテゴリ"
		RESULTS:0 = 카게로우
		RESULTS:1 = 오버로드
		RESULTS:4 = 뱅가드
ENDSELECT


