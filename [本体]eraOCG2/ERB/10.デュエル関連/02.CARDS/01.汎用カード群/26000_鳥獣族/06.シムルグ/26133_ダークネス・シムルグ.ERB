@CARDNAME_26133,参照先
;ココで지정カードの名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
		RESULTS = 다크니스 시무르그
	CASE "略称"
		RESULTS = 암흑 다무르그
	CASE "カテゴリ"
		RESULTS:0 = 시무르그
ENDSELECT


@CARD_26133(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
		RETURN 효과モン
	CASE "속성"
		RESULT:1 = 바람속성
		RETURN 어둠속성
	CASE "종족"
		RETURN 비행야수족
	CASE "レベル"
		RETURN 8
	CASE "攻撃力"
		RETURN 2900
	CASE "守備力"
		RETURN 2000
	CASE "성별"
		RETURN 牝性
	CASE "추가日"
		RETURN 250917
ENDSELECT

RETURN 0

@CARD_EXPLANATION_26133(종류)
#DIM DYNAMIC 종류

PRINTL 이 카드는 바람속성으로도 취급한다。
PRINTL 이 카드명의 ①②의 효과는 각각 １턴에 １번밖에 사용할 수 없다。
CALLFORM CARD_ABLE(1,효과後誘発)
PRINTL ①：자신이 레벨５ 이상의 어둠 / 바람속성 몬스터를 소환했을 경우에 발동할 수 있다。
PRINTL 패 / 묘지의 이 카드를 특수 소환한다。
CALLFORM CARD_ABLE(2,발동誘発)
PRINTL ②：상대의 마법 / 함정카드의 효과가 발동했을 때、자신 필드의 비행야수족 몬스터 １장을 릴리스하고 발동할 수 있다。
PRINTL 그 발동을 무효로 하고、파괴한다。


@CARDCAN_26133(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC 対面者
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,26133)
SIF RESULT == 1
	RETURN 0

IF 종류 == 효과後誘発
	SIF 발동済_1(듀얼리스트,26133)
		RETURN 0
	SIF !GROUPMATCH(ゾーン,"손패","묘지")
		RETURN 0
	CALL CARD_SS_CHECK(듀얼리스트,26133,ゾーン)
	SIF RESULT == 1
		RETURN 0
	CALL FULLRESET
	효과条件속성:0 = 어둠속성
	효과条件속성:1 = 바람속성
	CALLFORM 효과条件レベル_ADV(5,"以上")
	CALL CARD_CHECK_FIN_DATA(듀얼리스트,"通常소환")
	SIF RESULT != 1
		RETURN 0
	RETURN 1

ELSEIF 종류 == 발동誘発
	SIF 발동済_2(듀얼리스트,26133)
		RETURN 0
	SIF ゾーン != "필드"
		RETURN 0
	CALL FULLRESET
	효과範囲필드 = 1
	효과条件종족 = 비행야수족
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	CALL FULLRESET
	효과条件종류 = 마함全般
	CALL CARD_CHECK_START_DATA(듀얼리스트,1)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ENDIF


@CARDEFFECT_26133(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC 選択カード
#DIMS DYNAMIC 対面者

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

IF 종류 == 효과後誘発
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,26133,1)
	IF RESULT == 0
		CALL FULLRESET
		IF ゾーン == "손패"
			효과範囲손패 = 1
		ELSEIF ゾーン == "묘지"
			효과範囲묘지 = 1
		ENDIF
		효과条件名前 = 다크니스 시무르그
		CALL CARD_SP_SUMMON_ADV(듀얼리스트,ゾーン,場所)
	ENDIF

ELSEIF 종류 == 지속효과
	
ENDIF
