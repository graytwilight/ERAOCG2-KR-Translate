			SIF RESULT != 1
				RETURN 戻り値
		ENDIF
	ENDIF
	
	IF 효과条件所有 >= 1
		IF 효과条件所有 == 1
			IF 듀얼리스트 == "자신"
				SIF GETVAR(@"%実行者%_%MZ魔판정%_所有:カウンタ") != 1
					RETURN 戻り値
			ELSEIF 듀얼리스트 == "상대"
				SIF GETVAR(@"%実行者%_%MZ魔판정%_所有:カウンタ") != 2
					RETURN 戻り値
			ENDIF
		ELSEIF 효과条件所有 == 2
			IF 듀얼리스트 == "자신"
				SIF GETVAR(@"%実行者%_%MZ魔판정%_所有:カウンタ") != 2
					RETURN 戻り値
			ELSEIF 듀얼리스트 == "상대"
				SIF GETVAR(@"%実行者%_%MZ魔판정%_所有:カウンタ") != 1
					RETURN 戻り値
			ENDIF
		ENDIF
	ENDIF
ENDIF


IF 参照先 == "필드"
	IF 효과条件重ねてＸ >= 1
		SIF GETVAR(@"%実行者%_MZ_不材:カウンタ") >= 1
