@CARDCAN_278004(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC 対面者
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,278004)
SIF RESULT == 1
	RETURN 0


IF 종류 == EX덱起動
	CALL CARD_SS_CHECK(듀얼리스트,278004,"EX덱")
	SIF RESULT == 1
		RETURN 0
	CALL FULLRESET
	효과範囲덱 = 1
	효과条件종류 = モン全般
	효과条件CNT = 1
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT < 12
		RETURN 0
	RETURN 1

ELSEIF 종류 == 발동誘発
	SIF ゾーン != "필드"
		RETURN 0
	CALL FULLRESET
	CALL CARD_CHECK_START_DATA(듀얼리스트,1)
	SIF RESULT != 1
		RETURN 0
	RETURN 1

ELSEIF 종류 == ターン終了時強制
	SIF ゾーン != "필드"
		RETURN 0
	RETURN 1

ELSEIF 종류 == 지속효과
	SIF ゾーン != "필드"
		RETURN 0
	RETURN 1
ENDIF


@CARDEFFECT_278004(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC 攻撃力修正

IF 종류 == EX덱起動
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,278004)
	IF RESULT == 0
		REPEAT 99
			CALL FULLRESET
			효과範囲덱 = 1
			효과条件종류 = モン全般
			CALL CARD_ADD_GY_ADV(듀얼리스트,ゾーン,場所)

			CALL FULLRESET
			효과範囲묘지 = 1
			효과条件종류 = モン全般
			효과条件CNT = 1
			CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
			SIF RESULT >= 12
				BREAK
			CALL FULLRESET
			효과範囲덱 = 1
			효과条件종류 = モン全般
			CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
			SIF RESULT == 0
				RETURN 0
		REND
		CALL FULLRESET
		CALL CARD_EX_SUMMON(듀얼리스트,"EX덱",場所)
	ENDIF

ELSEIF 종류 == 발동誘発
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,278004)
	IF RESULT == 0
		CALL FULLRESET
		CALL CARD_REMOVAL_BASIC(듀얼리스트,"발동無効",場所,"제외")
		RETURN 1
	ENDIF
	RETURN 0

ELSEIF 종류 == ターン終了時強制
	攻撃力修正 = GETVAR(@"%듀얼리스트%_MZ_攻:場所")
	CALL SET_FIELD_INFO(듀얼리스트,"필드",場所,"永攻",攻撃力修正)
	SETCOLOR C_YELLOW
	PRINTW 빅・코어의 공격이 더욱 거세져간다！
	RESETCOLOR

ELSEIF 종류 == 지속효과
	CALL STATCHANGE_EFFECT(듀얼리스트,"自身",場所,"戦耐")
	CALL STATCHANGE_EFFECT(듀얼리스트,"自身",場所,"モ耐")
	CALL STATCHANGE_EFFECT(듀얼리스트,"自身",場所,"魔耐")
	CALL STATCHANGE_EFFECT(듀얼리스트,"自身",場所,"함정耐")
ENDIF

