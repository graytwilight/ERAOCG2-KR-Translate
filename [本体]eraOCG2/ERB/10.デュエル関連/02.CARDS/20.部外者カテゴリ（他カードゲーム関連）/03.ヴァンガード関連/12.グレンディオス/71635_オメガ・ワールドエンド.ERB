@CARD_EXPLANATION_71635(종류)
#DIM DYNAMIC 종류

PRINTL 자신 필드는 "Ω월드엔드"가 된다。
PRINTL 이 카드명의 ①②의 효과는 각각 １턴에 １번밖에 사용할 수 없다。
CALL CARD_ABLE(2,마함起動)
PRINTL ①：자신 메인 페이즈에 발동할 수 있다。
PRINTL 덱 / 패에서 "Я" 몬스터 １장을 묘지에 보내고, 덱에서 이 카드명 이외의 "스타베이더"카드 １장을 패에 넣는다。
PRINTL 이 효과를 발동하는 턴, 자신은 "그렌디오스" 몬스터밖에 ＥＸ덱에서 특수 소환할 수 없다。
CALL CARD_ABLE(3,ターン開始時)
PRINTL ③：자신 스탠바이 페이즈에、자신의 ＬＰ가 １０００이하에、
PRINTL 자신 필드에 "그렌디오스"몬스터가 존재하고、상대 필드에 주박상태인 몬스터가 ５장 존재하는 경우에 발동할 수 있다。
PRINTL 자신은 듀얼에서 승리한다。

@CARDCAN_71635(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ数

#DIM DYNAMIC 条件달성

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,71635)
SIF RESULT == 1
	RETURN 0

IF 종류 == 마함起動 && GETVAR(@"%듀얼리스트%_魔_表:場所") == 1
	SIF 발동済_2(듀얼리스트,71635)
		RETURN 0
	SIF メインフェイズ != 1
		RETURN 0
	SIF ターンプレイヤー != 듀얼리스트
		RETURN 0
	CALL FULLRESET
	효과範囲덱 = 1
	효과範囲손패 = 1
	효과条件カテゴリ = Я
	효과条件종류 = モン全般
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	CALL FULLRESET
	효과範囲덱 = 1
	효과条件カテゴリ = 스타베이더
	효과条件固名前 = “Ω” 월드엔드
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	CALL FULLRESET
	효과条件固カテゴリ = 그렌디오스
	CALL CARD_CHECK_FIN_DATA(듀얼리스트,"EX이력")
	SIF RESULT != 0
		RETURN 0
	RETURN 1
ELSEIF 종류 == 마함起動 || 종류 == 손패起動
	SIF 발동済_1(듀얼리스트,71635)
		RETURN 0
	SIF メインフェイズ != 1
		RETURN 0
	SIF ターンプレイヤー != 듀얼리스트
		RETURN 0
	RETURN 1
ELSEIF 종류 == ターン開始時
	SIF 발동済_3(듀얼리스트,71635)
		RETURN 0
	SIF ゾーン != "마함"
		RETURN 0
	SIF ターンプレイヤー != 듀얼리스트
		RETURN 0
	SIF GETVAR(@"%듀얼리스트%_魔_表:場所") >= 2
		RETURN 0
	CALL FULLRESET
	효과範囲필드 = 1
	효과条件カテゴリ = 그렌디오스
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	SIF GETVAR(@"ライフ_%듀얼리스트%") > 1000
		RETURN 0
	CALL FULLRESET
	효과範囲상대필드 = 1
	효과条件표시 = 裏側표시
	효과条件CNT = 1
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT < 5
		RETURN 0
    RETURN 1
ELSEIF 종류 == 지속효과
	SIF ゾーン != "마함"
		RETURN 0
	SIF GETVAR(@"%듀얼리스트%_魔_表:場所") != 1
		RETURN 0
	RETURN 1
ENDIF

@CARDEFFECT_71635(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIMS DYNAMIC 対面者
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 選択カード
#DIM DYNAMIC 選択カードID

#DIM DYNAMIC 条件Ａ
#DIM DYNAMIC 条件Ｂ
#DIM DYNAMIC 選択효과

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%


IF 종류 == 마함起動 && GETVAR(@"%듀얼리스트%_魔_表:場所") == 1
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,71635,2)
	IF RESULT == 0
		CALL FULLRESET
		효과範囲덱 = 1
		효과範囲손패 = 1
		효과条件カテゴリ = Я
		효과条件종류 = モン全般
		CALL CARD_ADD_GY_ADV(듀얼리스트,ゾーン,場所)
		IF RESULT == 1
			CALL FULLRESET
			효과範囲덱 = 1
			효과条件カテゴリ = 스타베이더
			효과条件固名前 = “Ω” 월드엔드
			CALL CARD_ADD_ADV(듀얼리스트,ゾーン,場所)
		ENDIF
		CALL CARD_EX_RESTRICTION(듀얼리스트,"그렌디오스")
	ENDIF
ELSEIF 종류 == 마함起動 || 종류 == 손패起動
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,71635,1)
	IF RESULT == 0
		CALL CARD_DISPOSE(듀얼리스트,ゾーン,場所,마함行き)
	ENDIF
ELSEIF 종류 == ターン開始時
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,71635,3)
	IF RESULT == 0
		IF 듀얼리스트 == "자신"
			SETCOLOR C_YELLOW
			PRINTFORML >>——MESSAGE:——————————————————————————————————————————————————————————————————————————————————————————————————————————————————————————————————————————>>>
			PRINTL 파이널 턴──그리고、“세계”는 끝난다
			PRINTL 얼티밋 브레이크！ 월드 엔드！
			CUSTOMDRAWLINE ―
			RESETCOLOR
		ENDIF
		PRINTL  록                              보이드
		PRINTW 주박 카드의 역장에 의해 생겨난 거대한 허무가、상대 필드의、패의、덱의、묘지의 “모든것”을 집어삼킨다……！
		PRINTL
		CALL SET_PLAYER_INFO(듀얼리스트,"듀얼패배플래그",4,1)
	ENDIF
ELSEIF 종류 == 지속효과
	CALL SET_FIELD_MAGIC_INFO(듀얼리스트,"Ω월드엔드")
ENDIF