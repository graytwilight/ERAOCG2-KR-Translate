			RETURN 戻り値
	ENDIF
	IF 효과条件攻撃可 >= 1
		SIF GETVAR(@"%実行者%_MZ_攻避:カウンタ") >= 1
			RETURN 戻り値
	ENDIF
	IF 효과条件Ｘ素所持 > 0
		IF 효과条件Ｘ素所持 == 999
			SIF GETVAR(@"%実行者%_MZ_Ｘ:カウンタ") <= -1
				RETURN 戻り値

		ELSEIF 효과条件Ｘ素所持 == 888
			SIF GETVAR(@"%実行者%_MZ_Ｘ:カウンタ") >= 0
				RETURN 戻り値

		ELSE
			SIF GETVAR(@"%実行者%_MZ_Ｘ:カウンタ") + 1 < 효과条件Ｘ素所持
				RETURN 戻り値
		ENDIF
	ENDIF


	IF 효과条件튜너 > 0
		SIF GETVAR(@"%実行者%_MZ_Ｔ:カウンタ") == 0
			RETURN 戻り値
	ENDIF
	IF 효과条件속성 > 0
		VARSET 条件달성
		SIF MATCH(효과条件속성, GETVAR(@"%実行者%_MZ_属:カウンタ"))
			条件달성 ++
		;複数속성チェック
		TRYCALLFORM CARD_{설정カードID}, "속성"
		IF RESULT:1 >= 1
			VARSET 検出속성, -1
			FOR カウンタB,0,10
				SIF RESULT:カウンタB <= 0
					CONTINUE
				検出속성:カウンタB = RESULT:カウンタB
			NEXT
			FOR カウンタB,1,10
				IF 検出속성:カウンタB != -1
					SIF MATCH(検出속성:カウンタB, 효과条件속성)
						条件달성 ++
					SIF MATCH(효과条件속성, 検出속성:カウンタB)
						条件달성 ++
				ENDIF
			NEXT
		ENDIF
		SIF 条件달성 == 0	
			RETURN 戻り値
	ENDIF
	IF 효과条件종족 > 0
		VARSET 条件달성
		SIF MATCH(효과条件종족, GETVAR(@"%実行者%_MZ_種:カウンタ"))
			条件달성 ++
		;複数종족チェック
		TRYCALLFORM CARD_{설정カードID}, "종족"
		IF RESULT:1 >= 1
			VARSET 検出종족, -1
			FOR カウンタB,0,10
				SIF RESULT:カウンタB <= 0
					CONTINUE
				検出종족:カウンタB = RESULT:カウンタB
			NEXT
			FOR カウンタB,1,10
				IF 検出종족:カウンタB != -1
					SIF MATCH(検出종족:カウンタB, 효과条件종족)
						条件달성 ++
					SIF MATCH(효과条件종족, 検出종족:カウンタB)
						条件달성 ++
				ENDIF
			NEXT
		ENDIF
		SIF 条件달성 == 0	
			RETURN 戻り値
	ENDIF

	IF 효과条件レベル > 0
		VARSET 条件달성
		SIF MATCH(효과条件レベル, GETVAR(@"%実行者%_MZ_星:カウンタ"))
			条件달성 ++
		SIF 条件달성 == 0		
			RETURN 戻り値
	ENDIF
	;ザポルグ追記20250102
	IF 효과条件変レベル > 0
		TRYCALLFORM CARD_{설정カードID}, "レベル"
		元レベル = RESULT
		現レベル = GETVAR(@"%듀얼리스트%_MZ_星:カウンタ")
		IF 효과条件変レベル == 1
			SIF 元レベル >= 現レベル
				RETURN 戻り値
		ELSEIF 효과条件変レベル == 2
			SIF 元レベル <= 現レベル
				RETURN 戻り値
		ELSEIF 효과条件変レベル == 3
			SIF 元レベル == 現レベル
				RETURN 戻り値
		ENDIF
	ENDIF
	;ここまで
	IF 효과条件攻撃力 > 0
		SIF GETVAR(@"%実行者%_MZ_攻:カウンタ") > 효과条件攻撃力
			RETURN 戻り値
	ENDIF
	IF 효과条件固攻撃力 >= 0
		SIF GETVAR(@"%実行者%_MZ_攻:カウンタ") != 효과条件固攻撃力
			RETURN 戻り値
	ENDIF
	IF 효과条件以攻撃力 > 0
		SIF GETVAR(@"%実行者%_MZ_攻:カウンタ") < 효과条件以攻撃力
			RETURN 戻り値
	ENDIF
	IF 효과条件元固攻撃力 >= 0
		TRYCALLFORM CARD_{설정カードID}, "攻撃力"
		SIF RESULT != 효과条件元固攻撃力
			RETURN 戻り値
	ENDIF
	IF 효과条件守備力 > 0
		SIF GETVAR(@"%実行者%_MZ_守:カウンタ")  > 효과条件守備力
			RETURN 戻り値
	ENDIF
	IF 효과条件固守備力 >= 0
		SIF GETVAR(@"%実行者%_MZ_守:カウンタ")  != 효과条件固守備力
			RETURN 戻り値
	ENDIF
	IF 효과条件以守備力 > 0
		SIF GETVAR(@"%実行者%_MZ_守:カウンタ") < 효과条件以守備力
			RETURN 戻り値
	ENDIF
	IF 효과条件元固守備力 >= 0
		TRYCALLFORM CARD_{설정カードID}, "守備力"
		SIF RESULT != 효과条件元固守備力
			RETURN 戻り値
	ENDIF

	IF 효과条件同攻守 > 0
		攻撃力설정 = GETVAR(@"%実行者%_MZ_攻:カウンタ") 
		守備力설정 = GETVAR(@"%実行者%_MZ_守:カウンタ") 
		SIF 攻撃力설정 != 守備力설정
			RETURN 戻り値
	ENDIF

	IF 효과条件召種 > 0
		IF 효과条件召種 == 召種全특수
			SIF GETVAR(@"%実行者%_MZ_召種:カウンタ") == 召種通常
					RETURN 戻り値
		ELSE
			SIF GETVAR(@"%実行者%_MZ_召種:カウンタ") != 효과条件召種
				RETURN 戻り値
		ENDIF
	ENDIF

	IF 효과条件T1召種 > 0
		IF 효과条件T1召種 == 召種全특수
			SIF GETVAR(@"%実行者%_MZ_T1召種:カウンタ") == 召種通常
					RETURN 戻り値
		ELSEIF 효과条件T1召種 == 全召種
			SIF GETVAR(@"%実行者%_MZ_T1召種:カウンタ") == 0
					RETURN 戻り値
		ELSE
			SIF GETVAR(@"%実行者%_MZ_T1召種:カウンタ") != 효과条件召種
				RETURN 戻り値
		ENDIF
	ENDIF


	IF 효과条件성별 >= 0
		성별판정 = GETVAR(@"%実行者%_MZ_性:カウンタ")
		IF 효과条件성별 == 牝性
			SIF !GETBIT(성별판정,0)
				RETURN 戻り値
		ELSEIF 효과条件성별 == 雄性
			SIF !GETBIT(성별판정,1)
				RETURN 戻り値
		ELSEIF 효과条件성별 == 성별在
			SIF 성별판정 == 0
				RETURN 戻り値
		ELSEIF 효과条件성별 == 雌以外
			SIF 성별판정 == 1
				RETURN 戻り値
		ELSE
			SIF 성별판정 != 효과条件성별
				RETURN 戻り値
		ENDIF
	ENDIF
	IF 효과条件発情 != -1
		IF 효과条件発情 == 0 ;非発情
			SIF GETVAR(@"%実行者%_MZ_発情:カウンタ") != 0
				RETURN 戻り値
		ELSEIF 효과条件発情 >= 1 ;発情済
			SIF GETVAR(@"%実行者%_MZ_発情:カウンタ") < 1
				RETURN 戻り値
		ENDIF
	ENDIF
ELSE
	IF 효과条件튜너 > 0
		TRYCALLFORM CARD_{설정カードID}, "튜너"
		SIF RESULT == 0
			RETURN 戻り値
	ELSEIF 효과条件튜너 == -1
		TRYCALLFORM CARD_{설정カードID}, "튜너"
		SIF RESULT != 0
			RETURN 戻り値
	ENDIF


	IF 효과条件속성 > 0
		VARSET 条件달성
		;複数속성チェック
		TRYCALLFORM CARD_{설정カードID}, "속성"
		
		VARSET 検出속성, -1
		FOR カウンタB,0,10
			SIF RESULT:カウンタB <= 0
				CONTINUE
			検出속성:カウンタB = RESULT:カウンタB
		NEXT
		FOR カウンタB,0,10
			IF 検出속성:カウンタB != -1
				SIF MATCH(検出속성:カウンタB, 효과条件속성)
					条件달성 ++
				SIF MATCH(효과条件속성, 検出속성:カウンタB)
					条件달성 ++
			ENDIF
		NEXT
		SIF 条件달성 == 0	
			RETURN 戻り値
	ENDIF
	IF 효과条件종족 > 0
		VARSET 条件달성
		;複数속성チェック
		TRYCALLFORM CARD_{설정カードID}, "종족"
		
		VARSET 検出종족, -1
		FOR カウンタB,0,10
			SIF RESULT:カウンタB <= 0
				CONTINUE
			検出종족:カウンタB = RESULT:カウンタB
		NEXT
		FOR カウンタB,0,10
			IF 検出종족:カウンタB != -1
				SIF MATCH(検出종족:カウンタB, 효과条件종족)
					条件달성 ++
				SIF MATCH(효과条件종족, 検出종족:カウンタB)
					条件달성 ++
			ENDIF
		NEXT
		SIF 条件달성 == 0	
			RETURN 戻り値
	ENDIF



	IF 효과条件レベル > 0
		VARSET 条件달성
		TRYCALLFORM CARD_{설정カードID}, "レベル"
		SIF MATCH(효과条件レベル, RESULT)
			条件달성 ++
		TRYCALLFORM CARD_{설정カードID}, "コスト"
		SIF MATCH(효과条件レベル, RESULT)
			条件달성 ++
		SIF 条件달성 == 0		
			RETURN 戻り値
	ENDIF

	IF 효과条件攻撃力 > 0
		TRYCALLFORM CARD_{설정カードID}, "攻撃力"
		SIF RESULT > 효과条件攻撃力
			RETURN 戻り値
	ENDIF
	IF 효과条件固攻撃力 >= 0
		TRYCALLFORM CARD_{설정カードID}, "攻撃力"
		SIF RESULT != 효과条件固攻撃力
			RETURN 戻り値
	ENDIF
	IF 효과条件以攻撃力 > 0
		TRYCALLFORM CARD_{설정カードID}, "攻撃力"
		SIF RESULT < 효과条件以攻撃力
			RETURN 戻り値
	ENDIF
	IF 효과条件元固攻撃力 >= 0
		TRYCALLFORM CARD_{설정カードID}, "攻撃力"
		SIF RESULT != 효과条件元固攻撃力
			RETURN 戻り値
	ENDIF

	IF 효과条件守備力 > 0
		TRYCALLFORM CARD_{설정カードID}, "守備力"
		SIF RESULT > 효과条件守備力
			RETURN 戻り値
	ENDIF
	IF 효과条件固守備力 >= 0
		TRYCALLFORM CARD_{설정カードID}, "守備力"
		SIF RESULT != 효과条件固守備力
			RETURN 戻り値
	ENDIF
	IF 효과条件以守備力 > 0
		TRYCALLFORM CARD_{설정カードID}, "守備力"
		SIF RESULT < 효과条件以守備力
			RETURN 戻り値
	ENDIF
	IF 효과条件元固守備力 >= 0
		TRYCALLFORM CARD_{설정カードID}, "守備力"
		SIF RESULT != 효과条件元固守備力
			RETURN 戻り値
	ENDIF

	IF 효과条件同攻守 > 0
		TRYCALLFORM CARD_{설정カードID}, "攻撃力"
		攻撃力설정 = RESULT
		TRYCALLFORM CARD_{설정カードID}, "守備力"
		守備力설정 = RESULT
		SIF 攻撃力설정 != 守備力설정
			RETURN 戻り値
	ENDIF

	IF 효과条件성별 >= 0
		;Anonym修正
		TRYCALLFORM CARD_{설정カードID}, "성별"
		성별판정 = RESULT
		캐릭터番号 = NO_TO_CHARANUM(설정カードID)
		IF 캐릭터番号 >= 1 && オプション_성별
			성별판정 = TALENT:캐릭터番号:성별
			IF 효과条件성별 == 牝性
				SIF !GETBIT(성별판정,0)
					RETURN 戻り値
			ELSEIF 효과条件성별 == 雄性
				SIF !GETBIT(성별판정,1)
					RETURN 戻り値
			ELSEIF 효과条件성별 == 성별在
				SIF 성별판정 == 0
					RETURN 戻り値
			ELSE
				SIF !MATCH(성별판정, 효과条件성별)
					RETURN 戻り値
			ENDIF
		ELSE
			IF 효과条件성별 == 牝性
				SIF !MATCH(성별판정,1)
					RETURN 戻り値
			ELSEIF 효과条件성별 == 雄性
				SIF !MATCH(성별판정,2)
					RETURN 戻り値
			ELSEIF 효과条件성별 == 성별在
				SIF 성별판정 == 0
					RETURN 戻り値
			ELSE
				SIF !MATCH(성별판정, 효과条件성별)
					RETURN 戻り値
			ENDIF
		ENDIF
	ENDIF

	IF 효과条件所有 >= 1
		IF 효과条件所有 == 1
			IF 듀얼리스트 == "자신"
				SIF GETVAR(@"%実行者%_%MZ魔판정%_所有:カウンタ") != 1
					RETURN 0
			ELSEIF 듀얼리스트 == "상대"
				SIF GETVAR(@"%実行者%_%MZ魔판정%_所有:カウンタ") != 2
					RETURN 0
			ENDIF
		ELSEIF 효과条件所有 == 2
			IF 듀얼리스트 == "자신"
				SIF GETVAR(@"%実行者%_%MZ魔판정%_所有:カウンタ") != 2
					RETURN 0
			ELSEIF 듀얼리스트 == "상대"
				SIF GETVAR(@"%実行者%_%MZ魔판정%_所有:カウンタ") != 1
					RETURN 0
			ENDIF
		ENDIF
	ENDIF

	IF 효과条件소환可 > 0
		IF 효과条件特不無視 == 0
			TRYCALLFORM CARD_{설정カードID}, "종류"
			IF RESULT < 3 || RESULT == 초차원
				IF 효과条件소환可 == 1 || 효과条件소환可 == 3
					CALL CARD_SS_CHECK(実行者,설정カードID,参照先)
					SIF RESULT == 1
						RETURN 戻り値
				ELSEIF 효과条件소환可 == 2
					CALL CARD_SS_CHECK(実行者,설정カードID,参照先,1)
					SIF RESULT == 1
						RETURN 戻り値	
				ENDIF
			ELSEIF INRANGE(RESULT,3,10)

				IF RESULT == 의식
					IF 参照先 == "손패"
						TRYCALLFORM CARDSUMMON_{GETVAR(@"%実行者%_손패:カウンタ")}(実行者,カウンタ + 100,1)
						SIF RESULT != 1
							RETURN 戻り値
					ELSEIF 参照先 == "묘지"
						TRYCALLFORM CARDSUMMON_{GETVAR(@"%実行者%_묘지:カウンタ")}(実行者,カウンタ + 200,1)
						SIF RESULT != 1
							RETURN 戻り値
					ELSEIF 参照先 == "덱"
						TRYCALLFORM CARDSUMMON_{GETVAR(@"%実行者%_덱:カウンタ")}(実行者,カウンタ + 300,1)
						SIF RESULT != 1
							RETURN 戻り値
					ELSEIF 参照先 == "제외"
						TRYCALLFORM CARDSUMMON_{GETVAR(@"%実行者%_제외:カウンタ")}(実行者,カウンタ + 500,1)
						SIF RESULT != 1
							RETURN 戻り値
					ENDIF
				ELSEIF 参照先 == "EX덱"
					VARSET RESULT
					TRYCALLFORM CARDSUMMON_{GETVAR(@"%実行者%_EX덱:カウンタ")}(実行者,カウンタ,1)
					SIF RESULT != 1
						RETURN 戻り値
				ELSEIF 参照先 != "EX덱" ;Anonym추가
					IF 효과条件소환可 == 1 || 효과条件소환可 == 3
						CALL CARD_SS_CHECK(듀얼리스트,설정カードID,参照先)
						SIF RESULT == 1
							RETURN 戻り値
					ELSEIF 효과条件소환可 == 2
						CALL CARD_SS_CHECK(듀얼리스트,설정カードID,参照先,1)
						SIF RESULT == 1
						RETURN 戻り値
					ENDIF
				ENDIF
			ELSE
				IF 효과条件소환可 == 3
					CALL CARD_SS_CHECK(実行者,설정カードID,参照先)
					SIF RESULT == 1
						RETURN 戻り値
				ELSE
					RETURN 戻り値
				ENDIF
			ENDIF
		ENDIF
	ENDIF
ENDIF

IF 효과条件RUM >= 1
	TRYCALLFORM CARDRUM_{효과条件RUM}(実行者,参照先,カウンタ,설정カードID)
	SIF RESULT != 1
		RETURN 戻り値
ENDIF

;PRINTFORML チェック플래그:{チェック플래그}
IF チェック플래그 == 5
	CALL SET_FIELD_INFO(実行者,参照先,カウンタ,"リ数",1)
	CALL CARD_EROTICS_FIELD(実行者,参照先,カウンタ)
	RETURN RESULT
ENDIF

RETURN 1


;――――――――――――――――――――――――――――――――――――――――――――――――――――――――――――――――――――――――――――――――――――――――――――――――――――――――――――

@CARD_S_FILTER_SET(実行者,参照先,カウンタ,듀얼리스트,チェック플래그,カードID)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC 対面者
#DIMS DYNAMIC 実行者
#DIM DYNAMIC めくり数
#DIMS DYNAMIC 元参照先
#DIMS DYNAMIC 参照先
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC カウンタB
#DIM DYNAMIC 설정カードID
#DIM DYNAMIC カードID
#DIMS DYNAMIC カード追カテ,10
#DIM DYNAMIC チェック플래그
#DIM DYNAMIC 条件달성
#DIM DYNAMIC 検出속성,99
#DIM DYNAMIC 検出종족,99
#DIMS DYNAMIC MZ魔판정

SIF 参照先 == ""
	RETURN 0


IF チェック플래그 == 6
	参照先 = 묘지
	カウンタ = 0
	설정カードID = カードID
ELSE
	SIF GETVAR(@"%実行者%_%参照先%:カウンタ") == -1
		RETURN 0
	SIF GETVAR(@"%実行者%_%参照先%:カウンタ") == 使用불가ゾーン
		RETURN 0

	설정カードID = GETVAR(@"%実行者%_%参照先%:カウンタ")
ENDIF

IF 효과条件スーパー名前 != ""
	VARSET 条件달성
	TRYCALLFORM CARD_{설정カードID}, "別途ID"
	IF RESULT >= 1
		TRYCALLFORM CARDNAME_{RESULT}, "名前"
	ELSE
		TRYCALLFORM CARDNAME_{설정カードID}, "名前"
	ENDIF
	SIF MATCH(효과条件スーパー名前, RESULTS)
		条件달성 ++

	IF 参照先 == "필드"
		IF GETVAR(@"%実行者%_MZ_変化元ID:カウンタ") > -1
			TRYCALLFORM CARDNAME_{GETVAR(@"%実行者%_MZ_変化元ID:カウンタ")}, "名前"
			SIF MATCH(효과条件スーパー名前, RESULTS)
				条件달성 ++
		ENDIF
	ELSEIF 参照先 == "마함"
		IF GETVAR(@"%実行者%_魔_変化元ID:カウンタ") > -1
			TRYCALLFORM CARDNAME_{GETVAR(@"%実行者%_魔_変化元ID:カウンタ")}, "名前"
			SIF MATCH(효과条件スーパー名前, RESULTS)
				条件달성 ++
		ENDIF
	ENDIF
	SIF 条件달성 == 0
		RETURN 0
ENDIF

IF 효과条件スーパーカテゴリ != ""
	VARSET 条件달성
	TRYCALLFORM CARDNAME_{설정カードID}, "カテゴリ"
	FOR カウンタB,0,5
		IF RESULTS:カウンタB != ""
			SIF MATCH(RESULTS:カウンタB, 효과条件スーパーカテゴリ)
				条件달성 ++
			SIF MATCH(효과条件スーパーカテゴリ, RESULTS:カウンタB)
				条件달성 ++
		ENDIF
	NEXT
	IF 参照先 == "필드"
		VARSET カード追カテ
		カード追カテ = %GETVARS(@"%実行者%_MZ_追カテ:カウンタ")%
		カード追カテ:1 = %GETVARS(@"%実行者%_MZ_追カテ2:カウンタ")%
		IF GETVARS(@"%実行者%_MZ_追カテ:カウンタ") != ""
			IF MATCH(효과条件スーパーカテゴリ, カード追カテ)
				条件달성 ++
			ENDIF
			IF MATCH(カード追カテ, 효과条件スーパーカテゴリ)
				条件달성 ++
			ENDIF
		ENDIF

		IF GETVAR(@"%実行者%_MZ_変化元ID:カウンタ") > -1
			TRYCALLFORM CARDNAME_{GETVAR(@"%実行者%_MZ_変化元ID:カウンタ")}, "カテゴリ"
			IF MATCH(RESULTS, 효과条件スーパーカテゴリ)
				条件달성 ++
			ENDIF
			IF MATCH(효과条件スーパーカテゴリ, RESULTS)
				条件달성 ++
			ENDIF
		ENDIF
	ENDIF
	IF 参照先 == "마함"
		VARSET カード追カテ
		カード追カテ = %GETVARS(@"%実行者%_魔_追カテ:カウンタ")%
		カード追カテ:1 = %GETVARS(@"%実行者%_魔_追カテ2:カウンタ")%
		IF GETVARS(@"%実行者%_魔_追カテ:カウンタ") != ""
			SIF MATCH(효과条件スーパーカテゴリ, カード追カテ)
				条件달성 ++
			SIF MATCH(カード追カテ, 효과条件スーパーカテゴリ)
				条件달성 ++
		ENDIF
	ENDIF
	SIF 条件달성 == 0
		RETURN 0
ENDIF

IF 효과条件スーパー종류 > 0
	VARSET 条件달성
	TRYCALLFORM CARD_{설정カードID}, "종류"
	IF 효과条件スーパー종류 == モン全般
		SIF RESULT < モン全般
			条件달성 ++
	ELSEIF 효과条件スーパー종류 == EXモン全般
		SIF GROUPMATCH(RESULT, 융합, 싱크로, 엑시즈, 링크, 초차원)
			条件달성 ++
	ELSEIF 효과条件スーパー종류 == EX以外全般
		SIF !GROUPMATCH(RESULT, 융합, 싱크로, 엑시즈, 링크, 초차원) && RESULT < 10
			条件달성 ++
	
	ELSEIF INRANGE(효과条件スーパー종류,의식以外,링크以外)
		SIF RESULT != 효과条件スーパー종류 - 10
			条件달성 ++

	ELSEIF 효과条件スーパー종류 == 효과全般
		SIF RESULT != 通常モン
			条件달성 ++
		TRYCALLFORM CARD_{설정カードID}, "分割"
		SIF RESULT != 通常モン
			条件달성 ++
	ELSEIF 효과条件スーパー종류 == 효과無し全般
		SIF RESULT == 通常モン
			条件달성 ++
		TRYCALLFORM CARD_{설정カードID}, "分割"
		SIF RESULT == 通常モン
			条件달성 ++

	ELSEIF 효과条件スーパー종류 == 마법全般
		SIF INRANGE(RESULT, 20, 29) 
			条件달성 ++
	ELSEIF 효과条件スーパー종류 == 함정全般
		SIF INRANGE(RESULT, 30, 39) 
			条件달성 ++
	ELSEIF 효과条件スーパー종류 == 마함全般
		SIF INRANGE(RESULT, 20, 39) 
			条件달성 ++	
	ELSE
		SIF MATCH(효과条件スーパー종류, RESULT)
			条件달성 ++
		SIF MATCH(RESULT, 효과条件スーパー종류)
			条件달성 ++
	ENDIF
	SIF 条件달성 == 0
		RETURN 0
ENDIF

IF 효과条件スーパー속성 > 0
	VARSET 条件달성
	IF 参照先 == "필드"
		SIF MATCH(효과条件スーパー속성, GETVAR(@"%実行者%_MZ_属:カウンタ"))
			条件달성 ++
		;複数속성チェック
		TRYCALLFORM CARD_{설정カードID}, "속성"
		IF RESULT:1 >= 1
			VARSET 検出속성, -1
			FOR カウンタB,0,10
				SIF RESULT:カウンタB <= 0
					CONTINUE
				検出속성:カウンタB = RESULT:カウンタB
			NEXT
			FOR カウンタB,1,10
				IF 検出속성:カウンタB != -1
					SIF MATCH(検出속성:カウンタB, 효과条件スーパー속성)
						条件달성 ++
					SIF MATCH(효과条件スーパー속성, 検出속성:カウンタB)
						条件달성 ++
				ENDIF
			NEXT
		ENDIF
	ELSE
		;複数속성チェック
		TRYCALLFORM CARD_{설정カードID}, "속성"
		VARSET 検出속성, -1
		FOR カウンタB,0,10
			SIF RESULT:カウンタB <= 0
				CONTINUE
			検出속성:カウンタB = RESULT:カウンタB
		NEXT
		FOR カウンタB,0,10
			IF 検出속성:カウンタB != -1
				SIF MATCH(検出속성:カウンタB, 효과条件スーパー속성)
					条件달성 ++
				SIF MATCH(효과条件スーパー속성, 検出속성:カウンタB)
					条件달성 ++
			ENDIF
		NEXT
	ENDIF
	SIF 条件달성 == 0
		RETURN 0
ENDIF

IF 효과条件スーパー종족 > 0
	VARSET 条件달성
	IF 参照先 == "필드"
		SIF MATCH(효과条件スーパー종족, GETVAR(@"%実行者%_MZ_種:カウンタ"))
			条件달성 ++
		;複数종족チェック
		TRYCALLFORM CARD_{설정カードID}, "종족"
		IF RESULT:1 >= 1
			VARSET 検出종족, -1
			FOR カウンタB,0,10
				SIF RESULT:カウンタB <= 0
					CONTINUE
				検出종족:カウンタB = RESULT:カウンタB
			NEXT
			FOR カウンタB,1,10
				IF 検出종족:カウンタB != -1
					SIF MATCH(検出종족:カウンタB, 효과条件スーパー종족)
						条件달성 ++
					SIF MATCH(효과条件スーパー종족, 検出종족:カウンタB)
						条件달성 ++
				ENDIF
			NEXT
		ENDIF
		SIF 条件달성 == 0	
			RETURN 0
	ELSE
		;複数종족チェック
		TRYCALLFORM CARD_{설정カードID}, "종족"
		VARSET 検出종족, -1
		FOR カウンタB,0,10
			SIF RESULT:カウンタB <= 0
				CONTINUE
			検出종족:カウンタB = RESULT:カウンタB
		NEXT
		FOR カウンタB,0,10
			IF 検出종족:カウンタB != -1
				SIF MATCH(検出종족:カウンタB, 효과条件スーパー종족)
					条件달성 ++
				SIF MATCH(효과条件スーパー종족, 検出종족:カウンタB)
					条件달성 ++
			ENDIF
		NEXT
	ENDIF
	SIF 条件달성 == 0	
		RETURN 0
ENDIF

IF 효과条件スーパーレベル > 0
	VARSET 条件달성
	IF 参照先 == "필드"
		SIF MATCH(효과条件スーパーレベル, GETVAR(@"%実行者%_MZ_星:カウンタ"))
			条件달성 ++
		TRYCALLFORM CARD_{설정カードID}, "コスト"
		SIF MATCH(효과条件スーパーレベル, RESULT)
			条件달성 ++
	ELSE
		TRYCALLFORM CARD_{설정カードID}, "レベル"
		SIF MATCH(효과条件スーパーレベル, RESULT)
			条件달성 ++
		TRYCALLFORM CARD_{설정カードID}, "コスト"
		SIF MATCH(효과条件スーパーレベル, RESULT)
			条件달성 ++
	ENDIF
	SIF 条件달성 == 0		
		RETURN 0
ENDIF

VARSET RESULT
IF 효과条件スーパー튜너 > 0
	TRYCALLFORM CARD_{설정カードID}, "튜너"
	SIF RESULT == 0
		RETURN 0
ELSEIF 효과条件スーパー튜너 == -1
	TRYCALLFORM CARD_{설정カードID}, "튜너"
	SIF RESULT != 0
		RETURN 0
ENDIF

;Anonym추가
IF 参照先 == "필드" || 参照先 == "마함"
	IF 参照先 == "필드"
		MZ魔판정 = MZ
	ELSEIF 参照先 == "마함"
		MZ魔판정 = 魔
	ENDIF
	IF 효과条件표시 == -1 && チェック플래그 == 1
		CALLFORM CHECK_COND
		IF RESULT == 1
			IF 参照先 == "필드"
				SIF GETVAR(@"%実行者%_%MZ魔판정%_表:カウンタ") == 3
					RETURN 0
			ELSEIF 参照先 == "마함"
				SIF GETVAR(@"%実行者%_%MZ魔판정%_表:カウンタ") != 1
					RETURN 0
			ENDIF
		ENDIF
	ENDIF

	IF 효과条件표시 >= 0
		;효과불가裏側표시
		SIF 효과条件표시 == 30 && GETVAR(@"%実行者%_%MZ魔판정%_表:カウンタ") != 3
			RETURN 0
		;裏側표시
		IF 효과条件표시 == 裏側표시
			IF 参照先 == "필드"
				SIF GETVAR(@"%実行者%_%MZ魔판정%_表:カウンタ") != 3
					RETURN 0
			ELSEIF 参照先 == "마함"		
				SIF GETVAR(@"%実行者%_%MZ魔판정%_表:カウンタ") == 1
					RETURN 0
			ENDIF
		ENDIF
		;表側표시
		IF 효과条件표시 == 表側표시
			IF 参照先 == "필드"
				SIF GETVAR(@"%実行者%_%MZ魔판정%_表:カウンタ") == 3
					RETURN 0
			ELSEIF 参照先 == "마함"		
				SIF GETVAR(@"%実行者%_%MZ魔판정%_表:カウンタ") != 1
					RETURN 0
			ENDIF
		ENDIF
		;その他
		IF 효과条件표시 == 守備全般
			IF 参照先 == "필드"
				SIF GETVAR(@"%実行者%_%MZ魔판정%_表:カウンタ") == 1
					RETURN 0
			ENDIF
		ENDIF
		;효과条件표시 = 0はCHECKBASICをパスするため（モンスター全体をカウントしたい時とかに使用）
		SIF 효과条件표시 != 0 && 효과条件표시 <= 9 && GETVAR(@"%実行者%_%MZ魔판정%_表:カウンタ") != 효과条件표시
			RETURN 0
	ENDIF
	
ENDIF




;ザポルグ追記20240929
IF 효과条件スーパー攻撃力 > 0
	VARSET RESULT
	TRYCALLFORM CARD_{설정カードID}, "종류"
	IF RESULT < モン全般
		VARSET RESULT
		TRYCALLFORM CARD_{설정カードID}, "攻撃力"
		SIF RESULT > 효과条件攻撃力
			RETURN 0
	ENDIF
ENDIF


IF チェック플래그 == 2
	VARSET RESULT
	IF 효과条件特不無視 != 2
		TRYCALLFORM CARD_{설정カードID}, "종류"
		SIF RESULT > 10
			RETURN 0
		IF 효과条件特不無視 == 0
			SIF RESULT == 3
				RETURN 0
			TRYCALLFORM CARD_{설정カードID}, "특수불가"
			SIF RESULT == 1
				RETURN 0
			CALL CARD_SS_CHECK(듀얼리스트,설정カードID,参照先)
			SIF RESULT == 1
				RETURN 0
			ELSE
			SIF モンスター数(듀얼리스트) >= 5
				RETURN 0
			SIF GETVAR(@"%듀얼리스트%_SS阻害状態") >= 1
				RETURN 0
		ENDIF
	ENDIF
ENDIF

CALL S_FILTER_EXIST
SIF RESULT == 0
	RETURN 0

IF 参照先 != "필드"
	IF 효과条件소환可 > 0
		IF 효과条件特不無視 == 0
			TRYCALLFORM CARD_{설정カードID}, "종류"
			IF RESULT < 3 || RESULT == 초차원
				IF 효과条件소환可 == 1 || 효과条件소환可 == 3
					CALL CARD_SS_CHECK(듀얼리스트,설정カードID,参照先)
					SIF RESULT == 1
						RETURN 0
				ELSEIF 효과条件소환可 == 2
					CALL CARD_SS_CHECK(듀얼리스트,설정カードID,参照先,1)
					SIF RESULT == 1
						RETURN 0
				ENDIF
			ELSEIF INRANGE(RESULT,3,10)
				IF RESULT == 의식
					IF 参照先 == "손패"
						TRYCALLFORM CARDSUMMON_{GETVAR(@"%実行者%_손패:カウンタ")}(実行者,カウンタ + 100,1)
						SIF RESULT != 1
							RETURN 0
					ELSEIF 参照先 == "묘지"
						TRYCALLFORM CARDSUMMON_{GETVAR(@"%実行者%_묘지:カウンタ")}(実行者,カウンタ + 200,1)
						SIF RESULT != 1
							RETURN 0
					ELSEIF 参照先 == "덱"
						TRYCALLFORM CARDSUMMON_{GETVAR(@"%実行者%_덱:カウンタ")}(実行者,カウンタ + 300,1)
						SIF RESULT != 1
							RETURN 0
					ELSEIF 参照先 == "제외"
						TRYCALLFORM CARDSUMMON_{GETVAR(@"%実行者%_제외:カウンタ")}(実行者,カウンタ + 500,1)
						SIF RESULT != 1
							RETURN 0
					ENDIF
				;EX덱を参照に確認している
				ELSEIF 参照先 == "EX덱"
					VARSET RESULT
					TRYCALLFORM CARDSUMMON_{GETVAR(@"%実行者%_EX덱:カウンタ")}(実行者,カウンタ,1)
					SIF RESULT != 1
						RETURN 0
				;EX덱以外を参照に確認
				ELSEIF 参照先 != "EX덱" ;Anonym추가
					IF 효과条件소환可 == 1 || 효과条件소환可 == 3
						CALL CARD_SS_CHECK(듀얼리스트,설정カードID,参照先)
						SIF RESULT == 1
							RETURN 0
					ELSEIF 효과条件소환可 == 2
						CALL CARD_SS_CHECK(듀얼리스트,설정カードID,参照先,1)
						SIF RESULT == 1
							RETURN 0
					ENDIF
				ENDIF
			ELSE
				IF 효과条件소환可 == 3
					CALL CARD_SS_CHECK(듀얼리스트,설정カードID,参照先)
					SIF RESULT == 1
						RETURN 0
				ELSE
					RETURN 0
				ENDIF
			ENDIF
		ENDIF
	ENDIF
ENDIF



RETURN 1


@S_FILTER_EXIST
VARSET RESULT
SIF 효과条件スーパー名前 != ""
	RETURN 1
SIF 효과条件スーパーカテゴリ != ""
	RETURN 1
SIF 효과条件スーパー종류 > 0
	RETURN 1
SIF 효과条件スーパー속성 > 0
	RETURN 1
SIF 효과条件スーパー종족 > 0
	RETURN 1
SIF 효과条件スーパーレベル > 0
	RETURN 1
SIF 효과条件スーパー튜너 > 0
	RETURN 1
;ザポルグ追記20240929
SIF 효과条件スーパー攻撃力 > 0
	RETURN 1
RETURN 0

;待機された효과를 待機する場所。
@SELECT_UI_SET(実行者,選択カード_ID,설정타입,選択カード,종류,ゾーン)
#DIMS DYNAMIC 実行者
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 選択カード_ID
#DIM DYNAMIC 설정타입
#DIM DYNAMIC 選択カード

CALL CARD_UI_RESET
IF 実行者 == "상대"
	SETCOLOR C_W_RED
	CUSTOMDRAWLINE —
ELSE
	SETCOLOR C_W_BLUE
	CUSTOMDRAWLINE —
ENDIF
SELECTCASE 설정타입
CASE 1
	PRINT 【서치】
	確認듀얼리스트 = %実行者%
CASE 2
	PRINT 【묘지보내기】
	確認듀얼리스트 = %実行者%
CASE 3
	PRINT 【특수소환】
	確認듀얼리스트 = %実行者%
CASE 4
	PRINT 【선택】
	確認듀얼리스트 = %実行者%
CASE 5
	PRINT 【세트】
	確認듀얼리스트 = %実行者%
CASE 6
	PRINT 【장착처】
	確認듀얼리스트 = %実行者%
CASE 7
	PRINT 【장착선택】
	確認듀얼리스트 = %実行者%
CASE 8
	PRINT 【대기효과】
	確認듀얼리스트 =  %待機_듀얼리스트:選択カード%
	確認종류 = 待機_효과種:選択カード
	確認ゾーン = %待機_ゾーン:選択カード%
	確認場所 = 待機_場所:選択カード
CASE 9
	PRINT 【프리유발】
	確認듀얼리스트 = %実行者%
	確認종류 = フリー誘発
	確認ゾーン = %参照先（CS）%
	確認場所 = 選択カード
CASE 10
	PRINT 【효과 후 유발】
	確認듀얼리스트 = %実行者%
	確認종류 = 효과後誘発
	確認ゾーン = %参照先（CS）%
	確認場所 = 選択カード

CASE 15
	PRINT 【발동유발】
	確認듀얼리스트 = %実行者%
	確認종류 = 발동誘発
	確認ゾーン = %参照先（CS）%
	確認場所 = 選択カード


CASE 11
	PRINT 【제외】
	CALL CARD_SELECT(実行者,(選択カード + 2000))
	確認듀얼리스트 = %대상者（CS）%
	確認ゾーン = %参照先（CS）%
	確認場所 = 選択カード（CS）
CASE 12
	;両者確認
	PRINT 【제외】
	CALL CARD_SELECT(実行者,選択カード)
	確認듀얼리스트 = %대상者（CS）%
	確認ゾーン = %参照先（CS）%
	確認場所 = 選択カード（CS）
CASE 99
	PRINT 【발동확인】
	確認듀얼리스트 = %実行者%
	確認종류 = 종류
	確認ゾーン = %ゾーン%
	確認場所 = 選択カード
ENDSELECT
IF 実行者 == "상대"
	PRINTL 【상대가 고른 카드를 확인해주세요】
ELSE
	PRINTL 【선택 카드 확인】
ENDIF
CUSTOMDRAWLINE —
RESETCOLOR

IF GROUPMATCH(설정타입,1,2,3,4,5,6,7,11,12) && 実行者 == "자신"
	IF 確認ゾーン == "필드"
		IF GETVAR(@"%確認듀얼리스트%_MZ_表:確認場所") == 3 && 実行者 != 確認듀얼리스트
			TRYCALLFORM CARD_UI(999990,2)
		ELSE
			TRYCALLFORM CARD_UI_FIELD(確認듀얼리스트,確認場所,1)
		ENDIF
	ELSEIF 確認ゾーン == "마함"
		IF GETVAR(@"%確認듀얼리스트%_魔_表:確認場所") >= 2 && 実行者 != 確認듀얼리스트
			TRYCALLFORM CARD_UI(999990,2)
		ELSE
			TRYCALLFORM CARD_UI_MAGIC(確認듀얼리스트,確認場所,1)
		ENDIF
	ELSE
		TRYCALLFORM CARD_UI(選択カード_ID,2)
	ENDIF
ELSE
	TRYCALLFORM CARD_UI(選択カード_ID,2)
ENDIF
IF 実行者 == "자신"
	PRINTL
	CUSTOMDRAWLINE —
	PRINTL 이 카드로 괜찮겠습니까？
	PRINTL [0] - 예
	PRINTL [1] - 아니오
	BINPUT
	IF RESULT == 1
		RETURN 1
	ENDIF
ENDIF
RETURN 0

